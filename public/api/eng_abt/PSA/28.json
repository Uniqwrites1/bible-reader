{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":28,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Unto thee, O Jehovah, will I call:","poem":1},{"lineBreak":true},{"text":"My rock, be not thou deaf unto me;","poem":1},{"lineBreak":true},{"text":"Lest, if thou be silent unto me,","poem":1},{"lineBreak":true},{"text":"I become like them that go down into the pit.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Hear the voice of my supplications, when I cry unto thee,","poem":1},{"lineBreak":true},{"text":"When I lift up my hands toward thy holy oracle.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Draw me not away with the wicked,","poem":1},{"lineBreak":true},{"text":"And with the workers of iniquity;","poem":1},{"lineBreak":true},{"text":"That speak peace with their neighbors,","poem":1},{"lineBreak":true},{"text":"But mischief is in their hearts.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Give them according to their work, and according to the wickedness of their doings:","poem":1},{"lineBreak":true},{"text":"Give them after the operation of their hands;","poem":1},{"lineBreak":true},{"text":"Render to them their desert.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Because they regard not the works of Jehovah,","poem":1},{"lineBreak":true},{"text":"Nor the operation of his hands,","poem":1},{"lineBreak":true},{"text":"He will break them down and not build them up.","poem":1}]},{"type":"line_break"},{"type":"verse","number":6,"content":[{"text":"Blessed be Jehovah,","poem":1},{"lineBreak":true},{"text":"Because he hath heard the voice of my supplications.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Jehovah is my strength and my shield;","poem":1},{"lineBreak":true},{"text":"My heart hath trusted in him, and I am helped:","poem":1},{"lineBreak":true},{"text":"Therefore my heart greatly rejoiceth;","poem":1},{"lineBreak":true},{"text":"And with my song will I praise him.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Jehovah is their strength,","poem":1},{"lineBreak":true},{"text":"And he is a stronghold of salvation to his anointed.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Save thy people, and bless thine inheritance:","poem":1},{"lineBreak":true},{"text":"Be their shepherd also, and bear them up forever.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/28.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/29.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/27.json","previousChapterAudioLinks":{},"numberOfVerses":9}