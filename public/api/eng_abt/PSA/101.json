{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":101,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"I will sing of lovingkindness and justice:","poem":1},{"lineBreak":true},{"text":"Unto thee, O Jehovah, will I sing praises.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I will behave myself wisely in a perfect way:","poem":1},{"lineBreak":true},{"text":"Oh when wilt thou come unto me?","poem":1},{"lineBreak":true},{"text":"I will walk within my house with a perfect heart.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"I will set no base thing before mine eyes:","poem":1},{"lineBreak":true},{"text":"I hate the work of them that turn aside;","poem":1},{"lineBreak":true},{"text":"It shall not cleave unto me.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"A perverse heart shall depart from me:","poem":1},{"lineBreak":true},{"text":"I will know no evil thing.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Whoso privily slandereth his neighbor, him will I destroy:","poem":1},{"lineBreak":true},{"text":"Him that hath a high look and a proud heart will I not suffer.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Mine eyes shall be upon the faithful of the land, that they may dwell with me:","poem":1},{"lineBreak":true},{"text":"He that walketh in a perfect way, he shall minister unto me.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He that worketh deceit shall not dwell within my house:","poem":1},{"lineBreak":true},{"text":"He that speaketh falsehood shall not be established before mine eyes.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Morning by morning will I destroy all the wicked of the land;","poem":1},{"lineBreak":true},{"text":"To cut off all the workers of iniquity from the city of Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/101.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/102.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/100.json","previousChapterAudioLinks":{},"numberOfVerses":8}