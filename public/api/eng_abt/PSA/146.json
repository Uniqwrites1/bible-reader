{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":146,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"Praise Jehovah, O my soul.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"While I live will I praise Jehovah:","poem":1},{"lineBreak":true},{"text":"I will sing praises unto my God while I have any being.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Put not your trust in princes,","poem":1},{"lineBreak":true},{"text":"Nor in the son of man, in whom there is no help.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"His breath goeth forth, he returneth to his earth;","poem":1},{"lineBreak":true},{"text":"In that very day his thoughts perish.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Happy is he that hath the God of Jacob for his help,","poem":1},{"lineBreak":true},{"text":"Whose hope is in Jehovah his God:","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Who made heaven and earth,","poem":1},{"lineBreak":true},{"text":"The sea, and all that in them is;","poem":1},{"lineBreak":true},{"text":"Who keepeth truth forever;","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Who executeth justice for the oppressed;","poem":1},{"lineBreak":true},{"text":"Who giveth food to the hungry.","poem":1},{"lineBreak":true},{"text":"Jehovah looseth the prisoners;","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Jehovah openeth the eyes of the blind;","poem":1},{"lineBreak":true},{"text":"Jehovah raiseth up them that are bowed down;","poem":1},{"lineBreak":true},{"text":"Jehovah loveth the righteous;","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Jehovah preserveth the sojourners;","poem":1},{"lineBreak":true},{"text":"He upholdeth the fatherless and widow;","poem":1},{"lineBreak":true},{"text":"But the way of the wicked he turneth upside down.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Jehovah will reign forever,","poem":1},{"lineBreak":true},{"text":"Thy God, O Zion, unto all generations.","poem":1},{"lineBreak":true},{"text":"Praise ye Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/146.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/147.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/145.json","previousChapterAudioLinks":{},"numberOfVerses":10}