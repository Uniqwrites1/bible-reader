{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":82,"content":[{"type":"hebrew_subtitle","content":["A Psalm of Asaph."]},{"type":"verse","number":1,"content":[{"text":"God standeth in the congregation of God;","poem":1},{"lineBreak":true},{"text":"He judgeth among the gods.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"How long will ye judge unjustly,","poem":1},{"lineBreak":true},{"text":"And respect the persons of the wicked? Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Judge the poor and fatherless:","poem":1},{"lineBreak":true},{"text":"Do justice to the afflicted and destitute.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Rescue the poor and needy:","poem":1},{"lineBreak":true},{"text":"Deliver them out of the hand of the wicked.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"They know not, neither do they understand;","poem":1},{"lineBreak":true},{"text":"They walk to and fro in darkness:","poem":1},{"lineBreak":true},{"text":"All the foundations of the earth are shaken.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I said, Ye are gods,","poem":1},{"lineBreak":true},{"text":"And all of you sons of the Most High.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Nevertheless ye shall die like men,","poem":1},{"lineBreak":true},{"text":"And fall like one of the princes.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Arise, O God, judge the earth;","poem":1},{"lineBreak":true},{"text":"For thou shalt inherit all the nations.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/82.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/83.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/81.json","previousChapterAudioLinks":{},"numberOfVerses":8}