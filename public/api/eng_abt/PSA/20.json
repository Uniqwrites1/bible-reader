{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":20,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Jehovah answer thee in the day of trouble;","poem":1},{"lineBreak":true},{"text":"The name of the God of Jacob set thee up on high;","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Send thee help from the sanctuary,","poem":1},{"lineBreak":true},{"text":"And strengthen thee out of Zion;","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Remember all thy offerings,","poem":1},{"lineBreak":true},{"text":"And accept thy burnt sacrifice; Selah","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Grant thee thy heartâ€™s desire,","poem":1},{"lineBreak":true},{"text":"And fulfill all thy counsel.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"We will triumph in thy salvation,","poem":1},{"lineBreak":true},{"text":"And in the name of our God we will set up our banners:","poem":1},{"lineBreak":true},{"text":"Jehovah fulfill all thy petitions.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Now know I that Jehovah saveth his anointed;","poem":1},{"lineBreak":true},{"text":"He will answer him from his holy heaven","poem":1},{"lineBreak":true},{"text":"With the saving strength of his right hand.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Some trust in chariots, and some in horses;","poem":1},{"lineBreak":true},{"text":"But we will make mention of the name of Jehovah our God.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They are bowed down and fallen;","poem":1},{"lineBreak":true},{"text":"But we are risen, and stand upright.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Save, Jehovah:","poem":1},{"lineBreak":true},{"text":"Let the King answer us when we call.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/20.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/21.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/19.json","previousChapterAudioLinks":{},"numberOfVerses":9}