{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":149,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"Sing unto Jehovah a new song,","poem":1},{"lineBreak":true},{"text":"And his praise in the assembly of the saints.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let Israel rejoice in him that made him:","poem":1},{"lineBreak":true},{"text":"Let the children of Zion be joyful in their King.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Let them praise his name in the dance:","poem":1},{"lineBreak":true},{"text":"Let them sing praises unto him with timbrel and harp.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For Jehovah taketh pleasure in his people:","poem":1},{"lineBreak":true},{"text":"He will beautify the meek with salvation.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let the saints exult in glory:","poem":1},{"lineBreak":true},{"text":"Let them sing for joy upon their beds.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Let the high praises of God be in their mouth,","poem":1},{"lineBreak":true},{"text":"And a two-edged sword in their hand;","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"To execute vengeance upon the nations,","poem":1},{"lineBreak":true},{"text":"And punishments upon the peoples;","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"To bind their kings with chains,","poem":1},{"lineBreak":true},{"text":"And their nobles with fetters of iron;","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"To execute upon them the judgment written:","poem":1},{"lineBreak":true},{"text":"This honor have all his saints.","poem":1},{"lineBreak":true},{"text":"Praise ye Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}