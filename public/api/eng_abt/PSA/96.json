{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":96,"content":[{"type":"verse","number":1,"content":[{"text":"Oh sing unto Jehovah a new song:","poem":1},{"lineBreak":true},{"text":"Sing unto Jehovah, all the earth.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Sing unto Jehovah, bless his name;","poem":1},{"lineBreak":true},{"text":"Show forth his salvation from day to day.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Declare his glory among the nations,","poem":1},{"lineBreak":true},{"text":"His marvelous works among all the peoples.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For great is Jehovah, and greatly to be praised:","poem":1},{"lineBreak":true},{"text":"He is to be feared above all gods.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"For all the gods of the peoples are idols;","poem":1},{"lineBreak":true},{"text":"But Jehovah made the heavens.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Honor and majesty are before him:","poem":1},{"lineBreak":true},{"text":"Strength and beauty are in his sanctuary.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Ascribe unto Jehovah, ye kindreds of the peoples,","poem":1},{"lineBreak":true},{"text":"Ascribe unto Jehovah glory and strength.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Ascribe unto Jehovah the glory due unto his name:","poem":1},{"lineBreak":true},{"text":"Bring an offering, and come into his courts.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Oh worship Jehovah in holy array:","poem":1},{"lineBreak":true},{"text":"Tremble before him, all the earth.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Say among the nations, Jehovah reigneth:","poem":1},{"lineBreak":true},{"text":"The world also is established that it cannot be moved:","poem":1},{"lineBreak":true},{"text":"He will judge the peoples with equity.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Let the heavens be glad, and let the earth rejoice;","poem":1},{"lineBreak":true},{"text":"Let the sea roar, and the fullness thereof;","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Let the field exult, and all that is therein;","poem":1},{"lineBreak":true},{"text":"Then shall all the trees of the wood sing for joy","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Before Jehovah; for he cometh,","poem":1},{"lineBreak":true},{"text":"For he cometh to judge the earth:","poem":1},{"lineBreak":true},{"text":"He will judge the world with righteousness,","poem":1},{"lineBreak":true},{"text":"And the peoples with his truth.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/96.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/97.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/95.json","previousChapterAudioLinks":{},"numberOfVerses":13}