{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":112,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"Blessed is the man that feareth Jehovah,","poem":1},{"lineBreak":true},{"text":"That delighteth greatly in his commandments.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"His seed shall be mighty upon earth:","poem":1},{"lineBreak":true},{"text":"The generation of the upright shall be blessed.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Wealth and riches are in his house;","poem":1},{"lineBreak":true},{"text":"And his righteousness endureth forever.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Unto the upright there ariseth light in the darkness:","poem":1},{"lineBreak":true},{"text":"He is gracious, and merciful, and righteous.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Well is it with the man that dealeth graciously and lendeth;","poem":1},{"lineBreak":true},{"text":"He shall maintain his cause in judgment.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For he shall never be moved;","poem":1},{"lineBreak":true},{"text":"The righteous shall be had in everlasting remembrance.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He shall not be afraid of evil tidings:","poem":1},{"lineBreak":true},{"text":"His heart is fixed, trusting in Jehovah.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"His heart is established, he shall not be afraid,","poem":1},{"lineBreak":true},{"text":"Until he see his desire upon his adversaries.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He hath dispersed, he hath given to the needy;","poem":1},{"lineBreak":true},{"text":"His righteousness endureth forever:","poem":1},{"lineBreak":true},{"text":"His horn shall be exalted with honor.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"The wicked shall see it, and be grieved;","poem":1},{"lineBreak":true},{"text":"He shall gnash with his teeth, and melt away:","poem":1},{"lineBreak":true},{"text":"The desire of the wicked shall perish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/112.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/113.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/111.json","previousChapterAudioLinks":{},"numberOfVerses":10}