{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":111,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"I will give thanks unto Jehovah with my whole heart,","poem":1},{"lineBreak":true},{"text":"In the council of the upright, and in the congregation.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"The works of Jehovah are great,","poem":1},{"lineBreak":true},{"text":"Sought out of all them that have pleasure therein.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"His work is honor and majesty;","poem":1},{"lineBreak":true},{"text":"And his righteousness endureth forever.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He hath made his wonderful works to be remembered:","poem":1},{"lineBreak":true},{"text":"Jehovah is gracious and merciful.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"He hath given food unto them that fear him:","poem":1},{"lineBreak":true},{"text":"He will ever be mindful of his covenant.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He hath shown his people the power of his works,","poem":1},{"lineBreak":true},{"text":"In giving them the heritage of the nations.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"The works of his hands are truth and justice;","poem":1},{"lineBreak":true},{"text":"All his precepts are sure.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They are established forever and ever;","poem":1},{"lineBreak":true},{"text":"They are done in truth and uprightness.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He hath sent redemption unto his people;","poem":1},{"lineBreak":true},{"text":"He hath commanded his covenant forever:","poem":1},{"lineBreak":true},{"text":"Holy and reverend is his name.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"The fear of Jehovah is the beginning of wisdom;","poem":1},{"lineBreak":true},{"text":"A good understanding have all they that do his commandments:","poem":1},{"lineBreak":true},{"text":"His praise endureth forever.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/111.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/112.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/110.json","previousChapterAudioLinks":{},"numberOfVerses":10}