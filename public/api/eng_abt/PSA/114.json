{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":114,"content":[{"type":"verse","number":1,"content":[{"text":"When Israel went forth out of Egypt,","poem":1},{"lineBreak":true},{"text":"The house of Jacob from a people of strange language;","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Judah became his sanctuary,","poem":1},{"lineBreak":true},{"text":"Israel his dominion.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The sea saw it, and fled;","poem":1},{"lineBreak":true},{"text":"The Jordan was driven back.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The mountains skipped like rams,","poem":1},{"lineBreak":true},{"text":"The little hills like lambs.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"What aileth thee, O thou sea, that thou fleest?","poem":1},{"lineBreak":true},{"text":"Thou Jordan, that thou turnest back?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Ye mountains, that ye skip like rams;","poem":1},{"lineBreak":true},{"text":"Ye little hills, like lambs?","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Tremble, thou earth, at the presence of the Lord,","poem":1},{"lineBreak":true},{"text":"At the presence of the God of Jacob,","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Who turned the rock into a pool of water,","poem":1},{"lineBreak":true},{"text":"The flint into a fountain of waters.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/114.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/115.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/113.json","previousChapterAudioLinks":{},"numberOfVerses":8}