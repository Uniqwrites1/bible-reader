{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":142,"content":[{"type":"hebrew_subtitle","content":["Maskil of David, when he was in the cave; a Prayer."]},{"type":"verse","number":1,"content":[{"text":"I cry with my voice unto Jehovah;","poem":1},{"lineBreak":true},{"text":"With my voice unto Jehovah do I make supplication.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I pour out my complaint before him;","poem":1},{"lineBreak":true},{"text":"I show before him my trouble.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"When my spirit was overwhelmed within me,","poem":1},{"lineBreak":true},{"text":"Thou knewest my path.","poem":1},{"lineBreak":true},{"text":"In the way wherein I walk","poem":1},{"lineBreak":true},{"text":"Have they hidden a snare for me.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Look on my right hand, and see;","poem":1},{"lineBreak":true},{"text":"For there is no man that knoweth me:","poem":1},{"lineBreak":true},{"text":"Refuge hath failed me;","poem":1},{"lineBreak":true},{"text":"No man careth for my soul.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I cried unto thee, O Jehovah;","poem":1},{"lineBreak":true},{"text":"I said, Thou art my refuge,","poem":1},{"lineBreak":true},{"text":"My portion in the land of the living.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Attend unto my cry;","poem":1},{"lineBreak":true},{"text":"For I am brought very low:","poem":1},{"lineBreak":true},{"text":"Deliver me from my persecutors;","poem":1},{"lineBreak":true},{"text":"For they are stronger than I.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Bring my soul out of prison,","poem":1},{"lineBreak":true},{"text":"That I may give thanks unto thy name:","poem":1},{"lineBreak":true},{"text":"The righteous shall compass me about;","poem":1},{"lineBreak":true},{"text":"For thou wilt deal bountifully with me.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/142.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/143.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/141.json","previousChapterAudioLinks":{},"numberOfVerses":7}