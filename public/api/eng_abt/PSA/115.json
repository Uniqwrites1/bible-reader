{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":115,"content":[{"type":"verse","number":1,"content":[{"text":"Not unto us, O Jehovah, not unto us,","poem":1},{"lineBreak":true},{"text":"But unto thy name give glory,","poem":1},{"lineBreak":true},{"text":"For thy lovingkindness, and for thy truth’s sake.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Wherefore should the nations say,","poem":1},{"lineBreak":true},{"text":"Where is now their God?","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"But our God is in the heavens:","poem":1},{"lineBreak":true},{"text":"He hath done whatsoever he pleased.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Their idols are silver and gold,","poem":1},{"lineBreak":true},{"text":"The work of men’s hands.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"They have mouths, but they speak not;","poem":1},{"lineBreak":true},{"text":"Eyes have they, but they see not;","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"They have ears, but they hear not;","poem":1},{"lineBreak":true},{"text":"Noses have they, but they smell not;","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"They have hands, but they handle not;","poem":1},{"lineBreak":true},{"text":"Feet have they, but they walk not;","poem":1},{"lineBreak":true},{"text":"Neither speak they through their throat.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They that make them shall be like unto them;","poem":1},{"lineBreak":true},{"text":"Yea, everyone that trusteth in them.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"O Israel, trust thou in Jehovah:","poem":1},{"lineBreak":true},{"text":"He is their help and their shield.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"O house of Aaron, trust ye in Jehovah:","poem":1},{"lineBreak":true},{"text":"He is their help and their shield.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Ye that fear Jehovah, trust in Jehovah:","poem":1},{"lineBreak":true},{"text":"He is their help and their shield.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Jehovah hath been mindful of us; he will bless us:","poem":1},{"lineBreak":true},{"text":"He will bless the house of Israel;","poem":1},{"lineBreak":true},{"text":"He will bless the house of Aaron.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"He will bless them that fear Jehovah,","poem":1},{"lineBreak":true},{"text":"Both small and great.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"Jehovah increase you more and more,","poem":1},{"lineBreak":true},{"text":"You and your children.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"Blessed are ye of Jehovah,","poem":1},{"lineBreak":true},{"text":"Who made heaven and earth.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"The heavens are the heavens of Jehovah;","poem":1},{"lineBreak":true},{"text":"But the earth hath he given to the children of men.","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"The dead praise not Jehovah,","poem":1},{"lineBreak":true},{"text":"Neither any that go down into silence;","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"But we will bless Jehovah","poem":1},{"lineBreak":true},{"text":"From this time forth and forevermore.","poem":1},{"lineBreak":true},{"text":"Praise ye Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/115.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/116.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/114.json","previousChapterAudioLinks":{},"numberOfVerses":18}