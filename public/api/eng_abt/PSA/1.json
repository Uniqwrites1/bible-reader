{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":1,"content":[{"type":"verse","number":1,"content":[{"text":"Blessed is the man that walketh not in the counsel of the wicked,","poem":1},{"lineBreak":true},{"text":"Nor standeth in the way of sinners,","poem":1},{"lineBreak":true},{"text":"Nor sitteth in the seat of scoffers:","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"But his delight is in the law of Jehovah;","poem":1},{"lineBreak":true},{"text":"And on his law doth he meditate day and night.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"And he shall be like a tree planted by the streams of water,","poem":1},{"lineBreak":true},{"text":"That bringeth forth its fruit in its season,","poem":1},{"lineBreak":true},{"text":"Whose leaf also doth not wither;","poem":1},{"lineBreak":true},{"text":"And whatsoever he doeth shall prosper.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The wicked are not so,","poem":1},{"lineBreak":true},{"text":"But are like the chaff which the wind driveth away.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Therefore the wicked shall not stand in the judgment,","poem":1},{"lineBreak":true},{"text":"Nor sinners in the congregation of the righteous.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For Jehovah knoweth the way of the righteous;","poem":1},{"lineBreak":true},{"text":"But the way of the wicked shall perish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/1.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/2.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/JOB/42.json","previousChapterAudioLinks":{},"numberOfVerses":6}