":"verse","number":5,"content":[{"lineBreak":true},{"text":"But I have trusted in thy lovingkindness;","poem":1},{"lineBreak":true},{"text":"My heart shall rejoice in thy salvation.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will sing unto Jehovah,","poem":1},{"lineBreak":true},{"text":"Because he hath dealt bountifully with me.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/13.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/14.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/12.json","previousChapterAudioLinks":{},"numberOfVerses":6}