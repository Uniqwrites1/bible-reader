{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":97,"content":[{"type":"verse","number":1,"content":[{"text":"Jehovah reigneth; let the earth rejoice;","poem":1},{"lineBreak":true},{"text":"Let the multitude of isles be glad.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Clouds and darkness are round about him:","poem":1},{"lineBreak":true},{"text":"Righteousness and justice are the foundation of his throne.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"A fire goeth before him,","poem":1},{"lineBreak":true},{"text":"And burneth up his adversaries round about.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"His lightnings lightened the world:","poem":1},{"lineBreak":true},{"text":"The earth saw, and trembled.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The mountains melted like wax at the presence of Jehovah,","poem":1},{"lineBreak":true},{"text":"At the presence of the Lord of the whole earth.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"The heavens declare his righteousness,","poem":1},{"lineBreak":true},{"text":"And all the peoples have seen his glory.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Let all them be put to shame that serve graven images,","poem":1},{"lineBreak":true},{"text":"That boast themselves of idols:","poem":1},{"lineBreak":true},{"text":"Worship him, all ye gods.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Zion heard and was glad,","poem":1},{"lineBreak":true},{"text":"And the daughters of Judah rejoiced,","poem":1},{"lineBreak":true},{"text":"Because of thy judgments, O Jehovah.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"For thou, Jehovah, art most high above all the earth:","poem":1},{"lineBreak":true},{"text":"Thou art exalted far above all gods.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"O ye that love Jehovah, hate evil:","poem":1},{"lineBreak":true},{"text":"He preserveth the souls of his saints;","poem":1},{"lineBreak":true},{"text":"He delivereth them out of the hand of the wicked.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Light is sown for the righteous,","poem":1},{"lineBreak":true},{"text":"And gladness for the upright in heart.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Be glad in Jehovah, ye righteous;","poem":1},{"lineBreak":true},{"text":"And give thanks to his holy memorial name.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/97.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/98.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/96.json","previousChapterAudioLinks":{},"numberOfVerses":12}