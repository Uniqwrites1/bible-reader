{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":16,"content":[{"type":"hebrew_subtitle","content":["Miktam of David."]},{"type":"verse","number":1,"content":[{"text":"Preserve me, O God; for in thee do I take refuge.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"O my soul, thou hast said unto Jehovah, Thou art my Lord:","poem":1},{"lineBreak":true},{"text":"I have no good beyond thee.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"As for the saints that are in the earth,","poem":1},{"lineBreak":true},{"text":"They are the excellent in whom is all my delight.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Their sorrows shall be multiplied that give gifts for another god:","poem":1},{"lineBreak":true},{"text":"Their drink offerings of blood will I not offer,","poem":1},{"lineBreak":true},{"text":"Nor take their names upon my lips.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Jehovah is the portion of mine inheritance and of my cup:","poem":1},{"lineBreak":true},{"text":"Thou maintainest my lot.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"The lines are fallen unto me in pleasant places;","poem":1},{"lineBreak":true},{"text":"Yea, I have a goodly heritage.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"I will bless Jehovah, who hath given me counsel;","poem":1},{"lineBreak":true},{"text":"Yea, my heart instructeth me in the night seasons.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"I have set Jehovah always before me:","poem":1},{"lineBreak":true},{"text":"Because he is at my right hand, I shall not be moved.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Therefore my heart is glad, and my glory rejoiceth:","poem":1},{"lineBreak":true},{"text":"My flesh also shall dwell in safety.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"For thou wilt not leave my soul to Sheol;","poem":1},{"lineBreak":true},{"text":"Neither wilt thou suffer thy holy one to see corruption.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Thou wilt show me the path of life:","poem":1},{"lineBreak":true},{"text":"In thy presence is fullness of joy;","poem":1},{"lineBreak":true},{"text":"In thy right hand there are pleasures forevermore.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/16.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/17.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/15.json","previousChapterAudioLinks":{},"numberOfVerses":11}