{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":98,"content":[{"type":"hebrew_subtitle","content":["A Psalm."]},{"type":"verse","number":1,"content":[{"text":"Oh sing unto Jehovah a new song;","poem":1},{"lineBreak":true},{"text":"For he hath done marvelous things:","poem":1},{"lineBreak":true},{"text":"His right hand, and his holy arm, hath wrought salvation for him.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Jehovah hath made known his salvation:","poem":1},{"lineBreak":true},{"text":"His righteousness hath he openly shown in the sight of the nations.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"He hath remembered his lovingkindness and his faithfulness toward the house of Israel:","poem":1},{"lineBreak":true},{"text":"All the ends of the earth have seen the salvation of our God.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Make a joyful noise unto Jehovah, all the earth:","poem":1},{"lineBreak":true},{"text":"Break forth and sing for joy, yea, sing praises.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Sing praises unto Jehovah with the harp;","poem":1},{"lineBreak":true},{"text":"With the harp and the voice of melody.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"With trumpets and sound of cornet","poem":1},{"lineBreak":true},{"text":"Make a joyful noise before the King, Jehovah.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Let the sea roar, and the fullness thereof;","poem":1},{"lineBreak":true},{"text":"The world, and they that dwell therein;","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Let the floods clap their hands;","poem":1},{"lineBreak":true},{"text":"Let the hills sing for joy together","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Before Jehovah; for he cometh to judge the earth:","poem":1},{"lineBreak":true},{"text":"He will judge the world with righteousness,","poem":1},{"lineBreak":true},{"text":"And the peoples with equity.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/98.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/99.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/97.json","previousChapterAudioLinks":{},"numberOfVerses":9}