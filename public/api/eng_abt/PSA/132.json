{"translation":{"id":"eng_abt","name":"American Standard Version Byzantine Text","website":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engasvbt","licenseNotes":null,"shortName":"ABT","englishName":"American Standard Version Byzantine Text","language":"eng","textDirection":"ltr","sha256":"75dee28c05837f87b0433a8f9eb6e00ed7393b18446e492fa08e3457eadcd50d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_abt/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31098,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_abt","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"6f1e14e58a2bd60026d935a29789fc0f57abb70894fb53fa7776d8646c5d2f19","firstChapterApiLink":"/api/eng_abt/PSA/1.json","lastChapterApiLink":"/api/eng_abt/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":132,"content":[{"type":"hebrew_subtitle","content":["A Song of Ascents."]},{"type":"verse","number":1,"content":[{"text":"Jehovah, remember for David","poem":1},{"lineBreak":true},{"text":"All his affliction;","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"How he sware unto Jehovah,","poem":1},{"lineBreak":true},{"text":"And vowed unto the Mighty One of Jacob:","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Surely I will not come into the tabernacle of my house,","poem":1},{"lineBreak":true},{"text":"Nor go up into my bed;","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I will not give sleep to mine eyes,","poem":1},{"lineBreak":true},{"text":"Or slumber to mine eyelids;","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Until I find out a place for Jehovah,","poem":1},{"lineBreak":true},{"text":"A tabernacle for the Mighty One of Jacob.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Lo, we heard of it in Ephrathah:","poem":1},{"lineBreak":true},{"text":"We found it in the field of the wood.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"We will go into his tabernacles;","poem":1},{"lineBreak":true},{"text":"We will worship at his footstool.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Arise, O Jehovah, into thy resting place;","poem":1},{"lineBreak":true},{"text":"Thou, and the ark of thy strength.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Let thy priests be clothed with righteousness;","poem":1},{"lineBreak":true},{"text":"And let thy saints shout for joy.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"For thy servant Davidâ€™s sake","poem":1},{"lineBreak":true},{"text":"Turn not away the face of thine anointed.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Jehovah hath sworn unto David in truth;","poem":1},{"lineBreak":true},{"text":"He will not turn from it:","poem":1},{"lineBreak":true},{"text":"Of the fruit of thy body will I set upon thy throne.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"If thy children will keep my covenant","poem":1},{"lineBreak":true},{"text":"And my testimony that I shall teach them,","poem":1},{"lineBreak":true},{"text":"Their children also shall sit upon thy throne forevermore.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"For Jehovah hath chosen Zion;","poem":1},{"lineBreak":true},{"text":"He hath desired it for his habitation.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"This is my resting place forever:","poem":1},{"lineBreak":true},{"text":"Here will I dwell; for I have desired it.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"I will abundantly bless her provision:","poem":1},{"lineBreak":true},{"text":"I will satisfy her poor with bread.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"Her priests also will I clothe with salvation;","poem":1},{"lineBreak":true},{"text":"And her saints shall shout aloud for joy.","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"There will I make the horn of David to bud:","poem":1},{"lineBreak":true},{"text":"I have ordained a lamp for mine anointed.","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"His enemies will I clothe with shame;","poem":1},{"lineBreak":true},{"text":"But upon himself shall his crown flourish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_abt/PSA/132.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_abt/PSA/133.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_abt/PSA/131.json","previousChapterAudioLinks":{},"numberOfVerses":18}