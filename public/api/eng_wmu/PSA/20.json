{"translation":{"id":"eng_wmu","name":"World Messianic Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwmbb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmbb","licenseNotes":null,"shortName":"WMU","englishName":"World Messianic Bible British Edition","language":"eng","textDirection":"ltr","sha256":"62ea503e16e13853621cf02b8f8eaaa592da58240fb2f88205a26d7fc5bf228b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmu/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmu","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"9e63e7e7e5c19c87a87ed3de54d1b750d3371927252776f90a7ecbe61b0b1134","firstChapterApiLink":"/api/eng_wmu/PSA/1.json","lastChapterApiLink":"/api/eng_wmu/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":20,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"May the LORD answer you in the day of trouble.","poem":1},{"text":"May the name of the God of Jacob set you up on high,","poem":2}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"send you help from the sanctuary,","poem":2},{"lineBreak":true},{"text":"grant you support from Zion,","poem":2}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"remember all your offerings,","poem":2},{"lineBreak":true},{"text":"and accept your burnt sacrifice. Selah.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"May he grant you your heartâ€™s desire,","poem":1},{"text":"and fulfil all your counsel.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"We will triumph in your salvation.","poem":1},{"text":"In the name of our God, we will set up our banners.","poem":2},{"lineBreak":true},{"text":"May the LORD grant all your requests.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Now I know that the LORD saves his anointed.","poem":1},{"text":"He will answer him from his holy heaven,","poem":2},{"lineBreak":true},{"text":"with the saving strength of his right hand.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Some trust in chariots, and some in horses,","poem":1},{"text":"but we trust in the name of the LORD our God.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"They are bowed down and fallen,","poem":1},{"text":"but we rise up, and stand upright.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Save, LORD!","poem":1},{"text":"Let the King answer us when we call!","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmu/PSA/20.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmu/PSA/21.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmu/PSA/19.json","previousChapterAudioLinks":{},"numberOfVerses":9}