{"translation":{"id":"eng_wmu","name":"World Messianic Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwmbb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmbb","licenseNotes":null,"shortName":"WMU","englishName":"World Messianic Bible British Edition","language":"eng","textDirection":"ltr","sha256":"62ea503e16e13853621cf02b8f8eaaa592da58240fb2f88205a26d7fc5bf228b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmu/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmu","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"9e63e7e7e5c19c87a87ed3de54d1b750d3371927252776f90a7ecbe61b0b1134","firstChapterApiLink":"/api/eng_wmu/PSA/1.json","lastChapterApiLink":"/api/eng_wmu/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":99,"content":[{"type":"verse","number":1,"content":[{"text":"The LORD reigns! Let the peoples tremble.","poem":1},{"text":"He sits enthroned amongst the cherubim.","poem":2},{"lineBreak":true},{"text":"Let the earth be moved.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"The LORD is great in Zion.","poem":1},{"text":"He is high above all the peoples.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them praise your great and awesome name.","poem":1},{"text":"He is Holy!","poem":2}]},{"type":"line_break"},{"type":"verse","number":4,"content":[{"text":"The Kingâ€™s strength also loves justice.","poem":1},{"text":"You establish equity.","poem":2},{"lineBreak":true},{"text":"You execute justice and righteousness in Jacob.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Exalt the LORD our God.","poem":1},{"text":"Worship at his footstool.","poem":2},{"lineBreak":true},{"text":"He is Holy!","poem":2}]},{"type":"line_break"},{"type":"verse","number":6,"content":[{"text":"Moses and Aaron were amongst his priests,","poem":1},{"text":"Samuel was amongst those who call on his name.","poem":2},{"lineBreak":true},{"text":"They called on the LORD, and he answered them.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"He spoke to them in the pillar of cloud.","poem":1},{"text":"They kept his testimonies,","poem":2},{"lineBreak":true},{"text":"the statute that he gave them.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"You answered them, LORD our God.","poem":1},{"text":"You are a God who forgave them,","poem":2},{"lineBreak":true},{"text":"although you took vengeance for their doings.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Exalt the LORD, our God.","poem":1},{"text":"Worship at his holy hill,","poem":2},{"lineBreak":true},{"text":"for the LORD, our God, is holy!","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmu/PSA/99.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmu/PSA/100.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmu/PSA/98.json","previousChapterAudioLinks":{},"numberOfVerses":9}