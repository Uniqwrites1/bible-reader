rse","number":7,"content":[{"text":"Israel, hope in the LORD,","poem":1},{"text":"for there is loving kindness with the LORD.","poem":2},{"lineBreak":true},{"text":"Abundant redemption is with him.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"He will redeem Israel from all their sins.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmu/PSA/130.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmu/PSA/131.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmu/PSA/129.json","previousChapterAudioLinks":{},"numberOfVerses":8}