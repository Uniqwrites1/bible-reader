{"translation":{"id":"eng_web","name":"World English Bible with Deuterocanon","website":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseNotes":null,"shortName":"WEB","englishName":"World English Bible with Deuterocanon","language":"eng","textDirection":"ltr","sha256":"279485ea287ffa15c9631c7eaad042d36697288d387fb2e273ac7028382ac06a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_web/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_web","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"60f755f90797c8e1285210697ca69bbcd08c19fdddeeb4fb7329820991b6749e","firstChapterApiLink":"/api/eng_web/PSA/1.json","lastChapterApiLink":"/api/eng_web/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":26,"content":[{"type":"hebrew_subtitle","content":["By David."]},{"type":"verse","number":1,"content":[{"text":"Judge me, Yahweh, for I have walked in my integrity.","poem":1},{"text":"I have trusted also in Yahweh without wavering.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Examine me, Yahweh, and prove me.","poem":1},{"text":"Try my heart and my mind.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"For your loving kindness is before my eyes.","poem":1},{"text":"I have walked in your truth.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"I have not sat with deceitful men,","poem":1},{"text":"neither will I go in with hypocrites.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I hate the assembly of evildoers,","poem":1},{"text":"and will not sit with the wicked.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I will wash my hands in innocence,","poem":1},{"text":"so I will go about your altar, Yahweh,","poem":2}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"that I may make the voice of thanksgiving to be heard","poem":2},{"lineBreak":true},{"text":"and tell of all your wondrous deeds.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Yahweh, I love the habitation of your house,","poem":1},{"text":"the place where your glory dwells.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Donâ€™t gather my soul with sinners,","poem":1},{"text":"nor my life with bloodthirsty men","poem":2}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"in whose hands is wickedness;","poem":2},{"lineBreak":true},{"text":"their right hand is full of bribes.","poem":2}]},{"type":"line_break"},{"type":"verse","number":11,"content":[{"text":"But as for me, I will walk in my integrity.","poem":1},{"text":"Redeem me, and be merciful to me.","poem":2}]},{"type":"verse","number":12,"content":[{"text":"My foot stands in an even place.","poem":1},{"text":"In the congregations I will bless Yahweh.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_web/PSA/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_web/PSA/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_web/PSA/25.json","previousChapterAudioLinks":{},"numberOfVerses":12}