{"translation":{"id":"eng_web","name":"World English Bible with Deuterocanon","website":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseNotes":null,"shortName":"WEB","englishName":"World English Bible with Deuterocanon","language":"eng","textDirection":"ltr","sha256":"279485ea287ffa15c9631c7eaad042d36697288d387fb2e273ac7028382ac06a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_web/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_web","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"60f755f90797c8e1285210697ca69bbcd08c19fdddeeb4fb7329820991b6749e","firstChapterApiLink":"/api/eng_web/PSA/1.json","lastChapterApiLink":"/api/eng_web/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":130,"content":[{"type":"hebrew_subtitle","content":["A Song of Ascents."]},{"type":"verse","number":1,"content":[{"text":"Out of the depths I have cried to you, Yahweh.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Lord, hear my voice.","poem":1},{"text":"Let your ears be attentive to the voice of my petitions.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"If you, Yah, kept a record of sins,","poem":1},{"text":"Lord, who could stand?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"But there is forgiveness with you,","poem":1},{"text":"therefore you are feared.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I wait for Yahweh.","poem":1},{"text":"My soul waits.","poem":2},{"lineBreak":true},{"text":"I hope in his word.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"My soul longs for the Lord more than watchmen long for the morning,","poem":1},{"text":"more than watchmen for the morning.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Israel, hope in Yahweh,","poem":1},{"text":"for there is loving kindness with Yahweh.","poem":2},{"lineBreak":true},{"text":"Abundant redemption is with him.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"He will redeem Israel from all their sins.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_web/PSA/130.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_web/PSA/131.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_web/PSA/129.json","previousChapterAudioLinks":{},"numberOfVerses":8}