{"translation":{"id":"eng_web","name":"World English Bible with Deuterocanon","website":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseNotes":null,"shortName":"WEB","englishName":"World English Bible with Deuterocanon","language":"eng","textDirection":"ltr","sha256":"279485ea287ffa15c9631c7eaad042d36697288d387fb2e273ac7028382ac06a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_web/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_web","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"60f755f90797c8e1285210697ca69bbcd08c19fdddeeb4fb7329820991b6749e","firstChapterApiLink":"/api/eng_web/PSA/1.json","lastChapterApiLink":"/api/eng_web/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":101,"content":[{"type":"hebrew_subtitle","content":["A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"I will sing of loving kindness and justice.","poem":1},{"text":"To you, Yahweh, I will sing praises.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"I will be careful to live a blameless life.","poem":1},{"text":"When will you come to me?","poem":2},{"lineBreak":true},{"text":"I will walk within my house with a blameless heart.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"I will set no vile thing before my eyes.","poem":1},{"text":"I hate the deeds of faithless men.","poem":2},{"lineBreak":true},{"text":"They will not cling to me.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"A perverse heart will be far from me.","poem":1},{"text":"I will have nothing to do with evil.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I will silence whoever secretly slanders his neighbor.","poem":1},{"text":"I won’t tolerate one who is arrogant and conceited.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"My eyes will be on the faithful of the land,","poem":1},{"text":"that they may dwell with me.","poem":2},{"text":"He who walks in a perfect way,","poem":1},{"text":"he will serve me.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"He who practices deceit won’t dwell within my house.","poem":1},{"text":"He who speaks falsehood won’t be established before my eyes.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Morning by morning, I will destroy all the wicked of the land,","poem":1},{"text":"to cut off all the workers of iniquity from Yahweh’s city.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_web/PSA/101.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_web/PSA/102.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_web/PSA/100.json","previousChapterAudioLinks":{},"numberOfVerses":8}