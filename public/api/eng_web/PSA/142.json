{"translation":{"id":"eng_web","name":"World English Bible with Deuterocanon","website":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseNotes":null,"shortName":"WEB","englishName":"World English Bible with Deuterocanon","language":"eng","textDirection":"ltr","sha256":"279485ea287ffa15c9631c7eaad042d36697288d387fb2e273ac7028382ac06a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_web/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_web","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"60f755f90797c8e1285210697ca69bbcd08c19fdddeeb4fb7329820991b6749e","firstChapterApiLink":"/api/eng_web/PSA/1.json","lastChapterApiLink":"/api/eng_web/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":142,"content":[{"type":"hebrew_subtitle","content":["A contemplation by David, when he was in the cave. A Prayer."]},{"type":"verse","number":1,"content":[{"text":"I cry with my voice to Yahweh.","poem":1},{"text":"With my voice, I ask Yahweh for mercy.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"I pour out my complaint before him.","poem":1},{"text":"I tell him my troubles.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"When my spirit was overwhelmed within me,","poem":1},{"text":"you knew my route.","poem":2},{"text":"On the path in which I walk,","poem":1},{"text":"they have hidden a snare for me.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Look on my right, and see;","poem":1},{"text":"for there is no one who is concerned for me.","poem":2},{"lineBreak":true},{"text":"Refuge has fled from me.","poem":2},{"lineBreak":true},{"text":"No one cares for my soul.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I cried to you, Yahweh.","poem":1},{"text":"I said, “You are my refuge,","poem":2},{"lineBreak":true},{"text":"my portion in the land of the living.”","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Listen to my cry,","poem":1},{"text":"for I am in desperate need.","poem":2},{"text":"Deliver me from my persecutors,","poem":1},{"text":"for they are too strong for me.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Bring my soul out of prison,","poem":1},{"text":"that I may give thanks to your name.","poem":2},{"text":"The righteous will surround me,","poem":1},{"text":"for you will be good to me.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_web/PSA/142.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_web/PSA/143.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_web/PSA/141.json","previousChapterAudioLinks":{},"numberOfVerses":7}