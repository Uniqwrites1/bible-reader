{"translation":{"id":"eng_web","name":"World English Bible with Deuterocanon","website":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web","licenseNotes":null,"shortName":"WEB","englishName":"World English Bible with Deuterocanon","language":"eng","textDirection":"ltr","sha256":"279485ea287ffa15c9631c7eaad042d36697288d387fb2e273ac7028382ac06a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_web/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_web","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"60f755f90797c8e1285210697ca69bbcd08c19fdddeeb4fb7329820991b6749e","firstChapterApiLink":"/api/eng_web/PSA/1.json","lastChapterApiLink":"/api/eng_web/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":4,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; on stringed instruments. A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"Answer me when I call, God of my righteousness.","poem":1},{"text":"Give me relief from my distress.","poem":2},{"lineBreak":true},{"text":"Have mercy on me, and hear my prayer.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"You sons of men, how long shall my glory be turned into dishonor?","poem":1},{"text":"Will you love vanity and seek after falsehood? Selah.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"But know that Yahweh has set apart for himself him who is godly;","poem":1},{"text":"Yahweh will hear when I call to him.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Stand in awe, and don’t sin.","poem":1},{"text":"Search your own heart on your bed, and be still. Selah.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Offer the sacrifices of righteousness.","poem":1},{"text":"Put your trust in Yahweh.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Many say, “Who will show us any good?”","poem":1},{"text":"Yahweh, let the light of your face shine on us.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"You have put gladness in my heart,","poem":1},{"text":"more than when their grain and their new wine are increased.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"In peace I will both lay myself down and sleep,","poem":1},{"text":"for you alone, Yahweh, make me live in safety.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_web/PSA/4.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_web/PSA/5.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_web/PSA/3.json","previousChapterAudioLinks":{},"numberOfVerses":8}