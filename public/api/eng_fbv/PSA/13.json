{"translation":{"id":"eng_fbv","name":"Free Bible Version","website":"https://ebible.org/Scriptures/details.php?id=engfbv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engfbv","licenseNotes":null,"shortName":"FBV","englishName":"Free Bible Version","language":"eng","textDirection":"ltr","sha256":"ec5ebacc48bac1e22b7c8c538fd2e2835bb194b0d0726894e640dda11c6744ee","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_fbv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31104,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_fbv","name":"PSALMS","commonName":"Psalms","title":"Psalms","order":19,"numberOfChapters":150,"sha256":"d5be42f6b8a9be1d4670e20c95e550d143f61d9b2ea3d0c6fc534fef7f619d01","firstChapterApiLink":"/api/eng_fbv/PSA/1.json","lastChapterApiLink":"/api/eng_fbv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":13,"content":[{"type":"hebrew_subtitle","content":["For the music director. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"How long, Lord, are you going to forget me? Forever? How long will you turn away from me?","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"How long must I be in inner turmoil, feeling sad all day long? How long will my enemy be victorious over me?","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Think about this, and answer me, Lord my God. Give me back my strength,","poem":1},{"noteId":7056},{"text":"otherwise I'm going to die,","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"and my enemy will say, “I defeated him!” and my opponents will celebrate my downfall.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"But I trust in your never-failing love, I will be happy because you save me.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will sing to the Lord because he has treated me with such generosity!","poem":1}]},{"type":"line_break"}],"footnotes":[{"noteId":7056,"caller":"+","text":"Literally, “give light to my eyes.” Bright eyes were seen as indicative of good health.","reference":{"chapter":13,"verse":3}}]},"thisChapterLink":"/api/eng_fbv/PSA/13.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_fbv/PSA/14.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_fbv/PSA/12.json","previousChapterAudioLinks":{},"numberOfVerses":6}