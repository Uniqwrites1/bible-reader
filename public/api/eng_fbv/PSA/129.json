er":7,"content":[{"lineBreak":true},{"text":"There's not enough even for a reaper to hold, not enough even for the binder to bind.","poem":1},{"noteId":7463}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"May passers-by not say to them, “The blessing of the Lord be on you; we bless you in the name of the Lord.”","poem":1}]},{"type":"line_break"}],"footnotes":[{"noteId":7463,"caller":"+","text":"The image is of something that's completely useless.","reference":{"chapter":129,"verse":7}}]},"thisChapterLink":"/api/eng_fbv/PSA/129.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_fbv/PSA/130.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_fbv/PSA/128.json","previousChapterAudioLinks":{},"numberOfVerses":8}