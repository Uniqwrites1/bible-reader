{"translation":{"id":"eng_fbv","name":"Free Bible Version","website":"https://ebible.org/Scriptures/details.php?id=engfbv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engfbv","licenseNotes":null,"shortName":"FBV","englishName":"Free Bible Version","language":"eng","textDirection":"ltr","sha256":"ec5ebacc48bac1e22b7c8c538fd2e2835bb194b0d0726894e640dda11c6744ee","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_fbv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31104,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_fbv","name":"PSALMS","commonName":"Psalms","title":"Psalms","order":19,"numberOfChapters":150,"sha256":"d5be42f6b8a9be1d4670e20c95e550d143f61d9b2ea3d0c6fc534fef7f619d01","firstChapterApiLink":"/api/eng_fbv/PSA/1.json","lastChapterApiLink":"/api/eng_fbv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":101,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"I will sing of your faithful love and good judgment. I will sing praises to you, Lord.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I will make sure my life is blameless. When will you come and help me? My life will be one of integrity even in private.","poem":1},{"noteId":7368}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"I will not look at anything evil. I hate doing wrong—I won't have anything to do with it.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I won't think bad thoughts—I won't even consider anything evil.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I will silence anyone who secretly slanders their neighbor. I won't tolerate anyone who is proud and arrogant.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I look for faithful people to live with me; only those who are trustworthy will serve me.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"No one who is dishonest will live in my house; no liars will be allowed in my presence.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"I work every day to silence the wicked in the land, to rid the city of the Lord of all who do evil.","poem":1}]},{"type":"line_break"}],"footnotes":[{"noteId":7368,"caller":"+","text":"“Even in private”: literally, “in my house.”","reference":{"chapter":101,"verse":2}}]},"thisChapterLink":"/api/eng_fbv/PSA/101.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_fbv/PSA/102.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_fbv/PSA/100.json","previousChapterAudioLinks":{},"numberOfVerses":8}