ent":[{"text":"Olúwa yóò pa ọ́ mọ́ kúrò nínú ibi gbogbo","poem":1},{"text":"yóò pa ọkàn rẹ mọ́","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Olúwa yóò pa àlọ àti ààbọ̀ rẹ mọ́","poem":1},{"text":"láti ìgbà yìí lọ àti títí láéláé.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/yor_ycb/PSA/121.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/yor_ycb/PSA/122.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/yor_ycb/PSA/120.json","previousChapterAudioLinks":{},"numberOfVerses":8}