er":7,"content":[{"text":"Nítorí Ọlọ́run ni ọba gbogbo ayé,","poem":1},{"text":"ẹ kọrin ìyìn pẹ̀lú Saamu!","poem":2}]},{"type":"line_break"},{"type":"verse","number":8,"content":[{"text":"Ọlọ́run jẹ ọba lórí gbogbo kèfèrí;","poem":1},{"text":"Ọlọ́run jókòó lórí ìtẹ́ ìwà mímọ́ rẹ̀.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Àwọn aládé ayé kó ara wọn jọ","poem":1},{"text":"gẹ́gẹ́ bí ènìyàn Ọlọ́run Abrahamu","poem":2},{"text":"nítorí asà ayé ti Ọlọ́run ni,","poem":1},{"text":"òun ni ó ga jùlọ.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/yor_ycb/PSA/47.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/yor_ycb/PSA/48.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/yor_ycb/PSA/46.json","previousChapterAudioLinks":{},"numberOfVerses":9}