{"translation":{"id":"eng_webpb","name":"World English Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseNotes":null,"shortName":"WEBPB","englishName":"World English Bible British Edition","language":"eng","textDirection":"ltr","sha256":"3587077a18fbf017299592d178ce2a41cbea45b0db8666227afe3b4232f99678","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webpb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webpb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"da0b80c246a45181e07bd664c7e1130d4b4b22d469f1830dfd9dd4dbc932871f","firstChapterApiLink":"/api/eng_webpb/PSA/1.json","lastChapterApiLink":"/api/eng_webpb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"verse","number":1,"content":[{"text":"Vindicate me, God, and plead my cause against an ungodly nation.","poem":1},{"text":"Oh, deliver me from deceitful and wicked men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For you are the God of my strength. Why have you rejected me?","poem":1},{"text":"Why do I go mourning because of the oppression of the enemy?","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Oh, send out your light and your truth.","poem":1},{"text":"Let them lead me.","poem":2},{"lineBreak":true},{"text":"Let them bring me to your holy hill,","poem":2},{"lineBreak":true},{"text":"to your tents.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Then I will go to the altar of God,","poem":1},{"text":"to God, my exceeding joy.","poem":2},{"text":"I will praise you on the harp, God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why are you in despair, my soul?","poem":1},{"text":"Why are you disturbed within me?","poem":2},{"text":"Hope in God!","poem":1},{"text":"For I shall still praise him:","poem":2},{"lineBreak":true},{"text":"my Saviour, my helper, and my God.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webpb/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webpb/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webpb/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}