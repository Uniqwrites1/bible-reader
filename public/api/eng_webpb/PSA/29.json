{"translation":{"id":"eng_webpb","name":"World English Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseNotes":null,"shortName":"WEBPB","englishName":"World English Bible British Edition","language":"eng","textDirection":"ltr","sha256":"3587077a18fbf017299592d178ce2a41cbea45b0db8666227afe3b4232f99678","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webpb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webpb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"da0b80c246a45181e07bd664c7e1130d4b4b22d469f1830dfd9dd4dbc932871f","firstChapterApiLink":"/api/eng_webpb/PSA/1.json","lastChapterApiLink":"/api/eng_webpb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":29,"content":[{"type":"hebrew_subtitle","content":["A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"Ascribe to the LORD, you sons of the mighty,","poem":1},{"text":"ascribe to the LORD glory and strength.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Ascribe to the LORD the glory due to his name.","poem":1},{"text":"Worship the LORD in holy array.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"The LORD’s voice is on the waters.","poem":1},{"text":"The God of glory thunders, even the LORD on many waters.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"The LORD’s voice is powerful.","poem":1},{"text":"The LORD’s voice is full of majesty.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"The LORD’s voice breaks the cedars.","poem":1},{"text":"Yes, the LORD breaks in pieces the cedars of Lebanon.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"He makes them also to skip like a calf;","poem":1},{"text":"Lebanon and Sirion like a young, wild ox.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"The LORD’s voice strikes with flashes of lightning.","poem":1}]},{"type":"verse","number":8,"content":[{"text":"The LORD’s voice shakes the wilderness.","poem":2},{"lineBreak":true},{"text":"The LORD shakes the wilderness of Kadesh.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"The LORD’s voice makes the deer calve,","poem":1},{"text":"and strips the forests bare.","poem":2},{"lineBreak":true},{"text":"In his temple everything says, “Glory!”","poem":2}]},{"type":"line_break"},{"type":"verse","number":10,"content":[{"text":"The LORD sat enthroned at the Flood.","poem":1},{"text":"Yes, the LORD sits as King forever.","poem":2}]},{"type":"verse","number":11,"content":[{"text":"The LORD will give strength to his people.","poem":1},{"text":"The LORD will bless his people with peace.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webpb/PSA/29.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webpb/PSA/30.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webpb/PSA/28.json","previousChapterAudioLinks":{},"numberOfVerses":11}