{"translation":{"id":"eng_webpb","name":"World English Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseNotes":null,"shortName":"WEBPB","englishName":"World English Bible British Edition","language":"eng","textDirection":"ltr","sha256":"3587077a18fbf017299592d178ce2a41cbea45b0db8666227afe3b4232f99678","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webpb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webpb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"da0b80c246a45181e07bd664c7e1130d4b4b22d469f1830dfd9dd4dbc932871f","firstChapterApiLink":"/api/eng_webpb/PSA/1.json","lastChapterApiLink":"/api/eng_webpb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":97,"content":[{"type":"verse","number":1,"content":[{"text":"The LORD reigns!","poem":1},{"text":"Let the earth rejoice!","poem":2},{"lineBreak":true},{"text":"Let the multitude of islands be glad!","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Clouds and darkness are around him.","poem":1},{"text":"Righteousness and justice are the foundation of his throne.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"A fire goes before him,","poem":1},{"text":"and burns up his adversaries on every side.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"His lightning lights up the world.","poem":1},{"text":"The earth sees, and trembles.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"The mountains melt like wax at the presence of the LORD,","poem":1},{"text":"at the presence of the Lord of the whole earth.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"The heavens declare his righteousness.","poem":1},{"text":"All the peoples have seen his glory.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Let all them be shamed who serve engraved images,","poem":1},{"text":"who boast in their idols.","poem":2},{"lineBreak":true},{"text":"Worship him, all you gods!","poem":2},{"noteId":4659}]},{"type":"verse","number":8,"content":[{"text":"Zion heard and was glad.","poem":1},{"text":"The daughters of Judah rejoiced","poem":2},{"lineBreak":true},{"text":"because of your judgements, LORD.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"For you, LORD, are most high above all the earth.","poem":1},{"text":"You are exalted far above all gods.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"You who love the LORD, hate evil!","poem":1},{"text":"He preserves the souls of his saints.","poem":2},{"lineBreak":true},{"text":"He delivers them out of the hand of the wicked.","poem":2}]},{"type":"verse","number":11,"content":[{"text":"Light is sown for the righteous,","poem":1},{"text":"and gladness for the upright in heart.","poem":2}]},{"type":"verse","number":12,"content":[{"text":"Be glad in the LORD, you righteous people!","poem":1},{"text":"Give thanks to his holy Name.","poem":2}]}],"footnotes":[{"noteId":4659,"caller":"+","text":"LXX reads “angels” instead of “gods”.","reference":{"chapter":97,"verse":7}}]},"thisChapterLink":"/api/eng_webpb/PSA/97.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webpb/PSA/98.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webpb/PSA/96.json","previousChapterAudioLinks":{},"numberOfVerses":12}