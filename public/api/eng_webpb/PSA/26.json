{"translation":{"id":"eng_webpb","name":"World English Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseNotes":null,"shortName":"WEBPB","englishName":"World English Bible British Edition","language":"eng","textDirection":"ltr","sha256":"3587077a18fbf017299592d178ce2a41cbea45b0db8666227afe3b4232f99678","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webpb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webpb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"da0b80c246a45181e07bd664c7e1130d4b4b22d469f1830dfd9dd4dbc932871f","firstChapterApiLink":"/api/eng_webpb/PSA/1.json","lastChapterApiLink":"/api/eng_webpb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":26,"content":[{"type":"hebrew_subtitle","content":["By David."]},{"type":"verse","number":1,"content":[{"text":"Judge me, LORD, for I have walked in my integrity.","poem":1},{"text":"I have trusted also in the LORD without wavering.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Examine me, LORD, and prove me.","poem":1},{"text":"Try my heart and my mind.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"For your loving kindness is before my eyes.","poem":1},{"text":"I have walked in your truth.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"I have not sat with deceitful men,","poem":1},{"text":"neither will I go in with hypocrites.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I hate the assembly of evildoers,","poem":1},{"text":"and will not sit with the wicked.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I will wash my hands in innocence,","poem":1},{"text":"so I will go about your altar, LORD,","poem":2}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"that I may make the voice of thanksgiving to be heard","poem":2},{"lineBreak":true},{"text":"and tell of all your wondrous deeds.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"LORD, I love the habitation of your house,","poem":1},{"text":"the place where your glory dwells.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Donâ€™t gather my soul with sinners,","poem":1},{"text":"nor my life with bloodthirsty men","poem":2}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"in whose hands is wickedness;","poem":2},{"lineBreak":true},{"text":"their right hand is full of bribes.","poem":2}]},{"type":"line_break"},{"type":"verse","number":11,"content":[{"text":"But as for me, I will walk in my integrity.","poem":1},{"text":"Redeem me, and be merciful to me.","poem":2}]},{"type":"verse","number":12,"content":[{"text":"My foot stands in an even place.","poem":1},{"text":"In the congregations I will bless the LORD.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webpb/PSA/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webpb/PSA/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webpb/PSA/25.json","previousChapterAudioLinks":{},"numberOfVerses":12}