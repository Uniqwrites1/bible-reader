{"translation":{"id":"eng_webpb","name":"World English Bible British Edition","website":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebpb","licenseNotes":null,"shortName":"WEBPB","englishName":"World English Bible British Edition","language":"eng","textDirection":"ltr","sha256":"3587077a18fbf017299592d178ce2a41cbea45b0db8666227afe3b4232f99678","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webpb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webpb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"da0b80c246a45181e07bd664c7e1130d4b4b22d469f1830dfd9dd4dbc932871f","firstChapterApiLink":"/api/eng_webpb/PSA/1.json","lastChapterApiLink":"/api/eng_webpb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":24,"content":[{"type":"hebrew_subtitle","content":["A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"The earth is the LORD’s, with its fullness;","poem":1},{"text":"the world, and those who dwell in it.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For he has founded it on the seas,","poem":1},{"text":"and established it on the floods.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"Who may ascend to the LORD’s hill?","poem":1},{"text":"Who may stand in his holy place?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"He who has clean hands and a pure heart;","poem":1},{"text":"who has not lifted up his soul to falsehood,","poem":2},{"lineBreak":true},{"text":"and has not sworn deceitfully.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"He shall receive a blessing from the LORD,","poem":1},{"text":"righteousness from the God of his salvation.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"This is the generation of those who seek Him,","poem":1},{"text":"who seek your face—even Jacob. Selah.","poem":2}]},{"type":"line_break"},{"type":"verse","number":7,"content":[{"text":"Lift up your heads, you gates!","poem":1},{"text":"Be lifted up, you everlasting doors,","poem":2},{"lineBreak":true},{"text":"and the King of glory will come in.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Who is the King of glory?","poem":1},{"text":"The LORD strong and mighty,","poem":2},{"lineBreak":true},{"text":"the LORD mighty in battle.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Lift up your heads, you gates;","poem":1},{"text":"yes, lift them up, you everlasting doors,","poem":2},{"lineBreak":true},{"text":"and the King of glory will come in.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"Who is this King of glory?","poem":1},{"text":"The LORD of Armies is the King of glory! Selah.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webpb/PSA/24.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webpb/PSA/25.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webpb/PSA/23.json","previousChapterAudioLinks":{},"numberOfVerses":10}