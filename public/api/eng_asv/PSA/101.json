{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":101,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"I will sing of lovingkindness and justice:","poem":1},{"lineBreak":true},{"text":"Unto thee, O Jehovah, will I sing praises.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I will behave myself wisely in a perfect way:","poem":1},{"lineBreak":true},{"text":"Oh when wilt thou come unto me?","poem":1},{"lineBreak":true},{"text":"I will walk within my house with a perfect heart.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"I will set no base thing before mine eyes:","poem":1},{"lineBreak":true},{"text":"I hate the work of them that turn aside;","poem":1},{"lineBreak":true},{"text":"It shall not cleave unto me.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"A perverse heart shall depart from me:","poem":1},{"lineBreak":true},{"text":"I will know no evil thing.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Whoso privily slandereth his neighbor, him will I destroy:","poem":1},{"lineBreak":true},{"text":"Him that hath a high look and a proud heart will I not suffer.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Mine eyes shall be upon the faithful of the land, that they may dwell with me:","poem":1},{"lineBreak":true},{"text":"He that walketh in a perfect way, he shall minister unto me.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He that worketh deceit shall not dwell within my house:","poem":1},{"lineBreak":true},{"text":"He that speaketh falsehood shall not be established before mine eyes.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Morning by morning will I destroy all the wicked of the land;","poem":1},{"lineBreak":true},{"text":"To cut off all the workers of iniquity from the city of Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/101.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/102.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/100.json","previousChapterAudioLinks":{},"numberOfVerses":8}