{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":141,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Jehovah, I have called upon thee; make haste unto me:","poem":1},{"lineBreak":true},{"text":"Give ear unto my voice, when I call unto thee.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let my prayer be set forth as incense before thee;","poem":1},{"lineBreak":true},{"text":"The lifting up of my hands as the evening sacrifice.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Set a watch, O Jehovah, before my mouth;","poem":1},{"lineBreak":true},{"text":"Keep the door of my lips.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Incline not my heart to any evil thing,","poem":1},{"lineBreak":true},{"text":"To practise deeds of wickedness","poem":1},{"lineBreak":true},{"text":"With men that work iniquity:","poem":1},{"lineBreak":true},{"text":"And let me not eat of their dainties.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let the righteous smite me, it shall be a kindness;","poem":1},{"lineBreak":true},{"text":"And let him reprove me, it shall be as oil upon the head;","poem":1},{"lineBreak":true},{"text":"Let not my head refuse it:","poem":1},{"lineBreak":true},{"text":"For even in their wickedness shall my prayer continue.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Their judges are thrown down by the sides of the rock;","poem":1},{"lineBreak":true},{"text":"And they shall hear my words; for they are sweet.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"As when one ploweth and cleaveth the earth,","poem":1},{"lineBreak":true},{"text":"Our bones are scattered at the mouth of Sheol.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"For mine eyes are unto thee, O Jehovah the Lord:","poem":1},{"lineBreak":true},{"text":"In thee do I take refuge; leave not my soul destitute.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Keep me from the snare which they have laid for me,","poem":1},{"lineBreak":true},{"text":"And from the gins of the workers of iniquity.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Let the wicked fall into their own nets,","poem":1},{"lineBreak":true},{"text":"Whilst that I withal escape.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/141.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/142.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/140.json","previousChapterAudioLinks":{},"numberOfVerses":10}