lly,","poem":1},{"lineBreak":true},{"text":"Let God be magnified.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"But I am poor and needy;","poem":1},{"lineBreak":true},{"text":"Make haste unto me, O God:","poem":1},{"lineBreak":true},{"text":"Thou art my help and my deliverer;","poem":1},{"lineBreak":true},{"text":"O Jehovah, make no tarrying.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/70.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/71.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/69.json","previousChapterAudioLinks":{},"numberOfVerses":5}