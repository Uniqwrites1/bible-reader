{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":3,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David, when he fled from Absalom his son."]},{"type":"verse","number":1,"content":[{"text":"Jehovah, how are mine adversaries increased!","poem":1},{"lineBreak":true},{"text":"Many are they that rise up against me.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Many there are that say of my soul,","poem":1},{"lineBreak":true},{"text":"There is no help for him in God. [Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"But thou, O Jehovah, art a shield about me;","poem":1},{"lineBreak":true},{"text":"My glory, and the lifter up of my head.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I cry unto Jehovah with my voice,","poem":1},{"lineBreak":true},{"text":"And he answereth me out of his holy hill. [Selah","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I laid me down and slept;","poem":1},{"lineBreak":true},{"text":"I awaked; for Jehovah sustaineth me.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will not be afraid of ten thousands of the people","poem":1},{"lineBreak":true},{"text":"That have set themselves against me round about.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Arise, O Jehovah; save me, O my God:","poem":1},{"lineBreak":true},{"text":"For thou hast smitten all mine enemies upon the cheek bone;","poem":1},{"lineBreak":true},{"text":"Thou hast broken the teeth of the wicked.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Salvation belongeth unto Jehovah:","poem":1},{"lineBreak":true},{"text":"Thy blessing be upon thy people. [Selah","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/3.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/4.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/2.json","previousChapterAudioLinks":{},"numberOfVerses":8}