{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":29,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Ascribe unto Jehovah, O ye sons of the mighty,","poem":1},{"lineBreak":true},{"text":"Ascribe unto Jehovah glory and strength.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Ascribe unto Jehovah the glory due unto his name;","poem":1},{"lineBreak":true},{"text":"Worship Jehovah in holy array.","poem":1}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"The voice of Jehovah is upon the waters:","poem":1},{"lineBreak":true},{"text":"The God of glory thundereth,","poem":1},{"lineBreak":true},{"text":"Even Jehovah upon many waters.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The voice of Jehovah is powerful;","poem":1},{"lineBreak":true},{"text":"The voice of Jehovah is full of majesty.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The voice of Jehovah breaketh the cedars;","poem":1},{"lineBreak":true},{"text":"Yea, Jehovah breaketh in pieces the cedars of Lebanon.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He maketh them also to skip like a calf;","poem":1},{"lineBreak":true},{"text":"Lebanon and Sirion like a young wild-ox.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"The voice of Jehovah cleaveth the flames of fire.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"The voice of Jehovah shaketh the wilderness;","poem":1},{"lineBreak":true},{"text":"Jehovah shaketh the wilderness of Kadesh.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"The voice of Jehovah maketh the hinds to calve,","poem":1},{"lineBreak":true},{"text":"And strippeth the forests bare:","poem":1},{"lineBreak":true},{"text":"And in his temple everything saith, Glory.","poem":1}]},{"type":"line_break"},{"type":"verse","number":10,"content":[{"text":"Jehovah sat as King at the Flood;","poem":1},{"lineBreak":true},{"text":"Yea, Jehovah sitteth as King for ever.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Jehovah will give strength unto his people;","poem":1},{"lineBreak":true},{"text":"Jehovah will bless his people with peace.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/29.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/30.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/28.json","previousChapterAudioLinks":{},"numberOfVerses":11}