{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"verse","number":1,"content":[{"text":"Judge me, O God, and plead my cause against an ungodly nation:","poem":1},{"lineBreak":true},{"text":"Oh deliver me from the deceitful and unjust man.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"For thou art the God of my strength; why hast thou cast me off?","poem":1},{"lineBreak":true},{"text":"Why go I mourning because of the oppression of the enemy?","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Oh send out thy light and thy truth; let them lead me:","poem":1},{"lineBreak":true},{"text":"Let them bring me unto thy holy hill,","poem":1},{"lineBreak":true},{"text":"And to thy tabernacles.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Then will I go unto the altar of God,","poem":1},{"lineBreak":true},{"text":"Unto God my exceeding joy;","poem":1},{"lineBreak":true},{"text":"And upon the harp will I praise thee, O God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why art thou cast down, O my soul?","poem":1},{"lineBreak":true},{"text":"And why art thou disquieted within me?","poem":1},{"lineBreak":true},{"text":"Hope thou in God; for I shall yet praise him,","poem":1},{"lineBreak":true},{"text":"Who is the help of my countenance, and my God.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}