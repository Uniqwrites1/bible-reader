{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":108,"content":[{"type":"hebrew_subtitle","content":["A Song, a Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"My heart is fixed, O God;","poem":1},{"lineBreak":true},{"text":"I will sing, yea, I will sing praises, even with my glory.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Awake, psaltery and harp:","poem":1},{"lineBreak":true},{"text":"I myself will awake right early.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"I will give thanks unto thee, O Jehovah, among the peoples;","poem":1},{"lineBreak":true},{"text":"And I will sing praises unto thee among the nations.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For thy lovingkindness is great above the heavens;","poem":1},{"lineBreak":true},{"text":"And thy truth reacheth unto the skies.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Be thou exalted, O God, above the heavens,","poem":1},{"lineBreak":true},{"text":"And thy glory above all the earth.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"That thy beloved may be delivered,","poem":1},{"lineBreak":true},{"text":"Save with thy right hand, and answer us.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"God hath spoken in his holiness: I will exult;","poem":1},{"lineBreak":true},{"text":"I will divide Shechem, and mete out the valley of Succoth.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Gilead is mine; Manasseh is mine;","poem":1},{"lineBreak":true},{"text":"Ephraim also is the defence of my head;","poem":1},{"lineBreak":true},{"text":"Judah is my sceptre.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Moab is my washpot;","poem":1},{"lineBreak":true},{"text":"Upon Edom will I cast my shoe;","poem":1},{"lineBreak":true},{"text":"Over Philistia will I shout.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Who will bring me into the fortified city?","poem":1},{"lineBreak":true},{"text":"Who hath led me unto Edom?","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Hast not thou cast us off, O God?","poem":1},{"lineBreak":true},{"text":"And thou goest not forth, O God, with our hosts.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Give us help against the adversary;","poem":1},{"lineBreak":true},{"text":"For vain is the help of man.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Through God we shall do valiantly:","poem":1},{"lineBreak":true},{"text":"For he it is that will tread down our adversaries.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/108.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/109.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/107.json","previousChapterAudioLinks":{},"numberOfVerses":13}