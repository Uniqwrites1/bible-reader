{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":97,"content":[{"type":"verse","number":1,"content":[{"text":"Jehovah reigneth; let the earth rejoice;","poem":1},{"lineBreak":true},{"text":"Let the multitude of isles be glad.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Clouds and darkness are round about him:","poem":1},{"lineBreak":true},{"text":"Righteousness and justice are the foundation of his throne.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"A fire goeth before him,","poem":1},{"lineBreak":true},{"text":"And burneth up his adversaries round about.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"His lightnings lightened the world:","poem":1},{"lineBreak":true},{"text":"The earth saw, and trembled.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The mountains melted like wax at the presence of Jehovah,","poem":1},{"lineBreak":true},{"text":"At the presence of the Lord of the whole earth.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"The heavens declare his righteousness,","poem":1},{"lineBreak":true},{"text":"And all the peoples have seen his glory.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Let all them be put to shame that serve graven images,","poem":1},{"lineBreak":true},{"text":"That boast themselves of idols:","poem":1},{"lineBreak":true},{"text":"Worship him, all ye gods.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Zion heard and was glad,","poem":1},{"lineBreak":true},{"text":"And the daughters of Judah rejoiced,","poem":1},{"lineBreak":true},{"text":"Because of thy judgments, O Jehovah.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"For thou, Jehovah, art most high above all the earth:","poem":1},{"lineBreak":true},{"text":"Thou art exalted far above all gods.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"O ye that love Jehovah, hate evil:","poem":1},{"lineBreak":true},{"text":"He preserveth the souls of his saints;","poem":1},{"lineBreak":true},{"text":"He delivereth them out of the hand of the wicked.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Light is sown for the righteous,","poem":1},{"lineBreak":true},{"text":"And gladness for the upright in heart.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Be glad in Jehovah, ye righteous;","poem":1},{"lineBreak":true},{"text":"And give thanks to his holy memorial name.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/97.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/98.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/96.json","previousChapterAudioLinks":{},"numberOfVerses":12}