{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":148,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"Praise ye Jehovah from the heavens:","poem":1},{"lineBreak":true},{"text":"Praise him in the heights.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Praise ye him, all his angels:","poem":1},{"lineBreak":true},{"text":"Praise ye him, all his host.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Praise ye him, sun and moon:","poem":1},{"lineBreak":true},{"text":"Praise him, all ye stars of light.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Praise him, ye heavens of heavens,","poem":1},{"lineBreak":true},{"text":"And ye waters that are above the heavens.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let them praise the name of Jehovah;","poem":1},{"lineBreak":true},{"text":"For he commanded, and they were created.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He hath also established them for ever and ever:","poem":1},{"lineBreak":true},{"text":"He hath made a decree which shall not pass away.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Praise Jehovah from the earth,","poem":1},{"lineBreak":true},{"text":"Ye sea-monsters, and all deeps;","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Fire and hail, snow and vapor;","poem":1},{"lineBreak":true},{"text":"Stormy wind, fulfilling his word;","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Mountains and all hills;","poem":1},{"lineBreak":true},{"text":"Fruitful trees and all cedars;","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Beasts and all cattle;","poem":1},{"lineBreak":true},{"text":"Creeping things and flying birds;","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Kings of the earth and all peoples;","poem":1},{"lineBreak":true},{"text":"Princes and all judges of the earth;","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Both young men and virgins;","poem":1},{"lineBreak":true},{"text":"Old men and children:","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Let them praise the name of Jehovah;","poem":1},{"lineBreak":true},{"text":"For his name alone is exalted;","poem":1},{"lineBreak":true},{"text":"His glory is above the earth and the heavens.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"And he hath lifted up the horn of his people,","poem":1},{"lineBreak":true},{"text":"The praise of all his saints;","poem":1},{"lineBreak":true},{"text":"Even of the children of Israel, a people near unto him.","poem":1},{"lineBreak":true},{"text":"Praise ye Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/148.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/149.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/147.json","previousChapterAudioLinks":{},"numberOfVerses":14}