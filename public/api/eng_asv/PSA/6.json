{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":6,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; on stringed instruments, set to the Sheminith. A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"O Jehovah, rebuke me not in thine anger,","poem":1},{"lineBreak":true},{"text":"Neither chasten me in thy hot displeasure.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Have mercy upon me, O Jehovah; for I am withered away:","poem":1},{"lineBreak":true},{"text":"O Jehovah, heal me; for my bones are troubled.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"My soul also is sore troubled:","poem":1},{"lineBreak":true},{"text":"And thou, O Jehovah, how long?","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Return, O Jehovah, deliver my soul:","poem":1},{"lineBreak":true},{"text":"Save me for thy lovingkindnessâ€™ sake.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"For in death there is no remembrance of thee:","poem":1},{"lineBreak":true},{"text":"In Sheol who shall give thee thanks?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I am weary with my groaning;","poem":1},{"lineBreak":true},{"text":"Every night make I my bed to swim;","poem":1},{"lineBreak":true},{"text":"I water my couch with my tears.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Mine eye wasteth away because of grief;","poem":1},{"lineBreak":true},{"text":"It waxeth old because of all mine adversaries.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Depart from me, all ye workers of iniquity;","poem":1},{"lineBreak":true},{"text":"For Jehovah hath heard the voice of my weeping.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Jehovah hath heard my supplication;","poem":1},{"lineBreak":true},{"text":"Jehovah will receive my prayer.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"All mine enemies shall be put to shame and sore troubled:","poem":1},{"lineBreak":true},{"text":"They shall turn back, they shall be put to shame suddenly.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/6.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/7.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/5.json","previousChapterAudioLinks":{},"numberOfVerses":10}