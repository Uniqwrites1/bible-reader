{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":85,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. A Psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Jehovah, thou hast been favorable unto thy land;","poem":1},{"lineBreak":true},{"text":"Thou hast brought back the captivity of Jacob.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Thou hast forgiven the iniquity of thy people;","poem":1},{"lineBreak":true},{"text":"Thou hast covered all their sin. [Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Thou hast taken away all thy wrath;","poem":1},{"lineBreak":true},{"text":"Thou hast turned thyself from the fierceness of thine anger.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Turn us, O God of our salvation,","poem":1},{"lineBreak":true},{"text":"And cause thine indignation toward us to cease.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Wilt thou be angry with us for ever?","poem":1},{"lineBreak":true},{"text":"Wilt thou draw out thine anger to all generations?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Wilt thou not quicken us again,","poem":1},{"lineBreak":true},{"text":"That thy people may rejoice in thee?","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Show us thy lovingkindness, O Jehovah,","poem":1},{"lineBreak":true},{"text":"And grant us thy salvation.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"I will hear what God Jehovah will speak;","poem":1},{"lineBreak":true},{"text":"For he will speak peace unto his people, and to his saints:","poem":1},{"lineBreak":true},{"text":"But let them not turn again to folly.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Surely his salvation is nigh them that fear him,","poem":1},{"lineBreak":true},{"text":"That glory may dwell in our land.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Mercy and truth are met together;","poem":1},{"lineBreak":true},{"text":"Righteousness and peace have kissed each other.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Truth springeth out of the earth;","poem":1},{"lineBreak":true},{"text":"And righteousness hath looked down from heaven.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Yea, Jehovah will give that which is good;","poem":1},{"lineBreak":true},{"text":"And our land shall yield its increase.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Righteousness shall go before him,","poem":1},{"lineBreak":true},{"text":"And shall make his footsteps a way to walk in.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/85.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/86.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/84.json","previousChapterAudioLinks":{},"numberOfVerses":13}