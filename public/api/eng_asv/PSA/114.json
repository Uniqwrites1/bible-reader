{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":114,"content":[{"type":"verse","number":1,"content":[{"text":"When Israel went forth out of Egypt,","poem":1},{"lineBreak":true},{"text":"The house of Jacob from a people of strange language;","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Judah became his sanctuary,","poem":1},{"lineBreak":true},{"text":"Israel his dominion.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The sea saw it, and fled;","poem":1},{"lineBreak":true},{"text":"The Jordan was driven back.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The mountains skipped like rams,","poem":1},{"lineBreak":true},{"text":"The little hills like lambs.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"What aileth thee, O thou sea, that thou fleest?","poem":1},{"lineBreak":true},{"text":"Thou Jordan, that thou turnest back?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Ye mountains, that ye skip like rams;","poem":1},{"lineBreak":true},{"text":"Ye little hills, like lambs?","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Tremble, thou earth, at the presence of the Lord,","poem":1},{"lineBreak":true},{"text":"At the presence of the God of Jacob,","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Who turned the rock into a pool of water,","poem":1},{"lineBreak":true},{"text":"The flint into a fountain of waters.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/114.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/115.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/113.json","previousChapterAudioLinks":{},"numberOfVerses":8}