{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":87,"content":[{"type":"hebrew_subtitle","content":["A Psalm of the sons of Korah; a Song."]},{"type":"verse","number":1,"content":[{"text":"His foundation is in the holy mountains.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Jehovah loveth the gates of Zion","poem":1},{"lineBreak":true},{"text":"More than all the dwellings of Jacob.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Glorious things are spoken of thee,","poem":1},{"lineBreak":true},{"text":"O city of God. [Selah","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I will make mention of Rahab and Babylon as among them that know me:","poem":1},{"lineBreak":true},{"text":"Behold, Philistia, and Tyre, with Ethiopia:","poem":1},{"lineBreak":true},{"text":"This one was born there.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Yea, of Zion it shall be said, This one and that one was born in her;","poem":1},{"lineBreak":true},{"text":"And the Most High himself will establish her.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Jehovah will count, when he writeth up the peoples,","poem":1},{"lineBreak":true},{"text":"This one was born there. [Selah","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"They that sing as well as they that dance shall say,","poem":1},{"lineBreak":true},{"text":"All my fountains are in thee.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/87.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/88.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/86.json","previousChapterAudioLinks":{},"numberOfVerses":7}