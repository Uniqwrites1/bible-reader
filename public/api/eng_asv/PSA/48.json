{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":48,"content":[{"type":"hebrew_subtitle","content":["A Song; a Psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Great is Jehovah, and greatly to be praised,","poem":1},{"lineBreak":true},{"text":"In the city of our God, in his holy mountain.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Beautiful in elevation, the joy of the whole earth,","poem":1},{"lineBreak":true},{"text":"Is mount Zion, on the sides of the north,","poem":1},{"lineBreak":true},{"text":"The city of the great King.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"God hath made himself known in her palaces for a refuge.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For, lo, the kings assembled themselves,","poem":1},{"lineBreak":true},{"text":"They passed by together.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"They saw it, then were they amazed;","poem":1},{"lineBreak":true},{"text":"They were dismayed, they hasted away.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Trembling took hold of them there,","poem":1},{"lineBreak":true},{"text":"Pain, as of a woman in travail.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"With the east wind","poem":1},{"lineBreak":true},{"text":"Thou breakest the ships of Tarshish.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"As we have heard, so have we seen","poem":1},{"lineBreak":true},{"text":"In the city of Jehovah of hosts, in the city of our God:","poem":1},{"lineBreak":true},{"text":"God will establish it for ever. [Selah","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"We have thought on thy lovingkindness, O God,","poem":1},{"lineBreak":true},{"text":"In the midst of thy temple.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"As is thy name, O God,","poem":1},{"lineBreak":true},{"text":"So is thy praise unto the ends of the earth:","poem":1},{"lineBreak":true},{"text":"Thy right hand is full of righteousness.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Let mount Zion be glad,","poem":1},{"lineBreak":true},{"text":"Let the daughters of Judah rejoice,","poem":1},{"lineBreak":true},{"text":"Because of thy judgments.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Walk about Zion, and go round about her;","poem":1},{"lineBreak":true},{"text":"Number the towers thereof;","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Mark ye well her bulwarks;","poem":1},{"lineBreak":true},{"text":"Consider her palaces:","poem":1},{"lineBreak":true},{"text":"That ye may tell it to the generation following.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"For this God is our God for ever and ever:","poem":1},{"lineBreak":true},{"text":"He will be our guide even unto death.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/48.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/49.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/47.json","previousChapterAudioLinks":{},"numberOfVerses":14}