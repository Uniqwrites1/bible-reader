{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":95,"content":[{"type":"verse","number":1,"content":[{"text":"Oh come, let us sing unto Jehovah;","poem":1},{"lineBreak":true},{"text":"Let us make a joyful noise to the rock of our salvation.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let us come before his presence with thanksgiving;","poem":1},{"lineBreak":true},{"text":"Let us make a joyful noise unto him with psalms.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For Jehovah is a great God,","poem":1},{"lineBreak":true},{"text":"And a great King above all gods.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"In his hand are the deep places of the earth;","poem":1},{"lineBreak":true},{"text":"The heights of the mountains are his also.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The sea is his, and he made it;","poem":1},{"lineBreak":true},{"text":"And his hands formed the dry land.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Oh come, let us worship and bow down;","poem":1},{"lineBreak":true},{"text":"Let us kneel before Jehovah our Maker:","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For he is our God,","poem":1},{"lineBreak":true},{"text":"And we are the people of his pasture, and the sheep of his hand.","poem":1},{"lineBreak":true},{"text":"To-day, oh that ye would hear his voice!","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Harden not your heart, as at Meribah,","poem":1},{"lineBreak":true},{"text":"As in the day of Massah in the wilderness;","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"When your fathers tempted me,","poem":1},{"lineBreak":true},{"text":"Proved me, and saw my work.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Forty years long was I grieved with that generation,","poem":1},{"lineBreak":true},{"text":"And said, It is a people that do err in their heart,","poem":1},{"lineBreak":true},{"text":"And they have not known my ways:","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Wherefore I sware in my wrath,","poem":1},{"lineBreak":true},{"text":"That they should not enter into my rest.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/95.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/96.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/94.json","previousChapterAudioLinks":{},"numberOfVerses":11}