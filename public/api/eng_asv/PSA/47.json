{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":47,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. A Psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Oh clap your hands, all ye peoples;","poem":1},{"lineBreak":true},{"text":"Shout unto God with the voice of triumph.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"For Jehovah Most High is terrible;","poem":1},{"lineBreak":true},{"text":"He is a great King over all the earth.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"He subdueth peoples under us,","poem":1},{"lineBreak":true},{"text":"And nations under our feet.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He chooseth our inheritance for us,","poem":1},{"lineBreak":true},{"text":"The glory of Jacob whom he loved. [Selah","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"God is gone up with a shout,","poem":1},{"lineBreak":true},{"text":"Jehovah with the sound of a trumpet.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Sing praises to God, sing praises:","poem":1},{"lineBreak":true},{"text":"Sing praises unto our King, sing praises.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For God is the King of all the earth:","poem":1},{"lineBreak":true},{"text":"Sing ye praises with understanding.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"God reigneth over the nations:","poem":1},{"lineBreak":true},{"text":"God sitteth upon his holy throne.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"The princes of the peoples are gathered together","poem":1},{"lineBreak":true},{"text":"To be the people of the God of Abraham;","poem":1},{"lineBreak":true},{"text":"For the shields of the earth belong unto God:","poem":1},{"lineBreak":true},{"text":"He is greatly exalted.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/47.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/48.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/46.json","previousChapterAudioLinks":{},"numberOfVerses":9}