1},{"lineBreak":true},{"text":"So are the children of youth.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Happy is the man that hath his quiver full of them:","poem":1},{"lineBreak":true},{"text":"They shall not be put to shame,","poem":1},{"lineBreak":true},{"text":"When they speak with their enemies in the gate.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/127.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/128.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/126.json","previousChapterAudioLinks":{},"numberOfVerses":5}