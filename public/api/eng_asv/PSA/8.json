{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":8,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; set to the Gittith. A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"O Jehovah, our Lord,","poem":1},{"lineBreak":true},{"text":"How excellent is thy name in all the earth,","poem":1},{"lineBreak":true},{"text":"Who hast set thy glory upon the heavens!","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Out of the mouth of babes and sucklings hast thou established strength,","poem":1},{"lineBreak":true},{"text":"Because of thine adversaries,","poem":1},{"lineBreak":true},{"text":"That thou mightest still the enemy and the avenger.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"When I consider thy heavens, the work of thy fingers,","poem":1},{"lineBreak":true},{"text":"The moon and the stars, which thou hast ordained;","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"What is man, that thou art mindful of him?","poem":1},{"lineBreak":true},{"text":"And the son of man, that thou visitest him?","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"For thou hast made him but little lower than God,","poem":1},{"lineBreak":true},{"text":"And crownest him with glory and honor.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Thou makest him to have dominion over the works of thy hands;","poem":1},{"lineBreak":true},{"text":"Thou hast put all things under his feet:","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"All sheep and oxen,","poem":1},{"lineBreak":true},{"text":"Yea, and the beasts of the field,","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"The birds of the heavens, and the fish of the sea,","poem":1},{"lineBreak":true},{"text":"Whatsoever passeth through the paths of the seas.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"O Jehovah, our Lord,","poem":1},{"lineBreak":true},{"text":"How excellent is thy name in all the earth!","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/8.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/9.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/7.json","previousChapterAudioLinks":{},"numberOfVerses":9}