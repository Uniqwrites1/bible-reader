{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":1,"content":[{"type":"verse","number":1,"content":[{"text":"Blessed is the man that walketh not in the counsel of the wicked,","poem":1},{"lineBreak":true},{"text":"Nor standeth in the way of sinners,","poem":1},{"lineBreak":true},{"text":"Nor sitteth in the seat of scoffers:","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"But his delight is in the law of Jehovah;","poem":1},{"lineBreak":true},{"text":"And on his law doth he meditate day and night.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"And he shall be like a tree planted by the streams of water,","poem":1},{"lineBreak":true},{"text":"That bringeth forth its fruit in its season,","poem":1},{"lineBreak":true},{"text":"Whose leaf also doth not wither;","poem":1},{"lineBreak":true},{"text":"And whatsoever he doeth shall prosper.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The wicked are not so,","poem":1},{"lineBreak":true},{"text":"But are like the chaff which the wind driveth away.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Therefore the wicked shall not stand in the judgment,","poem":1},{"lineBreak":true},{"text":"Nor sinners in the congregation of the righteous.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For Jehovah knoweth the way of the righteous;","poem":1},{"lineBreak":true},{"text":"But the way of the wicked shall perish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/1.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/2.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/JOB/42.json","previousChapterAudioLinks":{},"numberOfVerses":6}