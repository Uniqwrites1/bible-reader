{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":96,"content":[{"type":"verse","number":1,"content":[{"text":"Oh sing unto Jehovah a new song:","poem":1},{"lineBreak":true},{"text":"Sing unto Jehovah, all the earth.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Sing unto Jehovah, bless his name;","poem":1},{"lineBreak":true},{"text":"Show forth his salvation from day to day.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Declare his glory among the nations,","poem":1},{"lineBreak":true},{"text":"His marvellous works among all the peoples.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For great is Jehovah, and greatly to be praised:","poem":1},{"lineBreak":true},{"text":"He is to be feared above all gods.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"For all the gods of the peoples are idols;","poem":1},{"lineBreak":true},{"text":"But Jehovah made the heavens.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Honor and majesty are before him:","poem":1},{"lineBreak":true},{"text":"Strength and beauty are in his sanctuary.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Ascribe unto Jehovah, ye kindreds of the peoples,","poem":1},{"lineBreak":true},{"text":"Ascribe unto Jehovah glory and strength.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Ascribe unto Jehovah the glory due unto his name:","poem":1},{"lineBreak":true},{"text":"Bring an offering, and come into his courts.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Oh worship Jehovah in holy array:","poem":1},{"lineBreak":true},{"text":"Tremble before him, all the earth.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Say among the nations, Jehovah reigneth:","poem":1},{"lineBreak":true},{"text":"The world also is established that it cannot be moved:","poem":1},{"lineBreak":true},{"text":"He will judge the peoples with equity.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Let the heavens be glad, and let the earth rejoice;","poem":1},{"lineBreak":true},{"text":"Let the sea roar, and the fulness thereof;","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Let the field exult, and all that is therein;","poem":1},{"lineBreak":true},{"text":"Then shall all the trees of the wood sing for joy","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Before Jehovah; for he cometh,","poem":1},{"lineBreak":true},{"text":"For he cometh to judge the earth:","poem":1},{"lineBreak":true},{"text":"He will judge the world with righteousness,","poem":1},{"lineBreak":true},{"text":"And the peoples with his truth.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/96.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/97.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/95.json","previousChapterAudioLinks":{},"numberOfVerses":13}