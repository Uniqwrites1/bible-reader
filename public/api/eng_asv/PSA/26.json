{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":26,"content":[{"type":"hebrew_subtitle","content":["A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Judge me, O Jehovah, for I have walked in mine integrity:","poem":1},{"lineBreak":true},{"text":"I have trusted also in Jehovah without wavering.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Examine me, O Jehovah, and prove me;","poem":1},{"lineBreak":true},{"text":"Try my heart and my mind.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For thy lovingkindness is before mine eyes;","poem":1},{"lineBreak":true},{"text":"And I have walked in thy truth.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I have not sat with men of falsehood;","poem":1},{"lineBreak":true},{"text":"Neither will I go in with dissemblers.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I hate the assembly of evil-doers,","poem":1},{"lineBreak":true},{"text":"And will not sit with the wicked.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will wash my hands in innocency:","poem":1},{"lineBreak":true},{"text":"So will I compass thine altar, O Jehovah;","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"That I may make the voice of thanksgiving to be heard,","poem":1},{"lineBreak":true},{"text":"And tell of all thy wondrous works.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Jehovah, I love the habitation of thy house,","poem":1},{"lineBreak":true},{"text":"And the place where thy glory dwelleth.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Gather not my soul with sinners,","poem":1},{"lineBreak":true},{"text":"Nor my life with men of blood;","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"In whose hands is wickedness,","poem":1},{"lineBreak":true},{"text":"And their right hand is full of bribes.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"But as for me, I will walk in mine integrity:","poem":1},{"lineBreak":true},{"text":"Redeem me, and be merciful unto me.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"My foot standeth in an even place:","poem":1},{"lineBreak":true},{"text":"In the congregations will I bless Jehovah.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/25.json","previousChapterAudioLinks":{},"numberOfVerses":12}