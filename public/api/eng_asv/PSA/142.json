{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":142,"content":[{"type":"hebrew_subtitle","content":["Maschil of David, when he was in the cave; a Prayer."]},{"type":"verse","number":1,"content":[{"text":"I cry with my voice unto Jehovah;","poem":1},{"lineBreak":true},{"text":"With my voice unto Jehovah do I make supplication.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I pour out my complaint before him;","poem":1},{"lineBreak":true},{"text":"I show before him my trouble.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"When my spirit was overwhelmed within me,","poem":1},{"lineBreak":true},{"text":"Thou knewest my path.","poem":1},{"lineBreak":true},{"text":"In the way wherein I walk","poem":1},{"lineBreak":true},{"text":"Have they hidden a snare for me.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Look on my right hand, and see;","poem":1},{"lineBreak":true},{"text":"For there is no man that knoweth me:","poem":1},{"lineBreak":true},{"text":"Refuge hath failed me;","poem":1},{"lineBreak":true},{"text":"No man careth for my soul.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I cried unto thee, O Jehovah;","poem":1},{"lineBreak":true},{"text":"I said, Thou art my refuge,","poem":1},{"lineBreak":true},{"text":"My portion in the land of the living.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Attend unto my cry;","poem":1},{"lineBreak":true},{"text":"For I am brought very low:","poem":1},{"lineBreak":true},{"text":"Deliver me from my persecutors;","poem":1},{"lineBreak":true},{"text":"For they are stronger than I.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Bring my soul out of prison,","poem":1},{"lineBreak":true},{"text":"That I may give thanks unto thy name:","poem":1},{"lineBreak":true},{"text":"The righteous shall compass me about;","poem":1},{"lineBreak":true},{"text":"For thou wilt deal bountifully with me.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/142.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/143.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/141.json","previousChapterAudioLinks":{},"numberOfVerses":7}