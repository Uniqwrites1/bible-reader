{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_asv","name":"Psalms","commonName":"Psalms","title":"THE PSALMS","order":19,"numberOfChapters":150,"sha256":"80f14d6a9ef01b59b2d1f7614ff34b68b5ac4ce1c805f073965dd07ddf9d580f","firstChapterApiLink":"/api/eng_asv/PSA/1.json","lastChapterApiLink":"/api/eng_asv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":112,"content":[{"type":"verse","number":1,"content":[{"text":"Praise ye Jehovah.","poem":1},{"lineBreak":true},{"text":"Blessed is the man that feareth Jehovah,","poem":1},{"lineBreak":true},{"text":"That delighteth greatly in his commandments.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"His seed shall be mighty upon earth:","poem":1},{"lineBreak":true},{"text":"The generation of the upright shall be blessed.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Wealth and riches are in his house;","poem":1},{"lineBreak":true},{"text":"And his righteousness endureth for ever.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Unto the upright there ariseth light in the darkness:","poem":1},{"lineBreak":true},{"text":"He is gracious, and merciful, and righteous.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Well is it with the man that dealeth graciously and lendeth;","poem":1},{"lineBreak":true},{"text":"He shall maintain his cause in judgment.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For he shall never be moved;","poem":1},{"lineBreak":true},{"text":"The righteous shall be had in everlasting remembrance.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He shall not be afraid of evil tidings:","poem":1},{"lineBreak":true},{"text":"His heart is fixed, trusting in Jehovah.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"His heart is established, he shall not be afraid,","poem":1},{"lineBreak":true},{"text":"Until he see his desire upon his adversaries.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He hath dispersed, he hath given to the needy;","poem":1},{"lineBreak":true},{"text":"His righteousness endureth for ever:","poem":1},{"lineBreak":true},{"text":"His horn shall be exalted with honor.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"The wicked shall see it, and be grieved;","poem":1},{"lineBreak":true},{"text":"He shall gnash with his teeth, and melt away:","poem":1},{"lineBreak":true},{"text":"The desire of the wicked shall perish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/PSA/112.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/PSA/113.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/PSA/111.json","previousChapterAudioLinks":{},"numberOfVerses":10}