{"translation":{"id":"eng_asv","name":"American Standard Version (1901)","website":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-asv","licenseNotes":null,"shortName":"ASV","englishName":"American Standard Version (1901)","language":"eng","textDirection":"ltr","sha256":"975f156c32676f334a42b0d3ac7178eac1ed270608244b56aad7cb385c04bb67","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_asv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"JOB","translationId":"eng_asv","name":"Job","commonName":"Job","title":"THE BOOK OF JOB","order":18,"numberOfChapters":42,"sha256":"f4d22f20a5ee2d38e3b04a2cee92d6ed7c52327abdf590264b3c50012391e594","firstChapterApiLink":"/api/eng_asv/JOB/1.json","lastChapterApiLink":"/api/eng_asv/JOB/42.json","totalNumberOfVerses":1070},"chapter":{"number":26,"content":[{"type":"verse","number":1,"content":["Then Job answered and said,"]},{"type":"verse","number":2,"content":[{"text":"How hast thou helped him that is without power!","poem":1},{"lineBreak":true},{"text":"How hast thou saved the arm that hath no strength!","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"How hast thou counselled him that hath no wisdom,","poem":1},{"lineBreak":true},{"text":"And plentifully declared sound knowledge!","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"To whom hast thou uttered words?","poem":1},{"lineBreak":true},{"text":"And whose spirit came forth from thee?","poem":1}]},{"type":"line_break"},{"type":"verse","number":5,"content":[{"text":"They that are deceased tremble","poem":1},{"lineBreak":true},{"text":"Beneath the waters and the inhabitants thereof.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Sheol is naked before God,","poem":1},{"lineBreak":true},{"text":"And Abaddon hath no covering.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He stretcheth out the north over empty space,","poem":1},{"lineBreak":true},{"text":"And hangeth the earth upon nothing.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"He bindeth up the waters in his thick clouds;","poem":1},{"lineBreak":true},{"text":"And the cloud is not rent under them.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He incloseth the face of his throne,","poem":1},{"lineBreak":true},{"text":"And spreadeth his cloud upon it.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"He hath described a boundary upon the face of the waters,","poem":1},{"lineBreak":true},{"text":"Unto the confines of light and darkness.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"The pillars of heaven tremble","poem":1},{"lineBreak":true},{"text":"And are astonished at his rebuke.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"He stirreth up the sea with his power,","poem":1},{"lineBreak":true},{"text":"And by his understanding he smiteth through Rahab.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"By his Spirit the heavens are garnished;","poem":1},{"lineBreak":true},{"text":"His hand hath pierced the swift serpent.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"Lo, these are but the outskirts of his ways:","poem":1},{"lineBreak":true},{"text":"And how small a whisper do we hear of him!","poem":1},{"lineBreak":true},{"text":"But the thunder of his power who can understand?","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_asv/JOB/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_asv/JOB/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_asv/JOB/25.json","previousChapterAudioLinks":{},"numberOfVerses":14}