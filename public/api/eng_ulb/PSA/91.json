{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":91,"content":[{"type":"verse","number":1,"content":[{"text":"He who lives in the shelter of the Most High","poem":1},{"lineBreak":true},{"text":"will stay in the shadow of the Almighty.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I will say of Yahweh, “He is my refuge and my fortress,","poem":1},{"lineBreak":true},{"text":"my God, in whom I trust.”","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For he will rescue you from the snare of the hunter","poem":1},{"lineBreak":true},{"text":"and from the deadly plague.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He will cover you with his wings,","poem":1},{"lineBreak":true},{"text":"and under his wings you will find refuge.","poem":1},{"lineBreak":true},{"text":"His trustworthiness is a shield and protection.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"You will not be afraid of terror in the night,","poem":1},{"lineBreak":true},{"text":"or of the arrow that flies by day,","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"or of the plague that roams around in the darkness,","poem":1},{"lineBreak":true},{"text":"or of the disease that comes at noontime.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"A thousand may fall at your side","poem":1},{"lineBreak":true},{"text":"and ten thousand at your right hand,","poem":1},{"lineBreak":true},{"text":"but it will not reach you.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"You will only observe","poem":1},{"lineBreak":true},{"text":"and see the punishment of the wicked.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"For Yahweh is my refuge!","poem":1},{"lineBreak":true},{"text":"Make the Most High your refuge also.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"No evil will overtake you;","poem":1},{"lineBreak":true},{"text":"no affliction will come near your home.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"For he will direct his angels to protect you,","poem":1},{"lineBreak":true},{"text":"to guard you in all your ways.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"They will lift you up with their hand","poem":1},{"lineBreak":true},{"text":"so that you will not hit your foot on a stone.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"You will crush lions and adders under your feet;","poem":1},{"lineBreak":true},{"text":"you will trample on young lions and serpents.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"Because he is devoted to me, I will rescue him.","poem":1},{"lineBreak":true},{"text":"I will protect him because he is loyal to me.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"When he calls to me, I will answer him.","poem":1},{"lineBreak":true},{"text":"I will be with him in trouble;","poem":1},{"lineBreak":true},{"text":"I will give him victory and will honor him.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"I will satisfy him with long life","poem":1},{"lineBreak":true},{"text":"and show him my salvation.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/91.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/92.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/90.json","previousChapterAudioLinks":{},"numberOfVerses":16}