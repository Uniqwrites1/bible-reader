{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":141,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Yahweh, I am crying out to you; come quickly to me.","poem":1},{"lineBreak":true},{"text":"Listen to me when I call to you.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"May my prayer be like incense before you;","poem":1},{"lineBreak":true},{"text":"may my lifted hands be like the evening sacrifice.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Yahweh, place a guard over my mouth;","poem":1},{"lineBreak":true},{"text":"guard the door of my lips.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Do not let my heart desire any evil thing","poem":1},{"lineBreak":true},{"text":"or participate in sinful activities","poem":1},{"lineBreak":true},{"text":"with men who behave wickedly.","poem":1},{"lineBreak":true},{"text":"May I not eat any of their delicacies.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let a righteous man hit me; it will be a kindness to me.","poem":1},{"lineBreak":true},{"text":"Let him correct me; it will be like oil on my head;","poem":1},{"lineBreak":true},{"text":"may my head not refuse to accept it.","poem":1},{"lineBreak":true},{"text":"But my prayer is always against their wicked deeds.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Their leaders will be thrown down from the top of cliffs;","poem":1},{"lineBreak":true},{"text":"they will hear that my own words are pleasant.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"They will have to say, “As when one plows and breaks up the ground,","poem":1},{"lineBreak":true},{"text":"so our bones have been scattered at the mouth of Sheol.”","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Surely my eyes are on you, Yahweh, Lord;","poem":1},{"lineBreak":true},{"text":"in you I take refuge; do not leave my soul defenseless.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Protect me from the snares that they have laid for me,","poem":1},{"lineBreak":true},{"text":"from the traps of evildoers.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Let the wicked fall into their own nets","poem":1},{"lineBreak":true},{"text":"while I escape.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/141.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/142.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/140.json","previousChapterAudioLinks":{},"numberOfVerses":10}