{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":12,"content":[{"type":"hebrew_subtitle","content":["For the chief musician; set to the Sheminith. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Help, Yahweh, for the godly have disappeared;","poem":1},{"lineBreak":true},{"text":"the faithful have vanished.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Everyone says empty words to his neighbor;","poem":1},{"lineBreak":true},{"text":"everyone speaks with flattering lips and a double heart.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Yahweh, cut off all flattering lips,","poem":1},{"lineBreak":true},{"text":"every tongue declaring great things.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"These are those who have said, “With our tongues will we prevail.","poem":1},{"lineBreak":true},{"text":"When our lips speak, who can be master over us?”","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"“Because of violence against the poor, because of the groans of the needy,","poem":1},{"lineBreak":true},{"text":"I will arise,” says Yahweh.","poem":1},{"lineBreak":true},{"text":"“I will provide the safety for which they long.”","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"The words of Yahweh are pure words,","poem":1},{"lineBreak":true},{"text":"like silver purified in a furnace on the earth,","poem":1},{"lineBreak":true},{"text":"refined seven times.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"You are Yahweh! You keep them.","poem":1},{"lineBreak":true},{"text":"You preserve the godly people from this wicked generation and forever.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"The wicked walk on every side","poem":1},{"lineBreak":true},{"text":"when evil is exalted among the children of mankind.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/12.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/13.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/11.json","previousChapterAudioLinks":{},"numberOfVerses":8}