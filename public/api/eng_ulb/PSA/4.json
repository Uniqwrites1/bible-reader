{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":4,"content":[{"type":"hebrew_subtitle","content":["For the chief musician; on stringed instruments. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Answer me when I call, God of my righteousness;","poem":1},{"lineBreak":true},{"text":"give me room when I am hemmed in.","poem":1},{"lineBreak":true},{"text":"Have mercy on me and listen to my prayer.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"You people, how long will you turn my honor into shame?","poem":1},{"lineBreak":true},{"text":"How long will you love that which is worthless and seek after lies? Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"But know that Yahweh sets apart the godly for himself.","poem":1},{"lineBreak":true},{"text":"Yahweh will hear when I call to him.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Tremble in fear, but do not sin!","poem":1},{"lineBreak":true},{"text":"Meditate in your heart on your bed and be silent. Selah","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Offer the sacrifices of righteousness","poem":1},{"lineBreak":true},{"text":"and put your trust in Yahweh.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Many say, “Who will show us anything good?”","poem":1},{"lineBreak":true},{"text":"Yahweh, lift up the light of your face on us.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"You have given my heart more gladness","poem":1},{"lineBreak":true},{"text":"than others have when their grain and new wine abound.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"It is in peace that I will lie down and sleep,","poem":1},{"lineBreak":true},{"text":"for you alone, Yahweh, make me safe and secure.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/4.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/5.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/3.json","previousChapterAudioLinks":{},"numberOfVerses":8}