{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":115,"content":[{"type":"verse","number":1,"content":[{"text":"Not to us, Yahweh, not to us,","poem":1},{"lineBreak":true},{"text":"but to your name bring honor,","poem":1},{"lineBreak":true},{"text":"for your covenant faithfulness and for your trustworthiness.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Why should the nations say,","poem":1},{"lineBreak":true},{"text":"“Where is their God?”","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Our God is in heaven;","poem":1},{"lineBreak":true},{"text":"he does whatever he pleases.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The nations' idols are silver and gold,","poem":1},{"lineBreak":true},{"text":"the work of men's hands.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Those idols have mouths, but they do not speak;","poem":1},{"lineBreak":true},{"text":"they have eyes, but they do not see;","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"they have ears, but they do not hear;","poem":1},{"lineBreak":true},{"text":"they have noses, but they do not smell.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Those idols have hands, but do not feel;","poem":1},{"lineBreak":true},{"text":"they have feet, but they cannot walk;","poem":1},{"lineBreak":true},{"text":"nor do they speak from their mouths.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Those who make them are like them,","poem":1},{"lineBreak":true},{"text":"as is everyone who trusts in them.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Israel, trust in Yahweh;","poem":1},{"lineBreak":true},{"text":"he is your help and shield.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"House of Aaron, trust in Yahweh;","poem":1},{"lineBreak":true},{"text":"he is your help and shield.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"You who honor Yahweh, trust in him;","poem":1},{"lineBreak":true},{"text":"he is your help and shield.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Yahweh takes notice of us and will bless us;","poem":1},{"lineBreak":true},{"text":"he will bless the family of Israel;","poem":1},{"lineBreak":true},{"text":"he will bless the family of Aaron.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"He will bless those who honor him,","poem":1},{"lineBreak":true},{"text":"both young and old.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"May Yahweh increase your numbers more and more,","poem":1},{"lineBreak":true},{"text":"yours and your descendants'.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"May you be blessed by Yahweh,","poem":1},{"lineBreak":true},{"text":"who made heaven and earth.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"The heavens belong to Yahweh;","poem":1},{"lineBreak":true},{"text":"but the earth he has given to mankind.","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"The dead do not praise Yahweh,","poem":1},{"lineBreak":true},{"text":"nor do any who go down into silence;","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"But we will bless Yahweh","poem":1},{"lineBreak":true},{"text":"now and forevermore.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/115.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/116.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/114.json","previousChapterAudioLinks":{},"numberOfVerses":18}