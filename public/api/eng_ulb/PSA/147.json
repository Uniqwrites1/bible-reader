{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":147,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh,","poem":1},{"lineBreak":true},{"text":"for it is good to sing praises to our God,","poem":1},{"lineBreak":true},{"text":"it is pleasant, and praise is suitable.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Yahweh rebuilds Jerusalem,","poem":1},{"lineBreak":true},{"text":"he gathers together the scattered people of Israel.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"He heals the brokenhearted","poem":1},{"lineBreak":true},{"text":"and binds up their wounds.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He counts the stars,","poem":1},{"lineBreak":true},{"text":"he gives names to all of them.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Great is our Lord and awesome in power,","poem":1},{"lineBreak":true},{"text":"his understanding cannot be measured.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Yahweh lifts up the oppressed,","poem":1},{"lineBreak":true},{"text":"he brings the wicked down to the ground.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Sing to Yahweh with thanksgiving,","poem":1},{"lineBreak":true},{"text":"sing praises to our God with a harp.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"He covers the heavens with clouds","poem":1},{"lineBreak":true},{"text":"and prepares rain for the earth,","poem":1},{"lineBreak":true},{"text":"making the grass to grow on the mountains.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He gives food to the animals","poem":1},{"lineBreak":true},{"text":"and to the young ravens when they cry.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"He finds no delight in the strength of a horse,","poem":1},{"lineBreak":true},{"text":"he takes no pleasure in the strong legs of a man.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Yahweh takes pleasure in those who honor him,","poem":1},{"lineBreak":true},{"text":"who hope in his covenant faithfulness.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Praise Yahweh, Jerusalem,","poem":1},{"lineBreak":true},{"text":"praise your God, Zion.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"For he strengthens the bars of your gates,","poem":1},{"lineBreak":true},{"text":"he blesses your children among you.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"He brings prosperity inside your borders,","poem":1},{"lineBreak":true},{"text":"he satisfies you with the finest of wheat.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"He sends out his commandment to earth,","poem":1},{"lineBreak":true},{"text":"his command runs very swiftly.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"He makes the snow like wool,","poem":1},{"lineBreak":true},{"text":"he scatters the frost like ashes.","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"He dispenses the hail like crumbs,","poem":1},{"lineBreak":true},{"text":"who can withstand the cold he sends?","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"He sends out his command and melts them,","poem":1},{"lineBreak":true},{"text":"he makes the wind to blow and the water to flow.","poem":1}]},{"type":"verse","number":19,"content":[{"lineBreak":true},{"text":"He proclaimed his word to Jacob,","poem":1},{"lineBreak":true},{"text":"his statutes and his righteous decrees to Israel.","poem":1}]},{"type":"verse","number":20,"content":[{"lineBreak":true},{"text":"He has not done so with any other nation,","poem":1},{"lineBreak":true},{"text":"and as for his decrees, they do not know them.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/147.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/148.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/146.json","previousChapterAudioLinks":{},"numberOfVerses":20}