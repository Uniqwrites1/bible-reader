{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":146,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh, my soul.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I give praise to Yahweh with all my life;","poem":1},{"lineBreak":true},{"text":"I will sing praises to my God as long as I exist.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Do not put your trust in princes","poem":1},{"lineBreak":true},{"text":"or in mankind, in whom there is no salvation.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"When a person's life's breath stops, he returns to the ground;","poem":1},{"lineBreak":true},{"text":"on that day his plans end.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Blessed is he who has the God of Jacob for his help,","poem":1},{"lineBreak":true},{"text":"whose hope is in Yahweh his God.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Yahweh made heaven and earth,","poem":1},{"lineBreak":true},{"text":"the sea, and all that is in them;","poem":1},{"lineBreak":true},{"text":"he observes trustworthiness forever.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He executes justice for the oppressed","poem":1},{"lineBreak":true},{"text":"and gives food to the hungry.","poem":1},{"lineBreak":true},{"text":"Yahweh frees the prisoners;","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Yahweh opens the eyes of the blind;","poem":1},{"lineBreak":true},{"text":"Yahweh raises up those who are bowed down;","poem":1},{"lineBreak":true},{"text":"Yahweh loves the righteous people.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Yahweh protects the foreigners in the land;","poem":1},{"lineBreak":true},{"text":"he lifts up the fatherless and widow,","poem":1},{"lineBreak":true},{"text":"but he opposes the wicked.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Yahweh will reign forever,","poem":1},{"lineBreak":true},{"text":"your God, Zion, for all generations.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/146.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/147.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/145.json","previousChapterAudioLinks":{},"numberOfVerses":10}