{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":149,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"Sing to Yahweh a new song;","poem":1},{"lineBreak":true},{"text":"sing his praise in the assembly of the faithful ones.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let Israel rejoice in the one who made them;","poem":1},{"lineBreak":true},{"text":"let the people of Zion rejoice in their king.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Let them praise his name with dancing;","poem":1},{"lineBreak":true},{"text":"let them sing praises to him with tambourine and harp.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For Yahweh takes pleasure in his people;","poem":1},{"lineBreak":true},{"text":"he glorifies the humble with salvation.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let the godly rejoice in victory;","poem":1},{"lineBreak":true},{"text":"let them sing for joy on their beds.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"May the praises of God be in their mouths","poem":1},{"lineBreak":true},{"text":"and a two-edged sword in their hand","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"to execute vengeance on the nations","poem":1},{"lineBreak":true},{"text":"and acts of punishment on the peoples.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They will bind their kings with chains","poem":1},{"lineBreak":true},{"text":"and their nobles with iron shackles.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"They will execute the judgment that is written.","poem":1},{"lineBreak":true},{"text":"This will be an honor for all his faithful ones.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}