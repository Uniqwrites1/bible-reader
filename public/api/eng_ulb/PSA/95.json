{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":95,"content":[{"type":"verse","number":1,"content":[{"text":"Oh come, let us sing to Yahweh;","poem":1},{"lineBreak":true},{"text":"let us sing joyfully to the rock of our salvation.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let us enter his presence with thanksgiving;","poem":1},{"lineBreak":true},{"text":"let us sing to him with psalms of praise.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For Yahweh is a great God","poem":1},{"lineBreak":true},{"text":"and a great King superior to all gods.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"In his hand are the depths of the earth;","poem":1},{"lineBreak":true},{"text":"the heights of the mountains are his.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The sea is his, for he made it,","poem":1},{"lineBreak":true},{"text":"and his hands formed the dry land.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Oh come, let us worship and bow down;","poem":1},{"lineBreak":true},{"text":"let us kneel before Yahweh, our Creator:","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For he is our God,","poem":1},{"lineBreak":true},{"text":"and we are the people of his pasture and the sheep of his hand.","poem":1},{"lineBreak":true},{"text":"Today—oh, that you would hear his voice!","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"“Do not harden your heart, as at Meribah,","poem":1},{"lineBreak":true},{"text":"or as on the day of Massah in the wilderness,","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"where your forefathers tested me","poem":1},{"lineBreak":true},{"text":"and tried me, though they had seen my deeds.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"For forty years I was angry with that generation","poem":1},{"lineBreak":true},{"text":"and said, 'This is a people whose hearts wander astray;","poem":1},{"lineBreak":true},{"text":"they have not known my ways.'","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Therefore I vowed in my anger","poem":1},{"lineBreak":true},{"text":"that they would never enter into my resting place.”","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/95.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/96.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/94.json","previousChapterAudioLinks":{},"numberOfVerses":11}