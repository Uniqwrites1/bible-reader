{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":143,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Hear my prayer, Yahweh; listen to my pleas.","poem":1},{"lineBreak":true},{"text":"Because of your faithfulness and righteousness, answer me!","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Do not enter into judgment with your servant,","poem":1},{"lineBreak":true},{"text":"for in your sight no one is righteous.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The enemy has pursued my soul;","poem":1},{"lineBreak":true},{"text":"he has crushed me to the ground;","poem":1},{"lineBreak":true},{"text":"he has made me to live in darkness","poem":1},{"lineBreak":true},{"text":"like those who have been dead a long time.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"My spirit is overwhelmed within me;","poem":1},{"lineBreak":true},{"text":"my heart despairs.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I call to mind the old days;","poem":1},{"lineBreak":true},{"text":"I meditate on all your deeds;","poem":1},{"lineBreak":true},{"text":"I reflect on your accomplishments.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I spread my hands out to you;","poem":1},{"lineBreak":true},{"text":"my soul thirsts for you in a parched land. Selah","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Answer me quickly, Yahweh, because my spirit faints.","poem":1},{"lineBreak":true},{"text":"Do not hide your face from me,","poem":1},{"lineBreak":true},{"text":"or I will become like those who go down into the pit.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Let me hear your covenant faithfulness in the morning,","poem":1},{"lineBreak":true},{"text":"for I trust in you.","poem":1},{"lineBreak":true},{"text":"Show me the way where I should walk,","poem":1},{"lineBreak":true},{"text":"for I lift up my soul to you.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Rescue me from my enemies, Yahweh;","poem":1},{"lineBreak":true},{"text":"I flee to you to hide.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Teach me to do your will,","poem":1},{"lineBreak":true},{"text":"for you are my God.","poem":1},{"lineBreak":true},{"text":"May your good Spirit","poem":1},{"lineBreak":true},{"text":"lead me in the land of uprightness.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Yahweh, for your name's sake, keep me alive;","poem":1},{"lineBreak":true},{"text":"in your righteousness bring my soul out of trouble.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"In your covenant faithfulness cut off my enemies","poem":1},{"lineBreak":true},{"text":"and destroy all the enemies of my life,","poem":1},{"lineBreak":true},{"text":"for I am your servant.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/143.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/144.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/142.json","previousChapterAudioLinks":{},"numberOfVerses":12}