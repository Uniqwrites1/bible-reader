{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":97,"content":[{"type":"verse","number":1,"content":[{"text":"Yahweh reigns; let the earth rejoice;","poem":1},{"lineBreak":true},{"text":"let the many coastlands be glad.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Clouds and darkness surround him.","poem":1},{"lineBreak":true},{"text":"Righteousness and justice are the foundation of his throne.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Fire goes before him","poem":1},{"lineBreak":true},{"text":"and consumes his adversaries on every side.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"His lightning lights up the world;","poem":1},{"lineBreak":true},{"text":"the earth sees and trembles.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The mountains melt like wax before Yahweh,","poem":1},{"lineBreak":true},{"text":"the Lord of the whole earth.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"The skies declare his justice,","poem":1},{"lineBreak":true},{"text":"and all the nations see his glory.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"All those who worship carved figures will be shamed,","poem":1},{"lineBreak":true},{"text":"those who boast in worthless idolsâ€”","poem":1},{"lineBreak":true},{"text":"bow down to him, all you gods!","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Zion heard and was glad,","poem":1},{"lineBreak":true},{"text":"and the towns of Judah rejoiced","poem":1},{"lineBreak":true},{"text":"because of your righteous decrees, Yahweh.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"For you, Yahweh, are most high above all the earth.","poem":1},{"lineBreak":true},{"text":"You are exalted far above all gods.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"You who love Yahweh, hate evil!","poem":1},{"lineBreak":true},{"text":"He protects the lives of his saints,","poem":1},{"lineBreak":true},{"text":"and he takes them out of the hand of the wicked.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Light is sown for the righteous","poem":1},{"lineBreak":true},{"text":"and gladness for those with honest hearts.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Be glad in Yahweh, you righteous;","poem":1},{"lineBreak":true},{"text":"and give thanks when you remember his holiness.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/97.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/98.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/96.json","previousChapterAudioLinks":{},"numberOfVerses":12}