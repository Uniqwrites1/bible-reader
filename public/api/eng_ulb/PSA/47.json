{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":47,"content":[{"type":"hebrew_subtitle","content":["For the chief musician. A psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Clap your hands, all you peoples;","poem":1},{"lineBreak":true},{"text":"shout to God with the sound of triumph.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"For Yahweh Most High is terrifying;","poem":1},{"lineBreak":true},{"text":"he is a great King over all the earth.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"He subdues peoples under us","poem":1},{"lineBreak":true},{"text":"and nations under our feet.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He chooses our inheritance for us,","poem":1},{"lineBreak":true},{"text":"the glory of Jacob whom he loved. Selah","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"God has gone up with a shout,","poem":1},{"lineBreak":true},{"text":"Yahweh with the sound of a trumpet.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Sing praises to God, sing praises;","poem":1},{"lineBreak":true},{"text":"sing praises to our King, sing praises.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For God is the King over all the earth;","poem":1},{"lineBreak":true},{"text":"sing praises with understanding.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"God reigns over the nations;","poem":1},{"lineBreak":true},{"text":"God sits on his holy throne.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"The princes of the peoples have gathered together","poem":1},{"lineBreak":true},{"text":"to the people of the God of Abraham;","poem":1},{"lineBreak":true},{"text":"for the shields of the earth belong to God;","poem":1},{"lineBreak":true},{"text":"he is greatly exalted.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/47.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/48.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/46.json","previousChapterAudioLinks":{},"numberOfVerses":9}