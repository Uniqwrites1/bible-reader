{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":148,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh, you in the heavens;","poem":1},{"lineBreak":true},{"text":"praise him, you in the heights.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Praise him, all his angels;","poem":1},{"lineBreak":true},{"text":"praise him, all his hosts.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Praise him, sun and moon;","poem":1},{"lineBreak":true},{"text":"praise him, all you shining stars.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Praise him, highest heaven","poem":1},{"lineBreak":true},{"text":"and you waters that are above the sky.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Let them praise the name of Yahweh,","poem":1},{"lineBreak":true},{"text":"for he gave the command, and they were created.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He has also established them forever and ever;","poem":1},{"lineBreak":true},{"text":"he issued a decree that will never change.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Praise him from the earth,","poem":1},{"lineBreak":true},{"text":"you sea monsters and all ocean depths,","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"fire and hail, snow and clouds,","poem":1},{"lineBreak":true},{"text":"stormy wind fulfilling his word.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Praise him, mountains and all hills,","poem":1},{"lineBreak":true},{"text":"fruit trees and all cedars,","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"animals wild and tame,","poem":1},{"lineBreak":true},{"text":"creatures that crawl and birds.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Praise Yahweh, kings of the earth and all nations,","poem":1},{"lineBreak":true},{"text":"princes and all who govern on the earth,","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"both young men and young women,","poem":1},{"lineBreak":true},{"text":"elderly and children.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Let them all praise the name of Yahweh,","poem":1},{"lineBreak":true},{"text":"for his name alone is exalted","poem":1},{"lineBreak":true},{"text":"and his glory extends over the earth and the heavens.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"He has lifted up the horn of his people","poem":1},{"lineBreak":true},{"text":"for praise from all his faithful ones,","poem":1},{"lineBreak":true},{"text":"the Israelites, the people near to him.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/148.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/149.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/147.json","previousChapterAudioLinks":{},"numberOfVerses":14}