{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":6,"content":[{"type":"hebrew_subtitle","content":["For the chief musician; on stringed instruments, set to the Sheminith style. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Yahweh, do not rebuke me in your anger","poem":1},{"lineBreak":true},{"text":"or discipline me in your wrath.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Have mercy on me, Yahweh, for I am frail;","poem":1},{"lineBreak":true},{"text":"heal me, Yahweh, for my bones are shaking.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"My soul also is very troubled.","poem":1},{"lineBreak":true},{"text":"But you, Yahwehâ€”how long will this continue?","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Return, Yahweh! rescue me.","poem":1},{"lineBreak":true},{"text":"Save me because of your covenant faithfulness!","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"For in death there is no remembrance of you.","poem":1},{"lineBreak":true},{"text":"In Sheol who will give you thanks?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I am weary with my groaning.","poem":1},{"lineBreak":true},{"text":"All night I drench my bed with tears;","poem":1},{"lineBreak":true},{"text":"I wash my couch away with my tears.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"My eyes grow dim from grief;","poem":1},{"lineBreak":true},{"text":"they grow weak because of all my adversaries.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Get away from me, all you who practice iniquity;","poem":1},{"lineBreak":true},{"text":"for Yahweh has heard the sound of my weeping.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Yahweh has heard my appeal for mercy;","poem":1},{"lineBreak":true},{"text":"Yahweh has accepted my prayer.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"All my enemies will be ashamed and greatly troubled.","poem":1},{"lineBreak":true},{"text":"They will turn back and be suddenly humiliated.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/6.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/7.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/5.json","previousChapterAudioLinks":{},"numberOfVerses":10}