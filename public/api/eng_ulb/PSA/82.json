{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":82,"content":[{"type":"hebrew_subtitle","content":["A psalm of Asaph."]},{"type":"verse","number":1,"content":[{"text":"God stands in the divine assembly;","poem":1},{"lineBreak":true},{"text":"in the midst of the gods he renders judgment.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"How long will you judge unjustly","poem":1},{"lineBreak":true},{"text":"and show favoritism to the wicked? Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Defend the poor and fatherless;","poem":1},{"lineBreak":true},{"text":"maintain the rights of the afflicted and destitute.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Rescue the poor and needy;","poem":1},{"lineBreak":true},{"text":"take them out of the hand of the wicked.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"They neither know nor understand;","poem":1},{"lineBreak":true},{"text":"they wander around in the darkness;","poem":1},{"lineBreak":true},{"text":"all the foundations of the earth crumble.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I said, “You are gods,","poem":1},{"lineBreak":true},{"text":"and all of you sons of the Most High.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Nevertheless you will die like men","poem":1},{"lineBreak":true},{"text":"and fall like one of the princes.”","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Arise, God, judge the earth,","poem":1},{"lineBreak":true},{"text":"for you have an inheritance in all the nations.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/82.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/83.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/81.json","previousChapterAudioLinks":{},"numberOfVerses":8}