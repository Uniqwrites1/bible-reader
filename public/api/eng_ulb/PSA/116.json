{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":116,"content":[{"type":"verse","number":1,"content":[{"text":"I love Yahweh because he hears","poem":1},{"lineBreak":true},{"text":"my voice and my pleas for mercy.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Because he listened to me,","poem":1},{"lineBreak":true},{"text":"I will call on him as long as I live.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The cords of death surrounded me,","poem":1},{"lineBreak":true},{"text":"and the snares of Sheol confronted me;","poem":1},{"lineBreak":true},{"text":"I felt anguish and sorrow.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Then I called on the name of Yahweh:","poem":1},{"lineBreak":true},{"text":"“Please Yahweh, rescue my life.”","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Yahweh is merciful and fair;","poem":1},{"lineBreak":true},{"text":"our God is compassionate.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Yahweh protects the naive;","poem":1},{"lineBreak":true},{"text":"I was brought low, and he saved me.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"My soul can return to its resting place,","poem":1},{"lineBreak":true},{"text":"for Yahweh has been good to me.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"For you rescued my life from death,","poem":1},{"lineBreak":true},{"text":"my eyes from tears,","poem":1},{"lineBreak":true},{"text":"and my feet from stumbling.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"I will serve Yahweh","poem":1},{"lineBreak":true},{"text":"in the land of the living.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"I believed in him, even when I said,","poem":1},{"lineBreak":true},{"text":"“I am greatly afflicted.”","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"In my confusion I said,","poem":1},{"lineBreak":true},{"text":"“All men are liars.”","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"How can I repay Yahweh","poem":1},{"lineBreak":true},{"text":"for all his kindnesses to me?","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"I will raise the cup of salvation,","poem":1},{"lineBreak":true},{"text":"and call on the name of Yahweh.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"I will fulfill my vows to Yahweh","poem":1},{"lineBreak":true},{"text":"in the presence of all his people.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"Precious in the sight of Yahweh","poem":1},{"lineBreak":true},{"text":"is the death of his saints.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"Yahweh, indeed, I am your servant;","poem":1},{"lineBreak":true},{"text":"I am your servant, the son of your servant woman;","poem":1},{"lineBreak":true},{"text":"you have taken away my bonds.","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"I will offer to you the sacrifice of thanksgiving","poem":1},{"lineBreak":true},{"text":"and will call on the name of Yahweh.","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"I will fulfill my vows to Yahweh","poem":1},{"lineBreak":true},{"text":"in the presence of all his people,","poem":1}]},{"type":"verse","number":19,"content":[{"lineBreak":true},{"text":"in the courts of Yahweh's house,","poem":1},{"lineBreak":true},{"text":"in your midst, Jerusalem.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/116.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/117.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/115.json","previousChapterAudioLinks":{},"numberOfVerses":19}