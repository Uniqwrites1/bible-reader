{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":142,"content":[{"type":"hebrew_subtitle","content":["A maschil of David, when he was in the cave; a prayer."]},{"type":"verse","number":1,"content":[{"text":"With my voice I cry out for help to Yahweh;","poem":1},{"lineBreak":true},{"text":"with my voice I plead for Yahweh's favor.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I pour out my lament before him;","poem":1},{"lineBreak":true},{"text":"I tell him my troubles.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"When my spirit is weak within me,","poem":1},{"lineBreak":true},{"text":"you know my path.","poem":1},{"lineBreak":true},{"text":"In the way that I walk","poem":1},{"lineBreak":true},{"text":"they have hidden a trap for me.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I look to my right and and see","poem":1},{"lineBreak":true},{"text":"that there is no one who cares about me.","poem":1},{"lineBreak":true},{"text":"There is no escape for me;","poem":1},{"lineBreak":true},{"text":"no one cares about my life.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I called out to you, Yahweh;","poem":1},{"lineBreak":true},{"text":"I said, “You are my refuge,","poem":1},{"lineBreak":true},{"text":"my portion in the land of the living.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Listen to my call,","poem":1},{"lineBreak":true},{"text":"for I have been brought very low;","poem":1},{"lineBreak":true},{"text":"rescue me from my persecutors,","poem":1},{"lineBreak":true},{"text":"for they are stronger than I.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Bring my soul out of prison","poem":1},{"lineBreak":true},{"text":"so that I may give thanks to your name.","poem":1},{"lineBreak":true},{"text":"The righteous will gather around me","poem":1},{"lineBreak":true},{"text":"because you have been good to me.”","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/142.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/143.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/141.json","previousChapterAudioLinks":{},"numberOfVerses":7}