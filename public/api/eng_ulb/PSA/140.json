{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":140,"content":[{"type":"hebrew_subtitle","content":["For the chief musician. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Yahweh, rescue me from the wicked;","poem":1},{"lineBreak":true},{"text":"preserve me from violent men.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"They plan evil in their hearts;","poem":1},{"lineBreak":true},{"text":"they cause battles every day.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Their tongues wound like serpents;","poem":1},{"lineBreak":true},{"text":"vipers' poison is on their lips. Selah","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Keep me from the hands of the wicked, Yahweh;","poem":1},{"lineBreak":true},{"text":"preserve me from violent men","poem":1},{"lineBreak":true},{"text":"who plan to knock me over.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The proud have set a trap for me;","poem":1},{"lineBreak":true},{"text":"they have spread a net;","poem":1},{"lineBreak":true},{"text":"they have set a snare for me. Selah","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I said to Yahweh, “You are my God;","poem":1},{"lineBreak":true},{"text":"listen to my cries for mercy.”","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Yahweh, my Lord, you are powerfully able to save me;","poem":1},{"lineBreak":true},{"text":"you shield my head in the day of battle.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Yahweh, do not grant the desires of the wicked;","poem":1},{"lineBreak":true},{"text":"do not let their plots succeed. Selah","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Those who surround me raise their heads;","poem":1},{"lineBreak":true},{"text":"let the mischief of their own lips cover them.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Let burning coals fall on them;","poem":1},{"lineBreak":true},{"text":"throw them into the fire,","poem":1},{"lineBreak":true},{"text":"into bottomless pits, never more to rise.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"May men of tongues not be made secure on the earth;","poem":1},{"lineBreak":true},{"text":"may evil hunt down the violent man to strike him dead.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"I know that Yahweh will judge in favor of the afflicted,","poem":1},{"lineBreak":true},{"text":"and that he will give justice to the needy.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Surely the righteous people will give thanks to your name;","poem":1},{"lineBreak":true},{"text":"the upright people will live in your presence.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/140.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/141.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/139.json","previousChapterAudioLinks":{},"numberOfVerses":13}