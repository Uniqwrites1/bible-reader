{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":112,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"Blessed is the man who obeys Yahweh,","poem":1},{"lineBreak":true},{"text":"who greatly delights in his commandments.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"His descendants will be powerful on earth;","poem":1},{"lineBreak":true},{"text":"the descendants of the godly man will be blessed.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Wealth and riches are in his house;","poem":1},{"lineBreak":true},{"text":"his righteousness will endure forever.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Light shines in the darkness for the godly person;","poem":1},{"lineBreak":true},{"text":"he is gracious, merciful, and just.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"It goes well for the man who deals graciously and lends money,","poem":1},{"lineBreak":true},{"text":"who conducts his affairs with honesty.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For he will never be moved;","poem":1},{"lineBreak":true},{"text":"the righteous person will be remembered forever.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He does not fear bad news;","poem":1},{"lineBreak":true},{"text":"he is confident, trusting in Yahweh.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"His heart is tranquil, without fear,","poem":1},{"lineBreak":true},{"text":"until he looks in triumph over his adversaries.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He generously gives to the poor;","poem":1},{"lineBreak":true},{"text":"his righteousness endures forever;","poem":1},{"lineBreak":true},{"text":"he will be exalted with honor.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"The wicked person will see this and be angry;","poem":1},{"lineBreak":true},{"text":"he will grind his teeth in rage and he will melt away;","poem":1},{"lineBreak":true},{"text":"the desire of the wicked people will perish.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/112.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/113.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/111.json","previousChapterAudioLinks":{},"numberOfVerses":10}