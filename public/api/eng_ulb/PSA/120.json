{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":120,"content":[{"type":"hebrew_subtitle","content":["A song of ascents."]},{"type":"verse","number":1,"content":[{"text":"In my distress I called out to Yahweh,","poem":1},{"lineBreak":true},{"text":"and he answered me.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Rescue my life, Yahweh, from those who lie with their lips","poem":1},{"lineBreak":true},{"text":"and deceive with their tongues.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"How will he punish you, and what more will he do to you,","poem":1},{"lineBreak":true},{"text":"you who have a lying tongue?","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He will punish you with the arrows of a warrior","poem":1},{"lineBreak":true},{"text":"sharpened over burning coals of the broom tree.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Woe is me because I temporarily live in Meshech;","poem":1},{"lineBreak":true},{"text":"I lived previously among the tents of Kedar.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For too long I have lived","poem":1},{"lineBreak":true},{"text":"with those who hate peace.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"I am for peace,","poem":1},{"lineBreak":true},{"text":"but when I speak, they are for war.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/120.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/121.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/119.json","previousChapterAudioLinks":{},"numberOfVerses":7}