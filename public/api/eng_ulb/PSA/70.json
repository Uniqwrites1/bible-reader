{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":70,"content":[{"type":"hebrew_subtitle","content":["For the chief musician. A psalm of David; to bring to remembrance."]},{"type":"verse","number":1,"content":[{"text":"Save me, God!","poem":1},{"lineBreak":true},{"text":"Yahweh, come quickly and help me.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Let those who try to take my life","poem":1},{"lineBreak":true},{"text":"be ashamed and humiliated;","poem":1},{"lineBreak":true},{"text":"let them be turned back and brought to dishonor,","poem":1},{"lineBreak":true},{"text":"those who take pleasure in my pain.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Let them be turned back because of their shame,","poem":1},{"lineBreak":true},{"text":"those who say, “Aha, aha.”","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Let all those who seek you rejoice and be glad in you;","poem":1},{"lineBreak":true},{"text":"let those who love your salvation always say,","poem":1},{"lineBreak":true},{"text":"“May God be praised.”","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"But I am poor and needy;","poem":1},{"lineBreak":true},{"text":"hurry to me, God;","poem":1},{"lineBreak":true},{"text":"you are my help and you rescue me.","poem":1},{"lineBreak":true},{"text":"Yahweh, do not delay.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/70.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/71.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/69.json","previousChapterAudioLinks":{},"numberOfVerses":5}