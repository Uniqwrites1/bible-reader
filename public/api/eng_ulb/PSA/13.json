]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"But I have trusted in your covenant faithfulness;","poem":1},{"lineBreak":true},{"text":"my heart rejoices in your salvation.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will sing to Yahweh","poem":1},{"lineBreak":true},{"text":"because he has treated me very generously.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/13.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/14.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/12.json","previousChapterAudioLinks":{},"numberOfVerses":6}