{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":20,"content":[{"type":"hebrew_subtitle","content":["For the chief musician. A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"May Yahweh help you in the day of trouble;","poem":1},{"lineBreak":true},{"text":"may the name of the God of Jacob protect you","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"and send help from the holy place","poem":1},{"lineBreak":true},{"text":"to support you from Zion.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"May he call to mind all your offerings","poem":1},{"lineBreak":true},{"text":"and accept your burnt sacrifice. Selah","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"May he grant you your heart's desire","poem":1},{"lineBreak":true},{"text":"and fulfill all your plans.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Then we will rejoice in your victory,","poem":1},{"lineBreak":true},{"text":"and, in the name of our God, we will raise banners.","poem":1},{"lineBreak":true},{"text":"May Yahweh grant all your petitions.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Now I know that Yahweh will rescue his anointed one;","poem":1},{"lineBreak":true},{"text":"he will answer him from his holy heaven","poem":1},{"lineBreak":true},{"text":"with the strength of his right hand that can rescue him.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Some trust in chariots and others in horses,","poem":1},{"lineBreak":true},{"text":"but we call on Yahweh our God.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They will be brought down and fall,","poem":1},{"lineBreak":true},{"text":"but we will rise and stand upright!","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Yahweh, rescue the king;","poem":1},{"lineBreak":true},{"text":"help us when we call.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/20.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/21.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/19.json","previousChapterAudioLinks":{},"numberOfVerses":9}