{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":108,"content":[{"type":"hebrew_subtitle","content":["A song, a psalm of David."]},{"type":"verse","number":1,"content":[{"text":"My heart is fixed, God;","poem":1},{"lineBreak":true},{"text":"I will sing, yes, I will sing praises also with my honored heart.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Wake up, lute and harp;","poem":1},{"lineBreak":true},{"text":"I will wake up the dawn.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"I will give thanks to you, Yahweh, among the peoples;","poem":1},{"lineBreak":true},{"text":"I will sing praises to you among the nations.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For your covenant faithfulness is great above the heavens;","poem":1},{"lineBreak":true},{"text":"and your trustworthiness reaches to the skies.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Be exalted, God, above the heavens,","poem":1},{"lineBreak":true},{"text":"and may your glory be exalted over all the earth.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"So that those you love may be rescued,","poem":1},{"lineBreak":true},{"text":"rescue us with your right hand and answer me.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"God has spoken in his holiness; “I will rejoice;","poem":1},{"lineBreak":true},{"text":"I will divide Shechem and apportion out the Valley of Succoth.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Gilead is mine, and Manasseh is mine;","poem":1},{"lineBreak":true},{"text":"Ephraim also is my helmet;","poem":1},{"lineBreak":true},{"text":"Judah is my scepter.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Moab is my washbasin;","poem":1},{"lineBreak":true},{"text":"over Edom I will throw my shoe;","poem":1},{"lineBreak":true},{"text":"I will shout in triumph because of Philistia.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Who will bring me into the strong city?","poem":1},{"lineBreak":true},{"text":"Who will lead me to Edom?”","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"God, have you not rejected us?","poem":1},{"lineBreak":true},{"text":"You do not go into battle with our army.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Give us help against the enemy,","poem":1},{"lineBreak":true},{"text":"for man's help is futile.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"We will triumph with God's help;","poem":1},{"lineBreak":true},{"text":"he will trample down our enemies.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/108.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/109.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/107.json","previousChapterAudioLinks":{},"numberOfVerses":13}