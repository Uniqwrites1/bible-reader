{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":48,"content":[{"type":"hebrew_subtitle","content":["A song; a psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Great is Yahweh and greatly to be praised,","poem":1},{"lineBreak":true},{"text":"in the city of our God on his holy mountain.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Beautiful in elevation, the joy of the whole earth,","poem":1},{"lineBreak":true},{"text":"is Mount Zion, on the sides of the north,","poem":1},{"lineBreak":true},{"text":"the city of the great King.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"God has made himself known in her palaces as a refuge.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For, see, the kings assembled themselves;","poem":1},{"lineBreak":true},{"text":"they passed by together.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"They saw it, then they were amazed;","poem":1},{"lineBreak":true},{"text":"they were dismayed, and they hurried away.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Trembling took hold of them there,","poem":1},{"lineBreak":true},{"text":"pain as when a woman is in labor.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"With the east wind","poem":1},{"lineBreak":true},{"text":"you break the ships of Tarshish.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"As we have heard, so have we seen","poem":1},{"lineBreak":true},{"text":"in the city of Yahweh of hosts, in the city of our God;","poem":1},{"lineBreak":true},{"text":"God will establish it forever. Selah","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"We have thought about your covenant faithfulness, God,","poem":1},{"lineBreak":true},{"text":"in the middle of your temple.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"As your name is, God,","poem":1},{"lineBreak":true},{"text":"so is your praise to the ends of the earth;","poem":1},{"lineBreak":true},{"text":"your right hand is full of righteousness.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Let Mount Zion be glad,","poem":1},{"lineBreak":true},{"text":"let the daughters of Judah rejoice","poem":1},{"lineBreak":true},{"text":"because of your righteous decrees.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Walk around Mount Zion, go round about her;","poem":1},{"lineBreak":true},{"text":"count her towers,","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"notice well her walls,","poem":1},{"lineBreak":true},{"text":"and look at her palaces","poem":1},{"lineBreak":true},{"text":"so that you may tell it to the next generation.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"For this God is our God forever and ever;","poem":1},{"lineBreak":true},{"text":"he will be our guide to death.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/48.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/49.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/47.json","previousChapterAudioLinks":{},"numberOfVerses":14}