{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":99,"content":[{"type":"verse","number":1,"content":[{"text":"Yahweh reigns; let the nations tremble.","poem":1},{"lineBreak":true},{"text":"He sits enthroned above the cherubim; the earth quakes.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Yahweh is great in Zion;","poem":1},{"lineBreak":true},{"text":"he is exalted above all the nations.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Let them praise your great and awesome name;","poem":1},{"lineBreak":true},{"text":"he is holy.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The king is strong, and he loves justice.","poem":1},{"lineBreak":true},{"text":"You have established fairness;","poem":1},{"lineBreak":true},{"text":"you have done righteousness and justice in Jacob.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Praise Yahweh our God","poem":1},{"lineBreak":true},{"text":"and worship at his footstool.","poem":1},{"lineBreak":true},{"text":"He is holy.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Moses and Aaron were among his priests,","poem":1},{"lineBreak":true},{"text":"and Samuel was among those who called on his name.","poem":1},{"lineBreak":true},{"text":"They called to Yahweh, and he answered them.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He spoke to them from the pillar of cloud.","poem":1},{"lineBreak":true},{"text":"They kept his solemn commands","poem":1},{"lineBreak":true},{"text":"and the statutes that he gave them.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"You answered them, Yahweh our God.","poem":1},{"lineBreak":true},{"text":"A forgiving God you were to them,","poem":1},{"lineBreak":true},{"text":"but one who punished their sinful deeds.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Praise Yahweh our God,","poem":1},{"lineBreak":true},{"text":"and worship at his holy hill,","poem":1},{"lineBreak":true},{"text":"for Yahweh our God is holy.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/99.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/100.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/98.json","previousChapterAudioLinks":{},"numberOfVerses":9}