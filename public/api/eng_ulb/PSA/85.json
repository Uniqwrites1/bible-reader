{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":85,"content":[{"type":"hebrew_subtitle","content":["For the chief musician. A psalm of the sons of Korah."]},{"type":"verse","number":1,"content":[{"text":"Yahweh, you have showed favor to your land;","poem":1},{"lineBreak":true},{"text":"you have restored the well-being of Jacob.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"You have forgiven the sin of your people;","poem":1},{"lineBreak":true},{"text":"you have covered all their sin. Selah","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"You have withdrawn all your wrath;","poem":1},{"lineBreak":true},{"text":"you have turned back from your hot anger.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Restore us, God of our salvation,","poem":1},{"lineBreak":true},{"text":"and let go of your displeasure with us.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Will you be angry with us forever?","poem":1},{"lineBreak":true},{"text":"Will you remain angry throughout future generations?","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Will you not revive us again?","poem":1},{"lineBreak":true},{"text":"Then your people will rejoice in you.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Show us your covenant faithfulness, Yahweh,","poem":1},{"lineBreak":true},{"text":"grant us your salvation.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"I will listen to what Yahweh God says,","poem":1},{"lineBreak":true},{"text":"for he will make peace with his people, his faithful followers.","poem":1},{"lineBreak":true},{"text":"Yet they must not turn again to foolish ways.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Surely his salvation is near to those who fear him;","poem":1},{"lineBreak":true},{"text":"then glory will remain in our land.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Covenant faithfulness and trustworthiness have met together;","poem":1},{"lineBreak":true},{"text":"righteousness and peace have kissed each other.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Trustworthiness springs up from the ground,","poem":1},{"lineBreak":true},{"text":"and righteousness looks down from the sky.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"Yes, Yahweh will give his good blessings,","poem":1},{"lineBreak":true},{"text":"and our land will yield its crops.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Righteousness will go before him","poem":1},{"lineBreak":true},{"text":"and make a way for his footsteps.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/85.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/86.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/84.json","previousChapterAudioLinks":{},"numberOfVerses":13}