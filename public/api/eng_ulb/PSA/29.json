{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":29,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Ascribe to Yahweh, you sons of God,","poem":1},{"lineBreak":true},{"text":"ascribe to Yahweh glory and strength!","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Ascribe to Yahweh the glory his name deserves.","poem":1},{"lineBreak":true},{"text":"Bow down to Yahweh in the splendor of holiness!","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The voice of Yahweh is heard over the waters;","poem":1},{"lineBreak":true},{"text":"the God of glory thunders,","poem":1},{"lineBreak":true},{"text":"Yahweh thunders over many waters.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The voice of Yahweh is powerful;","poem":1},{"lineBreak":true},{"text":"the voice of Yahweh is majestic.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"The voice of Yahweh breaks the cedars;","poem":1},{"lineBreak":true},{"text":"Yahweh breaks in pieces the cedars of Lebanon.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He makes Lebanon skip like a calf","poem":1},{"lineBreak":true},{"text":"and Sirion like a young ox.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"The voice of Yahweh sends out flames of fire.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"The voice of Yahweh shakes the wilderness;","poem":1},{"lineBreak":true},{"text":"Yahweh shakes the wilderness of Kadesh.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"The voice of Yahweh causes the oaks to twist","poem":1},{"lineBreak":true},{"text":"and strips the forests bare.","poem":1},{"lineBreak":true},{"text":"Everyone in his temple says, “Glory!”","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Yahweh sits as king over the flood;","poem":1},{"lineBreak":true},{"text":"Yahweh sits as king forever.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Yahweh gives strength to his people;","poem":1},{"lineBreak":true},{"text":"Yahweh blesses his people with peace.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/29.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/30.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/28.json","previousChapterAudioLinks":{},"numberOfVerses":11}