content":[{"lineBreak":true},{"text":"I will not be afraid of the multitudes of people","poem":1},{"lineBreak":true},{"text":"who have set themselves against me on every side.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Rise up, Yahweh! Save me, my God!","poem":1},{"lineBreak":true},{"text":"For you will hit all my enemies on the jaw;","poem":1},{"lineBreak":true},{"text":"you will break the teeth of the wicked.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Salvation comes from Yahweh.","poem":1},{"lineBreak":true},{"text":"May your blessings be on your people. Selah","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/3.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/4.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/2.json","previousChapterAudioLinks":{},"numberOfVerses":8}