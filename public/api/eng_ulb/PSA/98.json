{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":98,"content":[{"type":"hebrew_subtitle","content":["A psalm."]},{"type":"verse","number":1,"content":[{"text":"Oh, sing to Yahweh a new song,","poem":1},{"lineBreak":true},{"text":"for he has done marvelous things;","poem":1},{"lineBreak":true},{"text":"his right hand and his holy arm have given him victory.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Yahweh has made known his salvation;","poem":1},{"lineBreak":true},{"text":"he has openly showed his justice to all the nations.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"He calls to mind his covenant loyalty and faithfulness for the house of Israel;","poem":1},{"lineBreak":true},{"text":"all the ends of the earth will see the victory of our God.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"Shout for joy to Yahweh, all the earth;","poem":1},{"lineBreak":true},{"text":"burst into song, sing for joy, and sing praises.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Sing praises to Yahweh with the harp,","poem":1},{"lineBreak":true},{"text":"with the harp and melodious song.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"With trumpets and the sound of the horn,","poem":1},{"lineBreak":true},{"text":"make a joyful noise before the King, Yahweh.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Let the sea shout and everything in it,","poem":1},{"lineBreak":true},{"text":"the world and those who live in it!","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Let the rivers clap their hands,","poem":1},{"lineBreak":true},{"text":"and let the mountains shout for joy.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Yahweh is coming to judge the earth;","poem":1},{"lineBreak":true},{"text":"he will judge the world with righteousness","poem":1},{"lineBreak":true},{"text":"and the nations with fairness.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/98.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/99.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/97.json","previousChapterAudioLinks":{},"numberOfVerses":9}