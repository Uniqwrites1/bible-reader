{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":24,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"The earth is Yahweh's, and its fullness,","poem":1},{"lineBreak":true},{"text":"the world, and all who live in it.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"For he has founded it upon the seas","poem":1},{"lineBreak":true},{"text":"and established it on the rivers.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Who will ascend the mountain of Yahweh?","poem":1},{"lineBreak":true},{"text":"Who will stand in his holy place?","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He who has clean hands and a pure heart;","poem":1},{"lineBreak":true},{"text":"who has not lifted up a falsehood,","poem":1},{"lineBreak":true},{"text":"and has not sworn an oath in order to deceive.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"He will receive a blessing from Yahweh","poem":1},{"lineBreak":true},{"text":"and righteousness from the God of his salvation.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Such is the generation of those who seek him,","poem":1},{"lineBreak":true},{"text":"those who seek the face of the God of Jacob. Selah","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Lift up your heads, you gates;","poem":1},{"lineBreak":true},{"text":"be lifted up, everlasting doors,","poem":1},{"lineBreak":true},{"text":"so that the King of glory may come in!","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Who is this King of glory?","poem":1},{"lineBreak":true},{"text":"Yahweh, strong and mighty; Yahweh, mighty in battle.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Lift up your heads, you gates;","poem":1},{"lineBreak":true},{"text":"be lifted up, everlasting doors,","poem":1},{"lineBreak":true},{"text":"so that the King of glory may come in!","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"Who is this King of glory?","poem":1},{"lineBreak":true},{"text":"Yahweh of hosts,","poem":1},{"lineBreak":true},{"text":"he is the King of glory. Selah","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/24.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/25.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/23.json","previousChapterAudioLinks":{},"numberOfVerses":10}