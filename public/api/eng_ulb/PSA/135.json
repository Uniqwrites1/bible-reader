{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":135,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"Praise the name of Yahweh.","poem":1},{"lineBreak":true},{"text":"Praise him, you servants of Yahweh,","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"you who stand in Yahweh's house,","poem":1},{"lineBreak":true},{"text":"in the courtyards of the house of our God.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Praise Yahweh, for he is good;","poem":1},{"lineBreak":true},{"text":"sing praises to his name, for it is pleasant to do so.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"For Yahweh has chosen Jacob for himself,","poem":1},{"lineBreak":true},{"text":"Israel as his possession.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I know that Yahweh is great,","poem":1},{"lineBreak":true},{"text":"that our Lord is above all gods.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Whatever Yahweh desires, he does","poem":1},{"lineBreak":true},{"text":"in heaven, on earth, in the seas and all the ocean depths.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He brings the clouds from far away,","poem":1},{"lineBreak":true},{"text":"making lightning bolts accompany the rain","poem":1},{"lineBreak":true},{"text":"and bringing the wind out of his storehouse.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"He killed the firstborn of Egypt,","poem":1},{"lineBreak":true},{"text":"both of man and animals.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He sent signs and wonders into your midst, Egypt,","poem":1},{"lineBreak":true},{"text":"against Pharaoh and all his servants.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"He attacked many nations","poem":1},{"lineBreak":true},{"text":"and killed mighty kings,","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"Sihon king of the Amorites","poem":1},{"lineBreak":true},{"text":"and Og king of Bashan","poem":1},{"lineBreak":true},{"text":"and all the kingdoms of Canaan.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"He gave us their land as an inheritance,","poem":1},{"lineBreak":true},{"text":"an inheritance to Israel his people.","poem":1}]},{"type":"verse","number":13,"content":[{"lineBreak":true},{"text":"Your name, Yahweh, endures forever;","poem":1},{"lineBreak":true},{"text":"your renown, Yahweh, endures throughout all generations.","poem":1}]},{"type":"verse","number":14,"content":[{"lineBreak":true},{"text":"For Yahweh defends his people","poem":1},{"lineBreak":true},{"text":"and has compassion on his servants.","poem":1}]},{"type":"verse","number":15,"content":[{"lineBreak":true},{"text":"The nations' idols are silver and gold,","poem":1},{"lineBreak":true},{"text":"the work of men's hands.","poem":1}]},{"type":"verse","number":16,"content":[{"lineBreak":true},{"text":"Those idols have mouths, but they do not speak;","poem":1},{"lineBreak":true},{"text":"they have eyes, but they do not see;","poem":1}]},{"type":"verse","number":17,"content":[{"lineBreak":true},{"text":"they have ears, but they do not hear,","poem":1},{"lineBreak":true},{"text":"nor is there breath in their mouths.","poem":1}]},{"type":"verse","number":18,"content":[{"lineBreak":true},{"text":"Those who make them are like them,","poem":1},{"lineBreak":true},{"text":"as is everyone who trusts in them.","poem":1}]},{"type":"verse","number":19,"content":[{"lineBreak":true},{"text":"Descendants of Israel, bless Yahweh;","poem":1},{"lineBreak":true},{"text":"descendants of Aaron, bless Yahweh.","poem":1}]},{"type":"verse","number":20,"content":[{"lineBreak":true},{"text":"Descendants of Levi, bless Yahweh;","poem":1},{"lineBreak":true},{"text":"you who honor Yahweh, bless Yahweh.","poem":1}]},{"type":"verse","number":21,"content":[{"lineBreak":true},{"text":"Blessed be Yahweh in Zion,","poem":1},{"lineBreak":true},{"text":"he who lives in Jerusalem.","poem":1},{"lineBreak":true},{"text":"Praise Yahweh.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/135.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/136.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/134.json","previousChapterAudioLinks":{},"numberOfVerses":21}