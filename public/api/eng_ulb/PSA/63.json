{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":63,"content":[{"type":"hebrew_subtitle","content":["A psalm of David, when he was in the wilderness of Judah."]},{"type":"verse","number":1,"content":[{"text":"God, you are my God! I earnestly search for you,","poem":1},{"lineBreak":true},{"text":"my soul thirsts for you, and my flesh longs for you,","poem":1},{"lineBreak":true},{"text":"in a dry and weary land where there is no water.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"So I have looked on you in the sanctuary,","poem":1},{"lineBreak":true},{"text":"to see your power and your glory.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Because your covenant faithfulness is better than life,","poem":1},{"lineBreak":true},{"text":"my lips will praise you.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"So I will bless you while I live;","poem":1},{"lineBreak":true},{"text":"I will lift up my hands in your name.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"It will be as if I ate a meal of marrow and fatness;","poem":1},{"lineBreak":true},{"text":"with joyful lips my mouth will praise you,","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"when I think about you on my bed","poem":1},{"lineBreak":true},{"text":"and meditate on you in the night hours.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For you have been my help,","poem":1},{"lineBreak":true},{"text":"and in the shadow of your wings I rejoice.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"I cling to you;","poem":1},{"lineBreak":true},{"text":"your right hand supports me.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"But those who seek to destroy my life","poem":1},{"lineBreak":true},{"text":"will go down into the lowest parts of the earth;","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"they will be given over to those whose hands use the sword,","poem":1},{"lineBreak":true},{"text":"and they will become food for the jackals.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"But the king will rejoice in God;","poem":1},{"lineBreak":true},{"text":"everyone who swears by him will be proud of him,","poem":1},{"lineBreak":true},{"text":"but the mouth of those who speak lies will be stopped up.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/63.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/64.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/62.json","previousChapterAudioLinks":{},"numberOfVerses":11}