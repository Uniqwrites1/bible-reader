{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":111,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh.","poem":1},{"lineBreak":true},{"text":"I will give thanks to Yahweh with my whole heart","poem":1},{"lineBreak":true},{"text":"in the assembly of the upright, in their gathering.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"The works of Yahweh are great,","poem":1},{"lineBreak":true},{"text":"eagerly awaited by all those who desire them.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"His work is majestic and glorious,","poem":1},{"lineBreak":true},{"text":"and his righteousness endures forever.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"He does wonderful things that will be remembered;","poem":1},{"lineBreak":true},{"text":"Yahweh is gracious and merciful.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"He gives food to his faithful followers.","poem":1},{"lineBreak":true},{"text":"He will always call to mind his covenant.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"He showed his powerful works to his people","poem":1},{"lineBreak":true},{"text":"in giving them the inheritance of the nations.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"The works of his hands are trustworthy and just;","poem":1},{"lineBreak":true},{"text":"all his instructions are reliable.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"They are established forever,","poem":1},{"lineBreak":true},{"text":"to be observed faithfully and properly.","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"He gave victory to his people;","poem":1},{"lineBreak":true},{"text":"he ordained his covenant forever;","poem":1},{"lineBreak":true},{"text":"holy and awesome is his name.","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"To honor Yahweh is the beginning of wisdom;","poem":1},{"lineBreak":true},{"text":"those who carry out his instructions have good understanding.","poem":1},{"lineBreak":true},{"text":"His praise endures forever.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/111.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/112.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/110.json","previousChapterAudioLinks":{},"numberOfVerses":10}