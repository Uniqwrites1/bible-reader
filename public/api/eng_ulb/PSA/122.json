{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":122,"content":[{"type":"hebrew_subtitle","content":["A song of ascents, of David."]},{"type":"verse","number":1,"content":[{"text":"I was glad when they said to me,","poem":1},{"lineBreak":true},{"text":"“Let us go to the house of Yahweh.”","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Jerusalem, our feet are standing","poem":1},{"lineBreak":true},{"text":"within your gates!","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Jerusalem, built","poem":1},{"lineBreak":true},{"text":"as a city carefully planned!","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The tribes go up to Jerusalem—the tribes of Yahweh—","poem":1},{"lineBreak":true},{"text":"as a testimony for Israel,","poem":1},{"lineBreak":true},{"text":"to give thanks to the name of Yahweh.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"There thrones of judgment were set,","poem":1},{"lineBreak":true},{"text":"thrones of the house of David.","poem":1}]