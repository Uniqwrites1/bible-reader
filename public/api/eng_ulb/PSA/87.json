{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":87,"content":[{"type":"hebrew_subtitle","content":["A psalm of the sons of Korah; a song."]},{"type":"verse","number":1,"content":[{"text":"On the holy mount stands the city he founded;","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Yahweh loves the gates of Zion","poem":1},{"lineBreak":true},{"text":"more than all the tents of Jacob.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"Glorious things are said of you, city of God. Selah","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"“I mention Rahab and Babylon to my followers.","poem":1},{"lineBreak":true},{"text":"See, there are Philistia, and Tyre, along with Cush—","poem":1},{"lineBreak":true},{"text":"and will say, 'This one was born there.'”","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Of Zion it will be said, “Each of these was born in her;","poem":1},{"lineBreak":true},{"text":"and the Most High himself will establish her.”","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Yahweh writes in the census book of the nations,","poem":1},{"lineBreak":true},{"text":"“This one was born there.” Selah","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"So also the singers and the dancers say together,","poem":1},{"lineBreak":true},{"text":"“All my fountains are in you.”","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/87.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/88.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/86.json","previousChapterAudioLinks":{},"numberOfVerses":7}