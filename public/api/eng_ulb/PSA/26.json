{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":26,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Judge me, Yahweh, for I have walked with integrity;","poem":1},{"lineBreak":true},{"text":"I have trusted in Yahweh without wavering.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Examine me, Yahweh, and test me;","poem":1},{"lineBreak":true},{"text":"test the purity of my inner parts and my heart!","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For your covenant faithfulness is before my eyes,","poem":1},{"lineBreak":true},{"text":"and I walk about in your faithfulness.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"I do not associate with deceitful people,","poem":1},{"lineBreak":true},{"text":"nor do I mingle with dishonest people.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"I hate the assembly of evildoers,","poem":1},{"lineBreak":true},{"text":"and I do not live with the wicked.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I wash my hands in innocence,","poem":1},{"lineBreak":true},{"text":"and I go around your altar, Yahweh,","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"to sing a loud song of praise","poem":1},{"lineBreak":true},{"text":"and report all your wonderful deeds.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Yahweh, I love the house where you live,","poem":1},{"lineBreak":true},{"text":"the place where your glory lives!","poem":1}]},{"type":"verse","number":9,"content":[{"lineBreak":true},{"text":"Do not sweep me away with sinners,","poem":1},{"lineBreak":true},{"text":"or my life with people who are bloodthirsty,","poem":1}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"in whose hands there is a plot,","poem":1},{"lineBreak":true},{"text":"and whose right hand is full of bribes.","poem":1}]},{"type":"verse","number":11,"content":[{"lineBreak":true},{"text":"But as for me, I will walk in integrity;","poem":1},{"lineBreak":true},{"text":"redeem me and have mercy on me.","poem":1}]},{"type":"verse","number":12,"content":[{"lineBreak":true},{"text":"My foot stands on level ground;","poem":1},{"lineBreak":true},{"text":"in the assemblies will I bless Yahweh!","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/25.json","previousChapterAudioLinks":{},"numberOfVerses":12}