{"translation":{"id":"eng_ulb","name":"Unlocked Literal Bible","website":"https://ebible.org/Scriptures/details.php?id=engULB","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engULB","licenseNotes":null,"shortName":"ULB","englishName":"Unlocked Literal Bible","language":"eng","textDirection":"ltr","sha256":"5063a05fdc9fa94aa1d9aab2b0dd0b3a9dd5a1590a82add60415273de585342f","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_ulb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31094,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_ulb","name":"Psalms","commonName":"Psalms","title":"Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"a1bbb8b3e49a7a6f48276a7bef3fcaab5893a0789a457b53033356841a9f68b8","firstChapterApiLink":"/api/eng_ulb/PSA/1.json","lastChapterApiLink":"/api/eng_ulb/PSA/150.json","totalNumberOfVerses":2460},"chapter":{"number":138,"content":[{"type":"hebrew_subtitle","content":["A psalm of David."]},{"type":"verse","number":1,"content":[{"text":"I will give you thanks with my whole heart;","poem":1},{"lineBreak":true},{"text":"before the gods I will sing praises to you.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"I will bow down toward your holy temple","poem":1},{"lineBreak":true},{"text":"and give thanks to your name for your covenant faithfulness and for your trustworthiness.","poem":1},{"lineBreak":true},{"text":"You have made your word and your name more important than anything else.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"On the day that I called you, you answered me;","poem":1},{"lineBreak":true},{"text":"you made me bold and strengthened my soul.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"All the kings of the earth will give you thanks, Yahweh,","poem":1},{"lineBreak":true},{"text":"for they will hear the words from your mouth.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Indeed, they will sing of the deeds of Yahweh,","poem":1},{"lineBreak":true},{"text":"for great is the glory of Yahweh.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"For though Yahweh is high, yet he cares for the lowly,","poem":1},{"lineBreak":true},{"text":"but the proud he knows from far off.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Though I walk in the middle of danger, you will preserve my life;","poem":1},{"lineBreak":true},{"text":"you will reach out with your hand against the anger of my enemies,","poem":1},{"lineBreak":true},{"text":"and your right hand will save me.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Yahweh is with me to the end;","poem":1},{"lineBreak":true},{"text":"your covenant faithfulness, Yahweh, endures forever.","poem":1},{"lineBreak":true},{"text":"Do not forsake the ones whom your hands have made.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/138.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/139.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/137.json","previousChapterAudioLinks":{},"numberOfVerses":8}