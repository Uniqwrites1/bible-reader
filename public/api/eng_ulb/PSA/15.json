":[{"lineBreak":true},{"text":"He does not charge interest when he lends money.","poem":1},{"lineBreak":true},{"text":"He does not take bribes to testify against the innocent.","poem":1},{"lineBreak":true},{"text":"He who does these things will never be shaken.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_ulb/PSA/15.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_ulb/PSA/16.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_ulb/PSA/14.json","previousChapterAudioLinks":{},"numberOfVerses":5}