{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":82,"content":[{"type":"hebrew_subtitle","content":["A Psalm by Asaph."]},{"type":"verse","number":1,"content":[{"text":"God presides in the great assembly.","poem":1},{"text":"He judges among the gods.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"“How long will you judge unjustly,","poem":1},{"text":"and show partiality to the wicked?” Selah.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"“Defend the weak, the poor, and the fatherless.","poem":1},{"text":"Maintain the rights of the poor and oppressed.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Rescue the weak and needy.","poem":1},{"text":"Deliver them out of the hand of the wicked.”","poem":2}]},{"type":"verse","number":5,"content":[{"text":"They don’t know, neither do they understand.","poem":1},{"text":"They walk back and forth in darkness.","poem":2},{"lineBreak":true},{"text":"All the foundations of the earth are shaken.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I said, “You are gods,","poem":1},{"text":"all of you are sons of the Most High.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Nevertheless you shall die like men,","poem":1},{"text":"and fall like one of the rulers.”","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Arise, God, judge the earth,","poem":1},{"text":"for you inherit all of the nations.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/82.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/83.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/81.json","previousChapterAudioLinks":{},"numberOfVerses":8}