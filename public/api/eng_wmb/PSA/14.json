{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":14,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. By David."]},{"type":"verse","number":1,"content":[{"text":"The fool has said in his heart, “There is no God.”","poem":1},{"text":"They are corrupt.","poem":2},{"lineBreak":true},{"text":"They have done abominable deeds.","poem":2},{"lineBreak":true},{"text":"There is no one who does good.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"The LORD looked down from heaven on the children of men,","poem":1},{"text":"to see if there were any who understood,","poem":2},{"lineBreak":true},{"text":"who sought after God.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"They have all gone aside.","poem":1},{"text":"They have together become corrupt.","poem":2},{"lineBreak":true},{"text":"There is no one who does good, no, not one.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Have all the workers of iniquity no knowledge,","poem":1},{"text":"who eat up my people as they eat bread,","poem":2},{"lineBreak":true},{"text":"and don’t call on the LORD?","poem":2}]},{"type":"verse","number":5,"content":[{"text":"There they were in great fear,","poem":1},{"text":"for God is in the generation of the righteous.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"You frustrate the plan of the poor,","poem":1},{"text":"because the LORD is his refuge.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Oh that the salvation [yeshuat] of Israel would come out of Zion!","poem":1},{"text":"When the LORD restores the fortunes of his people,","poem":2},{"lineBreak":true},{"text":"then Jacob shall rejoice, and Israel shall be glad.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/14.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/15.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/13.json","previousChapterAudioLinks":{},"numberOfVerses":7}