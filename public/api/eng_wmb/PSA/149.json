{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":149,"content":[{"type":"verse","number":1,"content":[{"text":"Praise the LORD!","poem":1},{"text":"Sing to the LORD a new song,","poem":2},{"lineBreak":true},{"text":"his praise in the assembly of the holy ones.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Let Israel rejoice in him who made them.","poem":1},{"text":"Let the children of Zion be joyful in their King.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them praise his name in the dance!","poem":1},{"text":"Let them sing praises to him with tambourine and harp!","poem":2}]},{"type":"verse","number":4,"content":[{"text":"For the LORD takes pleasure in his people.","poem":1},{"text":"He crowns the humble with salvation [yeshu`ah].","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let the holy ones rejoice in honor.","poem":1},{"text":"Let them sing for joy on their beds.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"May the high praises of God be in their mouths,","poem":1},{"text":"and a two-edged sword in their hand,","poem":2}]},{"type":"verse","number":7,"content":[{"text":"to execute vengeance on the nations,","poem":1},{"text":"and punishments on the peoples;","poem":2}]},{"type":"verse","number":8,"content":[{"text":"to bind their kings with chains,","poem":1},{"text":"and their nobles with fetters of iron;","poem":2}]},{"type":"verse","number":9,"content":[{"text":"to execute on them the written judgment.","poem":1},{"text":"All his holy ones have this honor.","poem":2},{"text":"Praise the LORD!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}