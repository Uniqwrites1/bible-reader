{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":4,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; on stringed instruments. A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"Answer me when I call, God of my righteousness.","poem":1},{"text":"Give me relief from my distress.","poem":2},{"lineBreak":true},{"text":"Have mercy on me, and hear my prayer.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"You sons of men, how long shall my glory be turned into dishonor?","poem":1},{"text":"Will you love vanity and seek after falsehood? Selah.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"But know that the LORD has set apart for himself him who is godly;","poem":1},{"text":"The LORD will hear when I call to him.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Stand in awe, and don’t sin.","poem":1},{"text":"Search your own heart on your bed, and be still. Selah.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Offer the sacrifices of righteousness.","poem":1},{"text":"Put your trust in the LORD.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Many say, “Who will show us any good?”","poem":1},{"text":"LORD, let the light of your face shine on us.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"You have put gladness in my heart,","poem":1},{"text":"more than when their grain and their new wine are increased.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"In peace I will both lay myself down and sleep,","poem":1},{"text":"for you alone, LORD, make me live in safety.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/4.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/5.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/3.json","previousChapterAudioLinks":{},"numberOfVerses":8}