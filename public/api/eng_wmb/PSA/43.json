{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"verse","number":1,"content":[{"text":"Vindicate me, God, and plead my cause against an ungodly nation.","poem":1},{"text":"Oh, deliver me from deceitful and wicked men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For you are the God of my strength. Why have you rejected me?","poem":1},{"text":"Why do I go mourning because of the oppression of the enemy?","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Oh, send out your light and your truth.","poem":1},{"text":"Let them lead me.","poem":2},{"lineBreak":true},{"text":"Let them bring me to your holy hill,","poem":2},{"lineBreak":true},{"text":"to your tents.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Then I will go to the altar of God,","poem":1},{"text":"to God, my exceeding joy.","poem":2},{"text":"I will praise you on the harp, God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why are you in despair, my soul?","poem":1},{"text":"Why are you disturbed within me?","poem":2},{"text":"Hope in God!","poem":1},{"text":"For I shall still praise him:","poem":2},{"lineBreak":true},{"text":"my Savior, my helper, and my God.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}