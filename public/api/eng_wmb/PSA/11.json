{"translation":{"id":"eng_wmb","name":"World Messianic Bible","website":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwmb","licenseNotes":null,"shortName":"WMB","englishName":"World Messianic Bible","language":"eng","textDirection":"ltr","sha256":"f0dbb078d9952fb47ccb862a04202c7b9c43c0ea77f155bc968d56dd14371fb9","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_wmb/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_wmb","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"96ac984eb4f7c01a740116c71025e61b667827aff59bdb32a89fdba378cd1d95","firstChapterApiLink":"/api/eng_wmb/PSA/1.json","lastChapterApiLink":"/api/eng_wmb/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":11,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. By David."]},{"type":"verse","number":1,"content":[{"text":"In the LORD, I take refuge.","poem":1},{"text":"How can you say to my soul, “Flee as a bird to your mountain”?","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For, behold, the wicked bend their bows.","poem":1},{"text":"They set their arrows on the strings,","poem":2},{"lineBreak":true},{"text":"that they may shoot in darkness at the upright in heart.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"If the foundations are destroyed,","poem":1},{"text":"what can the righteous do?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"The LORD is in his holy temple.","poem":1},{"text":"The LORD is on his throne in heaven.","poem":2},{"text":"His eyes observe.","poem":1},{"text":"His eyes examine the children of men.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"The LORD examines the righteous,","poem":1},{"text":"but his soul hates the wicked and him who loves violence.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"On the wicked he will rain blazing coals;","poem":1},{"text":"fire, sulfur, and scorching wind shall be the portion of their cup.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"For the LORD is righteous.","poem":1},{"text":"He loves righteousness.","poem":2},{"lineBreak":true},{"text":"The upright shall see his face.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_wmb/PSA/11.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_wmb/PSA/12.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_wmb/PSA/10.json","previousChapterAudioLinks":{},"numberOfVerses":7}