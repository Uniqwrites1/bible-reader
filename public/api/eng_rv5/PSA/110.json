ontent":[{"lineBreak":true},{"text":"He shall judge among the nations, he shall fill the places with dead bodies; he shall strike through the head in many countries.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He shall drink of the brook in the way: therefore shall he lift up the head.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_rv5/PSA/110.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_rv5/PSA/111.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_rv5/PSA/109.json","previousChapterAudioLinks":{},"numberOfVerses":7}