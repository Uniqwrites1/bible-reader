"number":7,"content":[{"lineBreak":true},{"text":"Nevertheless ye shall die like men, and fall like one of the princes.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Arise, O God, judge the earth: for thou shalt inherit all the nations.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_rv5/PSA/82.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_rv5/PSA/83.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_rv5/PSA/81.json","previousChapterAudioLinks":{},"numberOfVerses":8}