{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":101,"content":[{"type":"hebrew_subtitle","content":["A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"I will sing of loving kindness and justice.","poem":1},{"text":"To you, LORD, I will sing praises.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"I will be careful to live a blameless life.","poem":1},{"text":"When will you come to me?","poem":2},{"lineBreak":true},{"text":"I will walk within my house with a blameless heart.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"I will set no vile thing before my eyes.","poem":1},{"text":"I hate the deeds of faithless men.","poem":2},{"lineBreak":true},{"text":"They will not cling to me.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"A perverse heart will be far from me.","poem":1},{"text":"I will have nothing to do with evil.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I will silence whoever secretly slanders his neighbor.","poem":1},{"text":"I won’t tolerate one who is arrogant and conceited.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"My eyes will be on the faithful of the land,","poem":1},{"text":"that they may dwell with me.","poem":2},{"text":"He who walks in a perfect way,","poem":1},{"text":"he will serve me.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"He who practices deceit won’t dwell within my house.","poem":1},{"text":"He who speaks falsehood won’t be established before my eyes.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Morning by morning, I will destroy all the wicked of the land,","poem":1},{"text":"to cut off all the workers of iniquity from the LORD’s city.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/101.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/102.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/100.json","previousChapterAudioLinks":{},"numberOfVerses":8}