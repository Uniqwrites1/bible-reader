{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":70,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. By David. A reminder."]},{"type":"verse","number":1,"content":[{"text":"Hurry, God, to deliver me.","poem":1},{"text":"Come quickly to help me, LORD.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Let them be disappointed and confounded who seek my soul.","poem":1},{"text":"Let those who desire my ruin be turned back in disgrace.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them be turned because of their shame","poem":1},{"text":"who say, “Aha! Aha!”","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Let all those who seek you rejoice and be glad in you.","poem":1},{"text":"Let those who love your salvation continually say,","poem":2},{"lineBreak":true},{"text":"“Let God be exalted!”","poem":2}]},{"type":"verse","number":5,"content":[{"text":"But I am poor and needy.","poem":1},{"text":"Come to me quickly, God.","poem":2},{"text":"You are my help and my deliverer.","poem":1},{"text":"LORD, don’t delay.","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/70.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/71.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/69.json","previousChapterAudioLinks":{},"numberOfVerses":5}