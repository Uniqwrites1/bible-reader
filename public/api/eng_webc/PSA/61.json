{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":61,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. For a stringed instrument. By David."]},{"type":"verse","number":1,"content":[{"text":"Hear my cry, God.","poem":1},{"text":"Listen to my prayer.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"From the end of the earth, I will call to you when my heart is overwhelmed.","poem":1},{"text":"Lead me to the rock that is higher than I.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"For you have been a refuge for me,","poem":1},{"text":"a strong tower from the enemy.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"I will dwell in your tent forever.","poem":1},{"text":"I will take refuge in the shelter of your wings. Selah.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"For you, God, have heard my vows.","poem":1},{"text":"You have given me the heritage of those who fear your name.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"You will prolong the king’s life.","poem":1},{"text":"His years will be for generations.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"He shall be enthroned in God’s presence forever.","poem":1},{"text":"Appoint your loving kindness and truth, that they may preserve him.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"So I will sing praise to your name forever,","poem":1},{"text":"that I may fulfill my vows daily.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/61.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/62.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/60.json","previousChapterAudioLinks":{},"numberOfVerses":8}