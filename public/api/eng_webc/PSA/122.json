{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":122,"content":[{"type":"hebrew_subtitle","content":["A Song of Ascents. By David."]},{"type":"verse","number":1,"content":[{"text":"I was glad when they said to me,","poem":1},{"text":"“Let’s go to the LORD’s house!”","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Our feet are standing within your gates, Jerusalem!","poem":1}]},{"type":"verse","number":3,"content":[{"text":"Jerusalem is built as a city that is compact together,","poem":2}]},{"type":"verse","number":4,"content":[{"text":"where the tribes go up, even the LORD’s tribes,","poem":1},{"text":"according to an ordinance for Israel,","poem":2},{"lineBreak":true},{"text":"to give thanks to the LORD’s name.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"For there are set thrones for judgment,","poem":1},{"text":"the thrones of David’s house.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Pray for the peace of Jerusalem.","poem":1},{"text":"Those who love you will prosper.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Peace be within your walls,","poem":1},{"text":"and prosperity within your palaces.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"For my brothers’ and companions’ sakes,","poem":1},{"text":"I will now say, “Peace be within you.”","poem":2}]},{"type":"verse","number":9,"content":[{"text":"For the sake of the house of the LORD our God,","poem":1},{"text":"I will seek your good.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/122.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/123.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/121.json","previousChapterAudioLinks":{},"numberOfVerses":9}