{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"verse","number":1,"content":[{"text":"Vindicate me, God, and plead my cause against an ungodly nation.","poem":1},{"text":"Oh, deliver me from deceitful and wicked men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For you are the God of my strength. Why have you rejected me?","poem":1},{"text":"Why do I go mourning because of the oppression of the enemy?","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Oh, send out your light and your truth.","poem":1},{"text":"Let them lead me.","poem":2},{"lineBreak":true},{"text":"Let them bring me to your holy hill,","poem":2},{"lineBreak":true},{"text":"to your tents.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Then I will go to the altar of God,","poem":1},{"text":"to God, my exceeding joy.","poem":2},{"text":"I will praise you on the harp, God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why are you in despair, my soul?","poem":1},{"text":"Why are you disturbed within me?","poem":2},{"text":"Hope in God!","poem":1},{"text":"For I shall still praise him:","poem":2},{"lineBreak":true},{"text":"my Savior, my helper, and my God.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}