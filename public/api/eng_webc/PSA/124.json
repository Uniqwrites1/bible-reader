erse","number":7,"content":[{"text":"Our soul has escaped like a bird out of the fowler’s snare.","poem":1},{"text":"The snare is broken, and we have escaped.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Our help is in the LORD’s name,","poem":1},{"text":"who made heaven and earth.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/124.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/125.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/123.json","previousChapterAudioLinks":{},"numberOfVerses":8}