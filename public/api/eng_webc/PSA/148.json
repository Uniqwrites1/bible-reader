{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":148,"content":[{"type":"verse","number":1,"content":[{"text":"Praise the LORD!","poem":1},{"text":"Praise the LORD from the heavens!","poem":2},{"lineBreak":true},{"text":"Praise him in the heights!","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Praise him, all his angels!","poem":1},{"text":"Praise him, all his army!","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Praise him, sun and moon!","poem":1},{"text":"Praise him, all you shining stars!","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Praise him, you heavens of heavens,","poem":1},{"text":"you waters that are above the heavens.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let them praise the LORD’s name,","poem":1},{"text":"for he commanded, and they were created.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"He has also established them forever and ever.","poem":1},{"text":"He has made a decree which will not pass away.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Praise the LORD from the earth,","poem":1},{"text":"you great sea creatures, and all depths,","poem":2}]},{"type":"verse","number":8,"content":[{"text":"lightning and hail, snow and clouds,","poem":1},{"text":"stormy wind, fulfilling his word,","poem":2}]},{"type":"verse","number":9,"content":[{"text":"mountains and all hills,","poem":1},{"text":"fruit trees and all cedars,","poem":2}]},{"type":"verse","number":10,"content":[{"text":"wild animals and all livestock,","poem":1},{"text":"small creatures and flying birds,","poem":2}]},{"type":"verse","number":11,"content":[{"text":"kings of the earth and all peoples,","poem":1},{"text":"princes and all judges of the earth,","poem":2}]},{"type":"verse","number":12,"content":[{"text":"both young men and maidens,","poem":1},{"text":"old men and children.","poem":2}]},{"type":"verse","number":13,"content":[{"text":"Let them praise the LORD’s name,","poem":1},{"text":"for his name alone is exalted.","poem":2},{"lineBreak":true},{"text":"His glory is above the earth and the heavens.","poem":2}]},{"type":"verse","number":14,"content":[{"text":"He has lifted up the horn of his people,","poem":1},{"text":"the praise of all his saints,","poem":2},{"lineBreak":true},{"text":"even of the children of Israel, a people near to him.","poem":2},{"text":"Praise the LORD!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/148.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/149.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/147.json","previousChapterAudioLinks":{},"numberOfVerses":14}