verse","number":7,"content":[{"text":"who executes justice for the oppressed;","poem":1},{"text":"who gives food to the hungry.","poem":2},{"text":"The LORD frees the prisoners.","poem":1}]},{"type":"verse","number":8,"content":[{"text":"The LORD opens the eyes of the blind.","poem":2},{"lineBreak":true},{"text":"The LORD raises up those who are bowed down.","poem":2},{"lineBreak":true},{"text":"The LORD loves the righteous.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"The LORD preserves the foreigners.","poem":1},{"text":"He upholds the fatherless and widow,","poem":2},{"lineBreak":true},{"text":"but he turns the way of the wicked upside down.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"The LORD will reign forever;","poem":1},{"text":"your God, O Zion, to all generations.","poem":2},{"text":"Praise the LORD!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/146.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/147.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/145.json","previousChapterAudioLinks":{},"numberOfVerses":10}