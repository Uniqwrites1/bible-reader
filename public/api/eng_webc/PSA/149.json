{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":149,"content":[{"type":"verse","number":1,"content":[{"text":"Praise the LORD!","poem":1},{"text":"Sing to the LORD a new song,","poem":2},{"lineBreak":true},{"text":"his praise in the assembly of the saints.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Let Israel rejoice in him who made them.","poem":1},{"text":"Let the children of Zion be joyful in their King.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them praise his name in the dance!","poem":1},{"text":"Let them sing praises to him with tambourine and harp!","poem":2}]},{"type":"verse","number":4,"content":[{"text":"For the LORD takes pleasure in his people.","poem":1},{"text":"He crowns the humble with salvation.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let the saints rejoice in honor.","poem":1},{"text":"Let them sing for joy on their beds.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"May the high praises of God be in their mouths,","poem":1},{"text":"and a two-edged sword in their hand,","poem":2}]},{"type":"verse","number":7,"content":[{"text":"to execute vengeance on the nations,","poem":1},{"text":"and punishments on the peoples;","poem":2}]},{"type":"verse","number":8,"content":[{"text":"to bind their kings with chains,","poem":1},{"text":"and their nobles with fetters of iron;","poem":2}]},{"type":"verse","number":9,"content":[{"text":"to execute on them the written judgment.","poem":1},{"text":"All his saints have this honor.","poem":2},{"text":"Praise the LORD!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}