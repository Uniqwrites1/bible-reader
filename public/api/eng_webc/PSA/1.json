{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":1,"content":[{"type":"verse","number":1,"content":[{"text":"Blessed is the man who doesn’t walk in the counsel of the wicked,","poem":1},{"text":"nor stand on the path of sinners,","poem":2},{"lineBreak":true},{"text":"nor sit in the seat of scoffers;","poem":2}]},{"type":"verse","number":2,"content":[{"text":"but his delight is in the LORD’s","poem":1},{"noteId":2379},{"text":"law.","poem":1},{"text":"On his law he meditates day and night.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"He will be like a tree planted by the streams of water,","poem":1},{"text":"that produces its fruit in its season,","poem":2},{"lineBreak":true},{"text":"whose leaf also does not wither.","poem":2},{"lineBreak":true},{"text":"Whatever he does shall prosper.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"The wicked are not so,","poem":1},{"text":"but are like the chaff which the wind drives away.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Therefore the wicked shall not stand in the judgment,","poem":1},{"text":"nor sinners in the congregation of the righteous.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"For the LORD knows the way of the righteous,","poem":1},{"text":"but the way of the wicked shall perish.","poem":2}]}],"footnotes":[{"noteId":2379,"caller":"+","text":"When rendered in ALL CAPITAL LETTERS, “LORD” or “GOD” is the translation of God’s Proper Name.","reference":{"chapter":1,"verse":2}}]},"thisChapterLink":"/api/eng_webc/PSA/1.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/2.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/JOB/42.json","previousChapterAudioLinks":{},"numberOfVerses":6}