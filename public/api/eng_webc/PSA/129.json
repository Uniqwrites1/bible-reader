{"translation":{"id":"eng_webc","name":"World English Bible (Catholic)","website":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-web-c","licenseNotes":null,"shortName":"WEBC","englishName":"World English Bible (Catholic)","language":"eng","textDirection":"ltr","sha256":"b135c6feabc7d4494dd67c79ecac3e32137c9d8d90efde0065676b842675530a","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webc/books.json","numberOfBooks":64,"totalNumberOfChapters":1167,"totalNumberOfVerses":30579,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webc","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"6c002e1eb3bcd41e8f2e75722c809131890e1dba8eaf6a9c18a41d23c876e5b5","firstChapterApiLink":"/api/eng_webc/PSA/1.json","lastChapterApiLink":"/api/eng_webc/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":129,"content":[{"type":"hebrew_subtitle","content":["A Song of Ascents."]},{"type":"verse","number":1,"content":[{"text":"Many times they have afflicted me from my youth up.","poem":1},{"text":"Let Israel now say:","poem":2}]},{"type":"verse","number":2,"content":[{"text":"many times they have afflicted me from my youth up,","poem":1},{"text":"yet they have not prevailed against me.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"The plowers plowed on my back.","poem":1},{"text":"They made their furrows long.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"The LORD is righteous.","poem":1},{"text":"He has cut apart the cords of the wicked.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let them be disappointed and turned backward,","poem":1},{"text":"all those who hate Zion.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Let them be as the grass on the housetops,","poem":1},{"text":"which withers before it grows up,","poem":2}]},{"type":"verse","number":7,"content":[{"text":"with which the reaper doesn’t fill his hand,","poem":1},{"text":"nor he who binds sheaves, his bosom.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Neither do those who go by say,","poem":1},{"text":"“The blessing of the LORD be on you.","poem":2},{"lineBreak":true},{"text":"We bless you in the LORD’s name.”","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webc/PSA/129.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webc/PSA/130.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webc/PSA/128.json","previousChapterAudioLinks":{},"numberOfVerses":8}