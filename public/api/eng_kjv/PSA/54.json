,"number":6,"content":[{"lineBreak":true},{"text":"I will freely sacrifice unto thee: I will praise thy name, O LORD; for it is good.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For he hath delivered me out of all trouble: and mine eye hath seen his desire upon mine enemies.","poem":1}]}],"footnotes":[{"noteId":21256,"caller":"+","text":"54.1 chief…: or, overseer","reference":{"chapter":54,"verse":1}},{"noteId":21257,"caller":"+","text":"54.1 Maschil: or, of instruction","reference":{"chapter":54,"verse":1}},{"noteId":21258,"caller":"+","text":"54.5 mine…: Heb. those that observe me","reference":{"chapter":54,"verse":5}}]},"thisChapterLink":"/api/eng_kjv/PSA/54.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_kjv/PSA/55.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_kjv/PSA/53.json","previousChapterAudioLinks":{},"numberOfVerses":7}