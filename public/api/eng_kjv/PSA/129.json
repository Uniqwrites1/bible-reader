{"translation":{"id":"eng_kjv","name":"King James (Authorized) Version","website":"https://ebible.org/Scriptures/details.php?id=eng-kjv2006","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-kjv2006","licenseNotes":null,"shortName":"KJV","englishName":"King James (Authorized) Version","language":"eng","textDirection":"ltr","sha256":"c6d97cb13aa976c6968b91141b655c936e1f71157f60b92f312bf42688295359","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_kjv/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_kjv","name":"Psalms","commonName":"Psalms","title":"The Book of Psalms","order":19,"numberOfChapters":150,"sha256":"059a867943c4fc103f61be84f613c0acfcbb5ad728c37b018857c8f147743178","firstChapterApiLink":"/api/eng_kjv/PSA/1.json","lastChapterApiLink":"/api/eng_kjv/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":129,"content":[{"type":"hebrew_subtitle","content":["A Song of degrees."]},{"type":"verse","number":1,"content":[{"text":"Many a time have they afflicted me from my youth, may Israel now say:","poem":1},{"noteId":21639}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Many a time have they afflicted me from my youth: yet they have not prevailed against me.","poem":1},{"noteId":21640}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"The plowers plowed upon my back: they made long their furrows.","poem":1}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"The LORD is righteous: he hath cut asunder the cords of the wicked.","poem":1}]},{"type":"line_break"},{"type":"verse","number":5,"content":[{"text":"Let them all be confounded and turned back that hate Zion.","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Let them be as the grass upon the housetops, which withereth afore it groweth up:","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"Wherewith the mower filleth not his hand; nor he that bindeth sheaves his bosom.","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Neither do they which go by say, The blessing of the LORD be upon you: we bless you in the name of the LORD.","poem":1}]}],"footnotes":[{"noteId":21639,"caller":"+","text":"129.1 Many…: or, Much","reference":{"chapter":129,"verse":1}},{"noteId":21640,"caller":"+","text":"129.2 Many…: or, Much","reference":{"chapter":129,"verse":2}}]},"thisChapterLink":"/api/eng_kjv/PSA/129.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_kjv/PSA/130.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_kjv/PSA/128.json","previousChapterAudioLinks":{},"numberOfVerses":8}