r":6,"content":[{"lineBreak":true},{"text":"He shall judge among the heathen, he shall fill the places with the dead bodies; he shall wound the heads over many countries.","poem":1},{"noteId":21575}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"He shall drink of the brook in the way: therefore shall he lift up the head.","poem":1}]}],"footnotes":[{"noteId":21574,"caller":"+","text":"110.3 fromâ€¦: or, more than the womb of the morning: thou shalt have, etc","reference":{"chapter":110,"verse":3}},{"noteId":21575,"caller":"+","text":"110.6 many: or, great","reference":{"chapter":110,"verse":6}}]},"thisChapterLink":"/api/eng_kjv/PSA/110.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_kjv/PSA/111.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_kjv/PSA/109.json","previousChapterAudioLinks":{},"numberOfVerses":7}