1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"Who turned the rock into a pool of water, the flint into a fountain of waters.","poem":1}]}],"footnotes":[{"noteId":6902,"caller":"+","text":"114.7 Eloah","reference":{"chapter":114,"verse":7}}]},"thisChapterLink":"/api/eng_dby/PSA/114.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_dby/PSA/115.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_dby/PSA/113.json","previousChapterAudioLinks":{},"numberOfVerses":8}