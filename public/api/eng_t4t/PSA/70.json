verse","number":5,"content":[{"text":"As for me, I am poor and needy [DOU];","poem":1},{"text":"so God, come quickly to help me!","poem":2},{"text":"Yahweh, you are the one who saves and helps me,","poem":1},{"text":"so please come quickly!","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_t4t/PSA/70.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_t4t/PSA/71.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_t4t/PSA/69.json","previousChapterAudioLinks":{},"numberOfVerses":5}