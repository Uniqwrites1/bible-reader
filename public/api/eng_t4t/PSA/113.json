{"translation":{"id":"eng_t4t","name":"Translation for Translators","website":"https://ebible.org/Scriptures/details.php?id=eng-t4t","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-t4t","licenseNotes":null,"shortName":"T4T","englishName":"Translation for Translators","language":"eng","textDirection":"ltr","sha256":"e9d73e8779504b2bb9f9f630d23534eb9ae2c8c14556d2865560814b21933f9d","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_t4t/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":30640,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_t4t","name":"PSALMS","commonName":"Psalms","title":"This book contains songs that the Israeli people sang. We call this book Psalms","order":19,"numberOfChapters":150,"sha256":"24bef54ca68994510c67f8cd2835a666ea0618e84b8a9b6bed4c53ed254893b6","firstChapterApiLink":"/api/eng_t4t/PSA/1.json","lastChapterApiLink":"/api/eng_t4t/PSA/150.json","totalNumberOfVerses":2456},"chapter":{"number":113,"content":[{"type":"verse","number":1,"content":[{"text":"Praise Yahweh!","poem":1},{"lineBreak":true},{"text":"You people who serve Yahweh, praise him!","poem":1},{"lineBreak":true},{"text":"Praise ◄him/his name►!","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Yahweh should be praised now and forever!","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"People who live in the east and people who live in the west,","poem":1},{"text":"everyone, should praise Yahweh [MTY]!","poem":2}]},{"type":"line_break"},{"type":"verse","number":4,"content":[{"text":"Yahweh rules [MTY] over all the nations,","poem":1},{"lineBreak":true},{"text":"and high in the heavens he shows that his glory is very great.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"There is no one [RHQ] who is like Yahweh, our God,","poem":1},{"text":"who lives/sits/rules in the highest heaven,","poem":2}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"and looks far down through the heavens and sees the people on the earth.","poem":2}]},{"type":"line_break"},{"type":"verse","number":7,"content":[{"text":"Sometimes he lifts poor people up so that they no longer sit in the dirt;","poem":1},{"lineBreak":true},{"text":"he helps needy people so that they no longer sit on heaps of ashes","poem":1}]},{"type":"verse","number":8,"content":[{"lineBreak":true},{"text":"and causes them to be honored by sitting next to princes,","poem":1},{"text":"the sons of the kings who rule their people.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"He also enables women who have no children to have a family;","poem":1},{"text":"he causes them to be happy mothers.","poem":2},{"lineBreak":true},{"text":"Praise Yahweh!","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_t4t/PSA/113.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_t4t/PSA/114.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_t4t/PSA/112.json","previousChapterAudioLinks":{},"numberOfVerses":9}