:7,"content":[{"text":"I am for peace,","poem":1},{"text":"but when I speak, they are for war.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_weu/PSA/120.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_weu/PSA/121.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_weu/PSA/119.json","previousChapterAudioLinks":{},"numberOfVerses":7}