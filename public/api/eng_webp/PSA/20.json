{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":20,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician. A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"May Yahweh answer you in the day of trouble.","poem":1},{"text":"May the name of the God of Jacob set you up on high,","poem":2}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"send you help from the sanctuary,","poem":2},{"lineBreak":true},{"text":"grant you support from Zion,","poem":2}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"remember all your offerings,","poem":2},{"lineBreak":true},{"text":"and accept your burned sacrifice. Selah.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"May he grant you your heartâ€™s desire,","poem":1},{"text":"and fulfill all your counsel.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"We will triumph in your salvation.","poem":1},{"text":"In the name of our God, we will set up our banners.","poem":2},{"lineBreak":true},{"text":"May Yahweh grant all your requests.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Now I know that Yahweh saves his anointed.","poem":1},{"text":"He will answer him from his holy heaven,","poem":2},{"lineBreak":true},{"text":"with the saving strength of his right hand.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Some trust in chariots, and some in horses,","poem":1},{"text":"but we trust in the name of Yahweh our God.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"They are bowed down and fallen,","poem":1},{"text":"but we rise up, and stand upright.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Save, Yahweh!","poem":1},{"text":"Let the King answer us when we call!","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/20.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/21.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/19.json","previousChapterAudioLinks":{},"numberOfVerses":9}