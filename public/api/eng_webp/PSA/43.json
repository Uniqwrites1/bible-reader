{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"verse","number":1,"content":[{"text":"Vindicate me, God, and plead my cause against an ungodly nation.","poem":1},{"text":"Oh, deliver me from deceitful and wicked men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For you are the God of my strength. Why have you rejected me?","poem":1},{"text":"Why do I go mourning because of the oppression of the enemy?","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Oh, send out your light and your truth.","poem":1},{"text":"Let them lead me.","poem":2},{"lineBreak":true},{"text":"Let them bring me to your holy hill,","poem":2},{"lineBreak":true},{"text":"to your tents.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Then I will go to the altar of God,","poem":1},{"text":"to God, my exceeding joy.","poem":2},{"text":"I will praise you on the harp, God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why are you in despair, my soul?","poem":1},{"text":"Why are you disturbed within me?","poem":2},{"text":"Hope in God!","poem":1},{"text":"For I shall still praise him:","poem":2},{"lineBreak":true},{"text":"my Savior, my helper, and my God.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}