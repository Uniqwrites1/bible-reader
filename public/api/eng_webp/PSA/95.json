{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":95,"content":[{"type":"verse","number":1,"content":[{"text":"Oh come, let’s sing to Yahweh.","poem":1},{"text":"Let’s shout aloud to the rock of our salvation!","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Let’s come before his presence with thanksgiving.","poem":1},{"text":"Let’s extol him with songs!","poem":2}]},{"type":"verse","number":3,"content":[{"text":"For Yahweh is a great God,","poem":1},{"text":"a great King above all gods.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"In his hand are the deep places of the earth.","poem":1},{"text":"The heights of the mountains are also his.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"The sea is his, and he made it.","poem":1},{"text":"His hands formed the dry land.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Oh come, let’s worship and bow down.","poem":1},{"text":"Let’s kneel before Yahweh, our Maker,","poem":2}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"for he is our God.","poem":2},{"text":"We are the people of his pasture,","poem":1},{"text":"and the sheep in his care.","poem":2},{"text":"Today, oh that you would hear his voice!","poem":1}]},{"type":"verse","number":8,"content":[{"text":"Don’t harden your heart, as at Meribah,","poem":2},{"lineBreak":true},{"text":"as in the day of Massah in the wilderness,","poem":2}]},{"type":"verse","number":9,"content":[{"text":"when your fathers tempted me,","poem":1},{"text":"tested me, and saw my work.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"Forty long years I was grieved with that generation,","poem":1},{"text":"and said, “They are a people who err in their heart.","poem":2},{"lineBreak":true},{"text":"They have not known my ways.”","poem":2}]},{"type":"verse","number":11,"content":[{"text":"Therefore I swore in my wrath,","poem":1},{"text":"“They won’t enter into my rest.”","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/95.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/96.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/94.json","previousChapterAudioLinks":{},"numberOfVerses":11}