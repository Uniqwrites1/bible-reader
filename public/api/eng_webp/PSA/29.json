{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":29,"content":[{"type":"hebrew_subtitle","content":["A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"Ascribe to Yahweh, you sons of the mighty,","poem":1},{"text":"ascribe to Yahweh glory and strength.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Ascribe to Yahweh the glory due to his name.","poem":1},{"text":"Worship Yahweh in holy array.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"Yahweh’s voice is on the waters.","poem":1},{"text":"The God of glory thunders, even Yahweh on many waters.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Yahweh’s voice is powerful.","poem":1},{"text":"Yahweh’s voice is full of majesty.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Yahweh’s voice breaks the cedars.","poem":1},{"text":"Yes, Yahweh breaks in pieces the cedars of Lebanon.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"He makes them also to skip like a calf;","poem":1},{"text":"Lebanon and Sirion like a young, wild ox.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Yahweh’s voice strikes with flashes of lightning.","poem":1}]},{"type":"verse","number":8,"content":[{"text":"Yahweh’s voice shakes the wilderness.","poem":2},{"lineBreak":true},{"text":"Yahweh shakes the wilderness of Kadesh.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Yahweh’s voice makes the deer calve,","poem":1},{"text":"and strips the forests bare.","poem":2},{"lineBreak":true},{"text":"In his temple everything says, “Glory!”","poem":2}]},{"type":"line_break"},{"type":"verse","number":10,"content":[{"text":"Yahweh sat enthroned at the Flood.","poem":1},{"text":"Yes, Yahweh sits as King forever.","poem":2}]},{"type":"verse","number":11,"content":[{"text":"Yahweh will give strength to his people.","poem":1},{"text":"Yahweh will bless his people with peace.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/29.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/30.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/28.json","previousChapterAudioLinks":{},"numberOfVerses":11}