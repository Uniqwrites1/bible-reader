type":"verse","number":7,"content":[{"text":"to execute vengeance on the nations,","poem":1},{"text":"and punishments on the peoples;","poem":2}]},{"type":"verse","number":8,"content":[{"text":"to bind their kings with chains,","poem":1},{"text":"and their nobles with fetters of iron;","poem":2}]},{"type":"verse","number":9,"content":[{"text":"to execute on them the written judgment.","poem":1},{"text":"All his saints have this honor.","poem":2},{"text":"Praise Yah!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}