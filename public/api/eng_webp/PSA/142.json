{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":142,"content":[{"type":"hebrew_subtitle","content":["A contemplation by David, when he was in the cave. A Prayer."]},{"type":"verse","number":1,"content":[{"text":"I cry with my voice to Yahweh.","poem":1},{"text":"With my voice, I ask Yahweh for mercy.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"I pour out my complaint before him.","poem":1},{"text":"I tell him my troubles.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"When my spirit was overwhelmed within me,","poem":1},{"text":"you knew my route.","poem":2},{"text":"On the path in which I walk,","poem":1},{"text":"they have hidden a snare for me.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Look on my right, and see;","poem":1},{"text":"for there is no one who is concerned for me.","poem":2},{"lineBreak":true},{"text":"Refuge has fled from me.","poem":2},{"lineBreak":true},{"text":"No one cares for my soul.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I cried to you, Yahweh.","poem":1},{"text":"I said, “You are my refuge,","poem":2},{"lineBreak":true},{"text":"my portion in the land of the living.”","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Listen to my cry,","poem":1},{"text":"for I am in desperate need.","poem":2},{"text":"Deliver me from my persecutors,","poem":1},{"text":"for they are too strong for me.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Bring my soul out of prison,","poem":1},{"text":"that I may give thanks to your name.","poem":2},{"text":"The righteous will surround me,","poem":1},{"text":"for you will be good to me.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/142.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/143.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/141.json","previousChapterAudioLinks":{},"numberOfVerses":7}