{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":12,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; upon an eight-stringed lyre. A Psalm of David."]},{"type":"verse","number":1,"content":[{"text":"Help, Yahweh; for the godly man ceases.","poem":1},{"text":"For the faithful fail from among the children of men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Everyone lies to his neighbor.","poem":1},{"text":"They speak with flattering lips, and with a double heart.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"May Yahweh cut off all flattering lips,","poem":1},{"text":"and the tongue that boasts,","poem":2}]},{"type":"verse","number":4,"content":[{"text":"who have said, “With our tongue we will prevail.","poem":1},{"text":"Our lips are our own.","poem":2},{"lineBreak":true},{"text":"Who is lord over us?”","poem":2}]},{"type":"verse","number":5,"content":[{"text":"“Because of the oppression of the weak and because of the groaning of the needy,","poem":1},{"text":"I will now arise,” says Yahweh;","poem":2},{"text":"“I will set him in safety from those who malign him.”","poem":1}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"Yahweh’s words are flawless words,","poem":1},{"text":"as silver refined in a clay furnace, purified seven times.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"You will keep them, Yahweh.","poem":1},{"text":"You will preserve them from this generation forever.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"The wicked walk on every side,","poem":1},{"text":"when what is vile is exalted among the sons of men.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/12.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/13.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/11.json","previousChapterAudioLinks":{},"numberOfVerses":8}