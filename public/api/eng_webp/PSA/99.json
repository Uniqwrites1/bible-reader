{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":99,"content":[{"type":"verse","number":1,"content":[{"text":"Yahweh reigns! Let the peoples tremble.","poem":1},{"text":"He sits enthroned among the cherubim.","poem":2},{"lineBreak":true},{"text":"Let the earth be moved.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Yahweh is great in Zion.","poem":1},{"text":"He is high above all the peoples.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them praise your great and awesome name.","poem":1},{"text":"He is Holy!","poem":2}]},{"type":"line_break"},{"type":"verse","number":4,"content":[{"text":"The Kingâ€™s strength also loves justice.","poem":1},{"text":"You establish equity.","poem":2},{"lineBreak":true},{"text":"You execute justice and righteousness in Jacob.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Exalt Yahweh our God.","poem":1},{"text":"Worship at his footstool.","poem":2},{"lineBreak":true},{"text":"He is Holy!","poem":2}]},{"type":"line_break"},{"type":"verse","number":6,"content":[{"text":"Moses and Aaron were among his priests,","poem":1},{"text":"Samuel was among those who call on his name.","poem":2},{"lineBreak":true},{"text":"They called on Yahweh, and he answered them.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"He spoke to them in the pillar of cloud.","poem":1},{"text":"They kept his testimonies,","poem":2},{"lineBreak":true},{"text":"the statute that he gave them.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"You answered them, Yahweh our God.","poem":1},{"text":"You are a God who forgave them,","poem":2},{"lineBreak":true},{"text":"although you took vengeance for their doings.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Exalt Yahweh, our God.","poem":1},{"text":"Worship at his holy hill,","poem":2},{"lineBreak":true},{"text":"for Yahweh, our God, is holy!","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/99.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/100.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/98.json","previousChapterAudioLinks":{},"numberOfVerses":9}