{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":130,"content":[{"type":"hebrew_subtitle","content":["A Song of Ascents."]},{"type":"verse","number":1,"content":[{"text":"Out of the depths I have cried to you, Yahweh.","poem":1}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Lord, hear my voice.","poem":1},{"text":"Let your ears be attentive to the voice of my petitions.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"If you, Yah, kept a record of sins,","poem":1},{"text":"Lord, who could stand?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"But there is forgiveness with you,","poem":1},{"text":"therefore you are feared.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I wait for Yahweh.","poem":1},{"text":"My soul waits.","poem":2},{"lineBreak":true},{"text":"I hope in his word.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"My soul longs for the Lord more than watchmen long for the morning,","poem":1},{"text":"more than watchmen for the morning.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Israel, hope in Yahweh,","poem":1},{"text":"for there is loving kindness with Yahweh.","poem":2},{"lineBreak":true},{"text":"Abundant redemption is with him.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"He will redeem Israel from all their sins.","poem":1}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/130.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/131.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/129.json","previousChapterAudioLinks":{},"numberOfVerses":8}