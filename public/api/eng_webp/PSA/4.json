{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":4,"content":[{"type":"hebrew_subtitle","content":["For the Chief Musician; on stringed instruments. A Psalm by David."]},{"type":"verse","number":1,"content":[{"text":"Answer me when I call, God of my righteousness.","poem":1},{"text":"Give me relief from my distress.","poem":2},{"lineBreak":true},{"text":"Have mercy on me, and hear my prayer.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"You sons of men, how long shall my glory be turned into dishonor?","poem":1},{"text":"Will you love vanity and seek after falsehood? Selah.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"But know that Yahweh has set apart for himself him who is godly;","poem":1},{"text":"Yahweh will hear when I call to him.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Stand in awe, and don’t sin.","poem":1},{"text":"Search your own heart on your bed, and be still. Selah.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Offer the sacrifices of righteousness.","poem":1},{"text":"Put your trust in Yahweh.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Many say, “Who will show us any good?”","poem":1},{"text":"Yahweh, let the light of your face shine on us.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"You have put gladness in my heart,","poem":1},{"text":"more than when their grain and their new wine are increased.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"In peace I will both lay myself down and sleep,","poem":1},{"text":"for you alone, Yahweh, make me live in safety.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/4.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/5.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/3.json","previousChapterAudioLinks":{},"numberOfVerses":8}