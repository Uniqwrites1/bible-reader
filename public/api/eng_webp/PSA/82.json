{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":82,"content":[{"type":"hebrew_subtitle","content":["A Psalm by Asaph."]},{"type":"verse","number":1,"content":[{"text":"God presides in the great assembly.","poem":1},{"text":"He judges among the gods.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"“How long will you judge unjustly,","poem":1},{"text":"and show partiality to the wicked?” Selah.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"“Defend the weak, the poor, and the fatherless.","poem":1},{"text":"Maintain the rights of the poor and oppressed.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Rescue the weak and needy.","poem":1},{"text":"Deliver them out of the hand of the wicked.”","poem":2}]},{"type":"verse","number":5,"content":[{"text":"They don’t know, neither do they understand.","poem":1},{"text":"They walk back and forth in darkness.","poem":2},{"lineBreak":true},{"text":"All the foundations of the earth are shaken.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I said, “You are gods,","poem":1},{"text":"all of you are sons of the Most High.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Nevertheless you shall die like men,","poem":1},{"text":"and fall like one of the rulers.”","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Arise, God, judge the earth,","poem":1},{"text":"for you inherit all of the nations.","poem":2}]}],"footnotes":[]},"thisChapterLink":"/api/eng_webp/PSA/82.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/83.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/81.json","previousChapterAudioLinks":{},"numberOfVerses":8}