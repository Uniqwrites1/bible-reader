{"translation":{"id":"eng_webp","name":"World English Bible","website":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engwebp","licenseNotes":null,"shortName":"WEBP","englishName":"World English Bible","language":"eng","textDirection":"ltr","sha256":"955f587db7c3cb3dd07b5517301a44bd58f53075238a03d43100c4a2e8cdc4c5","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_webp/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31103,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_webp","name":"Psalms","commonName":"Psalms","title":"The Psalms Psalm","order":19,"numberOfChapters":150,"sha256":"f1b05b919d27ffcf7c09747985cfac390df46ddfcc6b364601db8d2ca744f1e0","firstChapterApiLink":"/api/eng_webp/PSA/1.json","lastChapterApiLink":"/api/eng_webp/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":97,"content":[{"type":"verse","number":1,"content":[{"text":"Yahweh reigns!","poem":1},{"text":"Let the earth rejoice!","poem":2},{"lineBreak":true},{"text":"Let the multitude of islands be glad!","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Clouds and darkness are around him.","poem":1},{"text":"Righteousness and justice are the foundation of his throne.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"A fire goes before him,","poem":1},{"text":"and burns up his adversaries on every side.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"His lightning lights up the world.","poem":1},{"text":"The earth sees, and trembles.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"The mountains melt like wax at the presence of Yahweh,","poem":1},{"text":"at the presence of the Lord of the whole earth.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"The heavens declare his righteousness.","poem":1},{"text":"All the peoples have seen his glory.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Let all them be shamed who serve engraved images,","poem":1},{"text":"who boast in their idols.","poem":2},{"lineBreak":true},{"text":"Worship him, all you gods!","poem":2},{"noteId":3536}]},{"type":"verse","number":8,"content":[{"text":"Zion heard and was glad.","poem":1},{"text":"The daughters of Judah rejoiced","poem":2},{"lineBreak":true},{"text":"because of your judgments, Yahweh.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"For you, Yahweh, are most high above all the earth.","poem":1},{"text":"You are exalted far above all gods.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"You who love Yahweh, hate evil!","poem":1},{"text":"He preserves the souls of his saints.","poem":2},{"lineBreak":true},{"text":"He delivers them out of the hand of the wicked.","poem":2}]},{"type":"verse","number":11,"content":[{"text":"Light is sown for the righteous,","poem":1},{"text":"and gladness for the upright in heart.","poem":2}]},{"type":"verse","number":12,"content":[{"text":"Be glad in Yahweh, you righteous people!","poem":1},{"text":"Give thanks to his holy Name.","poem":2}]}],"footnotes":[{"noteId":3536,"caller":"+","text":"LXX reads “angels” instead of “gods”.","reference":{"chapter":97,"verse":7}}]},"thisChapterLink":"/api/eng_webp/PSA/97.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_webp/PSA/98.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_webp/PSA/96.json","previousChapterAudioLinks":{},"numberOfVerses":12}