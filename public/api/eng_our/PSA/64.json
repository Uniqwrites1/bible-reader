{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":64,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Sh'ma ·Hear obey· my voice, God, in my complaint.","poem":1},{"text":"Preserve my life from fear of the enemy.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Hide me from the conspiracy of the wicked,","poem":1},{"text":"from the noisy crowd of the ones doing evil;","poem":2}]},{"type":"verse","number":3,"content":[{"text":"who sharpen their tongue like a sword,","poem":1},{"text":"and aim their arrows, deadly words,","poem":2}]},{"type":"verse","number":4,"content":[{"lineBreak":true},{"text":"to shoot innocent men from ambushes.","poem":2},{"lineBreak":true},{"text":"They shoot at him suddenly and fearlessly.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"They encourage themselves in evil plans.","poem":1},{"text":"They talk about laying snares secretly.","poem":2},{"lineBreak":true},{"text":"They say, “Who will see them?”","poem":2}]},{"type":"verse","number":6,"content":[{"text":"They plot injustice, saying, “We have made a perfect plan!”","poem":1},{"text":"Surely man’s mind and heart are cunning.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"But God will shoot at them.","poem":1},{"text":"They will be suddenly struck down with an arrow.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Their own tongues shall ruin them.","poem":1},{"text":"All who see them will shake their heads.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"All mankind shall be afraid.","poem":1},{"text":"They shall declare the work of God,","poem":2},{"lineBreak":true},{"text":"and shall wisely ponder what he has done.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"The upright shall be glad in Adonai ,","poem":1},{"text":"and shall take refuge in him.","poem":2},{"lineBreak":true},{"text":"All the upright in heart shall praise him!","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/64.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/65.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/63.json","previousChapterAudioLinks":{},"numberOfVerses":10}