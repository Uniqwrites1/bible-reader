{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":111,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Halleluyah ·praise Yah·!","poem":1},{"text":"I will yadah ·extend hands in thankful praise· to Adonai with my whole heart,","poem":2},{"lineBreak":true},{"text":"in the council of the upright, and in the congregation.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Adonai ’s works are great,","poem":1},{"text":"pondered by all those who delight in them.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"His work is honor and majesty.","poem":1},{"text":"His righteousness endures forever.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"He has caused his wonderful works to be remembered.","poem":1},{"text":"Adonai is gracious and merciful.","poem":2},{"noteId":3201}]},{"type":"verse","number":5,"content":[{"text":"He has given food to those who fear him.","poem":1},{"text":"He always remembers his covenant ·binding contract between two or more parties·.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"He has shown his people the power of his works,","poem":1},{"text":"in giving them the heritage of the nations.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"The works of his hands are truth and mishpat ·justice·.","poem":1},{"text":"All his precepts are sure.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"They are established forever and ever.","poem":1},{"text":"They are done in truth and uprightness.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"He has sent redemption to his people.","poem":1},{"text":"He has enjoined his covenant forever.","poem":2},{"lineBreak":true},{"text":"His name is holy","poem":2},{"noteId":3202},{"text":"and awesome!","poem":2}]},{"type":"verse","number":10,"content":[{"text":"The fear of Adonai is the beginning of wisdom.","poem":1},{"text":"All those who do his work have a good understanding.","poem":2},{"text":"His tehilah ·praise song· endures forever!","poem":1}]},{"type":"line_break"}],"footnotes":[{"noteId":3201,"caller":"+","text":"Quoted in James 5:11","reference":{"chapter":111,"verse":4}},{"noteId":3202,"caller":"+","text":"Quoted in Luke 1:49","reference":{"chapter":111,"verse":9}}]},"thisChapterLink":"/api/eng_our/PSA/111.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/112.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/110.json","previousChapterAudioLinks":{},"numberOfVerses":10}