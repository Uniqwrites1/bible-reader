{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":149,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Praise Adonai !","poem":1},{"text":"Sing to Adonai a new song,","poem":2},{"lineBreak":true},{"text":"his tehilah ·praise song· in the assembly of the saints.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Let Israel [God prevails] rejoice in him who made them.","poem":1},{"text":"Let the children of Zion [Mountain ridge, Marking] be joyful in their King.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Let them praise his name in the dance!","poem":1},{"text":"Let them sing zahmar ·musical praise· to him with tambourine and harp!","poem":2}]},{"type":"verse","number":4,"content":[{"text":"For Adonai takes pleasure in his people.","poem":1},{"text":"He crowns the humble with yishu'ah ·salvation·.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let the saints rejoice in kavod ·weighty glory·.","poem":1},{"text":"Let them sing for joy on their beds.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"May the high praises of God be in their mouths,","poem":1},{"text":"and a two-edged sword in their hand;","poem":2}]},{"type":"verse","number":7,"content":[{"text":"To execute vengeance on the nations,","poem":1},{"text":"and punishments on the peoples;","poem":2}]},{"type":"verse","number":8,"content":[{"text":"To bind their kings with chains,","poem":1},{"text":"and their nobles with fetters of iron;","poem":2}]},{"type":"verse","number":9,"content":[{"text":"to execute on them the written judgment.","poem":1},{"text":"All his saints have this honor.","poem":2},{"text":"Halleluyah ·praise Yah·!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/149.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/150.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/148.json","previousChapterAudioLinks":{},"numberOfVerses":9}