{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":129,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Many times they have afflicted me from my youth up.","poem":1},{"text":"Let Israel [God prevails] now say,","poem":2}]},{"type":"verse","number":2,"content":[{"text":"many times they have afflicted me from my youth up,","poem":1},{"text":"yet they have not prevailed against me.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"The plowers plowed on my back.","poem":1},{"text":"They made their furrows long.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Adonai is righteous.","poem":1},{"text":"He has cut apart the cords of the wicked.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Let them be disappointed and turned backward,","poem":1},{"text":"all those who hate Zion [Mountain ridge, Marking].","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Let them be as the grass on the housetops,","poem":1},{"text":"which withers before it grows up;","poem":2}]},{"type":"verse","number":7,"content":[{"text":"with which the reaper does not fill his hand,","poem":1},{"text":"nor he who binds sheaves, his bosom.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Neither do those who go by say,","poem":1},{"text":"“The blessing of Adonai be on you.","poem":2},{"lineBreak":true},{"text":"We bless you in Adonai ’s name.”","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/129.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/130.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/128.json","previousChapterAudioLinks":{},"numberOfVerses":8}