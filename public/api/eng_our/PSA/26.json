{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":26,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Judge me, Adonai , for I have walked in my integrity.","poem":1},{"text":"I have trusted also in Adonai without wavering.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Examine me, Adonai , and prove me.","poem":1},{"text":"Try my heart and my mind.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"For your cheshed ·loving-kindness· is before my eyes.","poem":1},{"text":"I have walked in your truth.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"I have not sat with deceitful men,","poem":1},{"text":"neither will I go in with hypocrites.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"I hate the assembly of evildoers,","poem":1},{"text":"and will not sit with the wicked.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I will wash my hands in innocence,","poem":1},{"text":"so I will go about your altar, Adonai ;","poem":2}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"that I may make the voice of thanksgiving to be sh'ma ·heard obeyed·,","poem":2},{"lineBreak":true},{"text":"and tell of all your wondrous deeds.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Adonai , I 'ahav ·affectionately love· the habitation of your house,","poem":1},{"text":"the place where your kavod ·weighty glory· dwells.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Don’t gather my soul with sinners,","poem":1},{"text":"nor my life with bloodthirsty men;","poem":2}]},{"type":"verse","number":10,"content":[{"lineBreak":true},{"text":"in whose hands is wickedness,","poem":2},{"lineBreak":true},{"text":"their right hand is full of bribes.","poem":2}]},{"type":"line_break"},{"type":"verse","number":11,"content":[{"text":"But as for me, I will walk in my integrity.","poem":1},{"text":"Redeem me, and be merciful to me.","poem":2}]},{"type":"verse","number":12,"content":[{"text":"My foot stands in an even place.","poem":1},{"text":"In the congregations I will bless Adonai .","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/26.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/27.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/25.json","previousChapterAudioLinks":{},"numberOfVerses":12}