ent":[{"text":"Israel [God prevails], hope in Adonai ,","poem":1},{"text":"for with Adonai there is cheshed 路loving-kindness路.","poem":2},{"lineBreak":true},{"text":"With him is abundant redemption.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"He will redeem Israel [God prevails] from all their sins.","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/130.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/131.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/129.json","previousChapterAudioLinks":{},"numberOfVerses":8}