{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":120,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"In my distress, I cried to Adonai .","poem":1},{"text":"He answered me.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Deliver my soul, Adonai , from lying lips,","poem":1},{"text":"from a deceitful tongue.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"What will be given to you, and what will be done more to you,","poem":1},{"text":"you deceitful tongue?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Sharp arrows of the mighty,","poem":1},{"text":"with coals of juniper.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Woe is me, that I live in Meshech,","poem":1},{"text":"that I dwell among the tents of Kedar!","poem":2}]},{"type":"verse","number":6,"content":[{"text":"My soul has had her dwelling too long","poem":1},{"text":"with him who hates peace.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"I am for peace,","poem":1},{"text":"but when I speak, they are for war.","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/120.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/121.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/119.json","previousChapterAudioLinks":{},"numberOfVerses":7}