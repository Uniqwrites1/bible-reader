{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":6,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Adonai , don’t rebuke me in your anger,","poem":1},{"text":"neither discipline me in your wrath.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Have mercy on me, Adonai , for I am faint.","poem":1},{"text":"Adonai , heal me, for my bones are troubled.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"My soul is also in great anguish.","poem":1},{"text":"But you, Adonai, how long?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Teshuvah ·Completely return·, Adonai . Deliver my soul,","poem":1},{"text":"and save me for your cheshed ·loving-kindness’· sake.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"For in death there is no memory of you.","poem":1},{"text":"In Sheol ·Place of the dead·, who shall yadah ·extend hands in thankful praise· to you?","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I am weary with my groaning.","poem":1},{"text":"Every night I flood my bed.","poem":2},{"lineBreak":true},{"text":"I drench my couch with my tears.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"My eye wastes away because of grief.","poem":1},{"text":"It grows old because of all my adversaries.","poem":2}]},{"type":"verse","number":8,"content":[{"text":"Depart from me, all you workers of iniquity,","poem":1},{"noteId":2995},{"text":"for Adonai has sh'ma ·heard obeyed· the voice of my weeping.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Adonai has sh'ma ·heard obeyed· my supplication.","poem":1},{"text":"Adonai accepts my prayer.","poem":2}]},{"type":"verse","number":10,"content":[{"text":"May all my enemies be ashamed and dismayed.","poem":1},{"text":"They shall turn back, they shall be disgraced suddenly.","poem":2}]},{"type":"line_break"}],"footnotes":[{"noteId":2995,"caller":"+","text":"Quoted in Matt 7:23, 25:41; Luke 13:27","reference":{"chapter":6,"verse":8}}]},"thisChapterLink":"/api/eng_our/PSA/6.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/7.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/5.json","previousChapterAudioLinks":{},"numberOfVerses":10}