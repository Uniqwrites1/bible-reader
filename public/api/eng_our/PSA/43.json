{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":43,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Vindicate me, God, and plead my cause against an ungodly nation.","poem":1},{"text":"Oh, deliver me from deceitful and wicked men.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"For you are the God of my strength. Why have you rejected me?","poem":1},{"text":"Why do I go mourning because of the oppression of the enemy?","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Oh, send out your light and your truth.","poem":1},{"text":"Let them lead me.","poem":2},{"lineBreak":true},{"text":"Let them bring me to your holy hill,","poem":2},{"lineBreak":true},{"text":"To your tents.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Then I will go to the altar of God,","poem":1},{"text":"to God, my exceeding joy.","poem":2},{"text":"I will yadah ·extend hands in thankful praise· to you on the harp, God, my God.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"Why are you in despair, my soul?","poem":1},{"text":"Why are you disturbed within me?","poem":2},{"text":"Hope in God!","poem":1},{"text":"For I shall still yadah ·extend hands in thankful praise· to him:","poem":2},{"lineBreak":true},{"text":"my yishu'ah ·Savior·, my helper, and my God.","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/43.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/44.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/42.json","previousChapterAudioLinks":{},"numberOfVerses":5}