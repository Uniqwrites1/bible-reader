{"translation":{"id":"eng_our","name":"One Unity Resource Bible","website":"https://ebible.org/Scriptures/details.php?id=engourb","licenseUrl":"https://ebible.org/Scriptures/details.php?id=engourb","licenseNotes":null,"shortName":"OUR","englishName":"One Unity Resource Bible","language":"eng","textDirection":"ltr","sha256":"0bf6d7e3ef09035147363ef700988394b6f905084ee0c9d31787a9c4fc3b2b0b","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_our/books.json","numberOfBooks":12,"totalNumberOfChapters":523,"totalNumberOfVerses":14299,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_our","name":"Psalms / Tehillim","commonName":"Psalms","title":"Psalms [Praises] Tehillim [Praises]","order":19,"numberOfChapters":150,"sha256":"14d1f8d181af7a7a3ebc77a4ba3542af876d58d3acafb5b8626ef13db09ea52b","firstChapterApiLink":"/api/eng_our/PSA/1.json","lastChapterApiLink":"/api/eng_our/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":20,"content":[{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"May Adonai answer you in the day of trouble.","poem":1},{"text":"May the name of the God of Jacob [Supplanter] set you up on high,","poem":2}]},{"type":"verse","number":2,"content":[{"text":"send you help from the sanctuary,","poem":1},{"text":"grant you support from Zion [Mountain ridge, Marking],","poem":2}]},{"type":"verse","number":3,"content":[{"text":"remember all your offerings,","poem":1},{"text":"and accept your burned sacrifice. Selah ·contemplation with musical interlude·.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"May He grant you your heart’s desire,","poem":1},{"text":"and fulfill all your counsel.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"We will triumph in your yishu'ah ·salvation·.","poem":1},{"text":"In the name of our God, we will set up our banners.","poem":2},{"lineBreak":true},{"text":"May Adonai grant all your requests.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Now I know that Adonai saves his anointed.","poem":1},{"text":"He will answer him from his holy heaven,","poem":2},{"lineBreak":true},{"text":"with the yesha' ·saving· strength of his right hand.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"Some trust in chariots, and some in horses,","poem":1},{"text":"but we trust the name of Yahweh Eloheikhem [Yahweh our God].","poem":2}]},{"type":"verse","number":8,"content":[{"text":"They are bowed down and fallen,","poem":1},{"text":"but we rise up, and stand upright.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"Save, Adonai !","poem":1},{"text":"Let the King answer us when we call!","poem":2}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/20.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/21.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/19.json","previousChapterAudioLinks":{},"numberOfVerses":9}