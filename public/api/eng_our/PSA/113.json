:7,"content":[{"text":"He raises up the poor out of the dust.","poem":1},{"text":"Lifts up the needy from the ash heap;","poem":2}]},{"type":"verse","number":8,"content":[{"text":"that he may set him with princes,","poem":1},{"text":"even with the princes of his people.","poem":2}]},{"type":"verse","number":9,"content":[{"text":"He settles the barren woman in her home,","poem":1},{"text":"as a joyful mother of children.","poem":2},{"text":"Halleluyah ·praise Yah·!","poem":1}]},{"type":"line_break"}],"footnotes":[]},"thisChapterLink":"/api/eng_our/PSA/113.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_our/PSA/114.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_our/PSA/112.json","previousChapterAudioLinks":{},"numberOfVerses":9}