{"translation":{"id":"eng_kja","name":"King James Version + Apocrypha","website":"https://ebible.org/Scriptures/details.php?id=eng-kjv","licenseUrl":"https://ebible.org/Scriptures/details.php?id=eng-kjv","licenseNotes":null,"shortName":"KJA","englishName":"King James Version + Apocrypha","language":"eng","textDirection":"ltr","sha256":"1eb7ca35203fff87f590c765f12aaa28ab46d55d62740e9b9ef33e173d8fa771","availableFormats":["json"],"listOfBooksApiLink":"/api/eng_kja/books.json","numberOfBooks":66,"totalNumberOfChapters":1189,"totalNumberOfVerses":31102,"languageName":"English","languageEnglishName":"English"},"book":{"id":"PSA","translationId":"eng_kja","name":"Psalms","commonName":"Psalms","title":"The Book of Psalms","order":19,"numberOfChapters":150,"sha256":"8d84c91bd2fa6713a21d034b6a5d93ff5e7416f14760c20bfcfa301e5948240d","firstChapterApiLink":"/api/eng_kja/PSA/1.json","lastChapterApiLink":"/api/eng_kja/PSA/150.json","totalNumberOfVerses":2461},"chapter":{"number":54,"content":[{"type":"hebrew_subtitle","content":["To the chief Musician on Neginoth, Maschil, A Psalm of David, when the Ziphims came and said to Saul, Doth not David hide himself with us?"]},{"type":"verse","number":1,"content":[{"text":"Save me, O God, by thy name, and judge me by thy strength.","poem":1},{"noteId":14297},{"noteId":14298}]},{"type":"verse","number":2,"content":[{"lineBreak":true},{"text":"Hear my prayer, O God; give ear to the words of my mouth.","poem":1}]},{"type":"verse","number":3,"content":[{"lineBreak":true},{"text":"For strangers are risen up against me, and oppressors seek after my soul: they have not set God before them. Selah.","poem":1}]},{"type":"line_break"},{"type":"verse","number":4,"content":[{"text":"Behold, God is mine helper: the Lord is with them that uphold my soul.","poem":1}]},{"type":"verse","number":5,"content":[{"lineBreak":true},{"text":"He shall reward evil unto mine enemies: cut them off in thy truth.","poem":1},{"noteId":14299}]},{"type":"verse","number":6,"content":[{"lineBreak":true},{"text":"I will freely sacrifice unto thee: I will praise thy name, O LORD; for it is good.","poem":1}]},{"type":"verse","number":7,"content":[{"lineBreak":true},{"text":"For he hath delivered me out of all trouble: and mine eye hath seen his desire upon mine enemies.","poem":1}]}],"footnotes":[{"noteId":14297,"caller":"+","text":"54.1 chief…: or, overseer","reference":{"chapter":54,"verse":1}},{"noteId":14298,"caller":"+","text":"54.1 Maschil: or, of instruction","reference":{"chapter":54,"verse":1}},{"noteId":14299,"caller":"+","text":"54.5 mine…: Heb. those that observe me","reference":{"chapter":54,"verse":5}}]},"thisChapterLink":"/api/eng_kja/PSA/54.json","thisChapterAudioLinks":{},"nextChapterApiLink":"/api/eng_kja/PSA/55.json","nextChapterAudioLinks":{},"previousChapterApiLink":"/api/eng_kja/PSA/53.json","previousChapterAudioLinks":{},"numberOfVerses":7}